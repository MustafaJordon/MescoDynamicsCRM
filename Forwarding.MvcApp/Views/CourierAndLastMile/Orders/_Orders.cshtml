                                <option value="1">Last Mile</option>
                                <option value="2">Courier</option>
                            </select>
                        </div>
                    </div>
           
                <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass" style="padding-top:15px; padding-left:15px;">
                    <div class="swapChildrenClass m-t-n">

                        <div class=" col-sm-4 swapChildrenClass m-t-md">
                            <button id="btn-Search" type="button" class="btn btn-lightblue btn-sm swapChildrenClass" onclick="Orders_LoadingWithPaging();"><i class="fa fa-search"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Search&nbsp;</b></button>
                            <button id="btn-NewAdd" type="button" class="btn btn-warning btn-sm swapChildrenClass" data-toggle="modal"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>
                            @*<button id="btn-Delete" type="button" class="btn btn-sm btn-danger swapChildrenClass"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>*@
                            @*<button id="btn-PermanentDelete" type="button" onclick="Orders_DeleteList('D');" class="btn btn-sm btn-info swapChildrenClass"><i class="fa fa-ban"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.PermanentDelete&nbsp;</b></button>*@
                        </div>
                        @*<div class="col-sm-2"></div>*@
                    </div>
                </div>
            </div>


            <!--Region the searh, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive  m-t-n" style="font-size:80%;">
                <table id="tblOrders" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>Order No</th>
                            <th>Order Date</th>
                            <th>Order Type</th>
                            <th>Customer Name</th>
                            <th>City</th>
                            <th>Region</th>
                            <th>Order .Status</th>
                            <th class="rounded-right"></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>



<!-- OrdersModal  -->
