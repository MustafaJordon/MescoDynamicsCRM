                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.City</label>
                        <input id="txtSearchCity" tabindex="600" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="50" placeholder="" data-required="false" style="text-transform:uppercase">
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Region</label>
                        <input id="txtSearchRegion" tabindex="700" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="50" placeholder="" data-required="false" style="text-transform:uppercase">
                    </div>
                </div>
                    <div class="swapChildrenClass m-t-n">
                        <div class="form-group col-sm-2">
                            <label>Company Name</label>
                            <input id="txtSearchCompanyName" tabindex="800" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="50" placeholder="" data-required="false" style="text-transform:uppercase">
                        </div>
                        <div class="form-group col-sm-2">
                            <label>Warehouse Name</label>
                            <input id="txtSearchWarehouseName" tabindex="900" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="50" placeholder="" data-required="false" style="text-transform:uppercase">
                        </div>
                        <div class="form-group col-sm-2">
                            <label>Order Type</label>
                            <select id="slOrderType" tabindex="300" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="true">
