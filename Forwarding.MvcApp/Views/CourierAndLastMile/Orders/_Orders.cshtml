<div id="div-Orders">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.Orders
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">

            </div>

            <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">
                <div class="swapChildrenClass m-t-n">
                    <div class="form-group col-sm-2">
