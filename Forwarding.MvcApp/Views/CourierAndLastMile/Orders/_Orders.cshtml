                        </div>
                    </div>
                    <div class="form-group  col-sm-2">
                        <label>Order No</label>
                        <input id="txtSearchOrderNo" tabindex="500" type="text" onfocus="CheckValueIsInteger(id);" onkeypress="    CheckValueIsInteger(id);" class="form-control parsley-validated input-sm" maxlength="100" placeholder="Order No" data-required="false" />
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>  @*class="control-label col-sm-2 m-t-xs" for="txtToDate"*@
                        <div>
                            @*class="col-sm-10"*@
                            <input id="txtToDate" tabindex="30" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.SelectFromMenu">
                        </div>
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</label>
