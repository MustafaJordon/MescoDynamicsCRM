<div id="div-DomesticAWB">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.Domestic_AWB
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
          
            <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">
                <div class="row text-sm wrapper hidden-print ">
                    <div id="div-main-options" class="col-sm-5 m-b-xs">
                        @*<button id="btn-NewAdd" class="btn btn-warning btn-sm" data-toggle="modal" data-target="#RateModal"><i class="fa fa-plus"></i>&nbsp;New</button>*@
                        <button id="btn-NewAdd" type="button" onclick="ClearModalAWB();" class="btn btn-warning btn-sm swapChildrenClass" data-toggle="modal"><i class="fa fa-plus"></i><b>&nbsp;New&nbsp;</b></button>
                    </div>
                    <div id="div-search-option" class="col-sm-7">
                        <div id="div-search-controls" class="input-group col-sm-9">
                            <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search Rate">
                            <span class="input-group-btn">
                                <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="DomesticAWB_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                            </span>
                        </div>
                        <div class="float-right col-sm-1">
                            <button id="btn-Delete" type="button" onclick="DomesticAWB_Delete();" class="float-right btn btn-sm btn-danger m-r-n"><i class="fa fa-trash-o"></i>&nbsp;Delete</button>
                        </div>
                    </div>
                </div>
            </div>


            <!--Region the searh, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive  m-t-n" style="font-size:80%;">
                <table id="tblDomesticAWB" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>AWB Number</th>
                            <th>creation Date</th>
                            <th>Shipper Name</th>
                            <th>Consignee Name</th>
                            <th>ActWgt</th>
                            <th>DimWgt</th>
                            <th>ChgWgt</th>
                            <th>Pcs</th>
                            <th>COD Amount</th>
                            <th>Description</th>
                            <th class="rounded-right"></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
