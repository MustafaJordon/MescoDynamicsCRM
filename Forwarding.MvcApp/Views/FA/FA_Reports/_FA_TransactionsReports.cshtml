<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="FA" href="javascript:void(0);" onclick="LoadViews('FA')">
            @Forwarding.MvcApp.App_Resources.App_Resources.FA
    </a>
</li>
<li>
    <a id="FA_Reports" href="javascript:void(0);" onclick="LoadViews('FA_Reports')">
        @Forwarding.MvcApp.App_Resources.App_Resources.FA_Reports
</a>
</li>
<li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.FA_TransactionsReports</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-AccountLedger">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.FA_TransactionsReports
            </h3>
        </header>
        <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">
            <!--Region Filter-->
            <div class="form-group col-sm-2">
                <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                <input id="txtFromDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
            </div>
            <div class="form-group col-sm-2">
                <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                <input id="txtToDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
            </div>

            <div class="col-sm-8"></div>
        </div>
        <div class="row text-sm wrapper hidden-print m-t-n-md swapChildrenClass">

            <div class="col-sm-2 ">
                <input onchange="ShowHideSections();" tabindex="" type="radio" name="nameReportType" id="FA_AdditionsAndExclusions_AR" checked="checked" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.AdditionsAndExclusions&nbsp;</b>

            </div>
            <div class="col-sm-2 ">
                <input onchange="ShowHideSections();" tabindex="" type="radio" name="nameReportType" id="FA_Transactions_AR" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.FA_TransactionsFollowUp&nbsp;</b>

            </div>
            <div class="col-sm-2 ">
                <input onchange="ShowHideSections();" tabindex="" type="radio" name="nameReportType" id="FA_DepreciationSummary_AR" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.FA_DepreciationSummary_AR&nbsp;</b>
            </div>
            <div class="col-sm-2 ">
                <input onchange="ShowHideSections();" tabindex="" type="radio" name="nameReportType" id="FA_FinancialTrans_AR" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.FA_FinancialTrans_AR&nbsp;</b>
            </div>
            <div class="col-sm-2 ">
                <input onchange="ShowHideSections();" tabindex="" type="radio" name="nameReportType" id="FA_FinancialTransByBranch_AR" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.FA_FinancialTransByBranch_AR&nbsp;</b>
            </div>
            <div class="col-sm-2 ">
                <input onchange="ShowHideSections();" tabindex="" type="radio" name="nameReportType" id="FA_FinancialTransByGroup_AR" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.FA_FinancialTransByGroup_AR&nbsp;</b>
            </div>
            <div class="col-sm-2 ">
                <input onchange="ShowHideSections();" tabindex="" type="radio" name="nameReportType" id="FA_FinancialTransByDepreciated_AR" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.FA_FinancialTransByDepreciated_AR&nbsp;</b>
            </div>
            <div class="col-sm-2 ">
                <input onchange="ShowHideSections();" tabindex="" type="radio" name="nameReportType" id="FA_FinancialTransSummary_AR" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.FA_FinancialTransSummary_AR&nbsp;</b>
            </div>


            
            <div class="col-sm-1">
                @*<button id="btn-ExportToExcel" type="button" onclick="PS_Reports_Print('Excel');" class="btn btn-sm btn-lightblue"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>*@
                <button id="btn-Print" type="button" onclick="Print('Print');" class="btn btn-sm btn-info"><i class="fa fa-print"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print&nbsp;</b></button>
            </div>

            <div class="col-sm-2">
                @*<button id="btn-ExportToExcel" type="button" onclick="PS_Reports_Print('Excel');" class="btn btn-sm btn-lightblue"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>*@
            </div>

        </div>
        <div class="row text-sm wrapper hidden-print m-t-n-md swapChildrenClass">

            <section id="secBranches" class=" col-sm-2 panel panel-default">
                <div class="">
                    <label class=""><input type="checkbox" id="cbCheckAllBranches" onchange="cbCheckAllBranchesChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllBranches</u></label><br />
                </div>
                <div id="divCbBranches" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                </div>
            </section>
            <section id="secDevisions" class="col-sm-2 panel panel-default">
                <div class="">
                    <label class=""><input type="checkbox" id="cbCheckAllDevisions" onchange="cbCheckAllDevisionsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllDivisions</u></label><br />
                </div>
                <div id="divCbDevisions" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                </div>
            </section>
            <section id="secDepartments" class="col-sm-2 panel panel-default">
                <div class="">
                    <label class=""><input type="checkbox" id="cbCheckAllDepartments" onchange="cbCheckAllDepartmentsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllDepartment</u></label><br />
                </div>
                <div id="divCbDepartments" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                </div>
            </section>

            <section id="secGroups" class=" col-sm-2 panel panel-default">
                <div class="">
                    <label class=""><input type="checkbox" id="cbCheckAllGroups" onchange="cbCheckAllGroupsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllGroups</u></label><br />
                </div>
                <div id="divCbGroups" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                </div>
            </section>

            <div class="col-sm-4"></div>
            <!--EOF Region Filter-->
            <div id="hExportedTable" class="hide"></div>
        </div>


        <footer class="panel-footer hide">
            <div class="row">
                <div class="col-sm-4 hidden-xs">
                    <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                        <option value="10">10</option>
                        <option value="25">25</option>
                        <option value="50">50</option>
                    </select>
                    <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                        @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                    </button>
                </div>
                <div id="div-Text-Total" class="col-sm-4 text-center">
                    <small class="text-muted inLine m-t-sm m-b-sm">
                        @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                        <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                        <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                    </small>
                </div>
                <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
            </div>
        </footer>
    </section>
</div>
</div>
