<!--Region the searh, delete, addnew-->
<div id="div-YardLinkTankCreditPosting">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>

        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.YardLinkTankCreditPosting
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                    <input id="txtSearchFrom" tabindex="10" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                </div>
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                    <input id="txtSearchTo" tabindex="20" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                </div>


                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Credit</label>
                    <input id="txtSearchInvoiceNo" tabindex="37" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="50" placeholder="" data-required="false" />
                </div>


                <div class="col-sm-1 m-t-md">
                    <button id="btn-Search" tabindex="40" type="button" onclick="YardLinkTankCreditPosting_LoadingWithPaging();" class="btn btn-lightblue btn-sm swapChildrenClass" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-search"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Search</b></button>
                </div>
                <div class=" col-sm-1 m-t-md">
                    <button id="btn-Post" tabindex="80" type="button" onclick="YardLinkTankCreditPosting_Post();" class="btn btn-primary btn-sm swapChildrenClass" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-mail-forward"></i><b>@Forwarding.MvcApp.App_Resources.App_Resources.Post&nbsp;</b></button>
                </div>
                <div class=" col-sm-1 m-t-md">
                    <button id="btn-UNPost" tabindex="80" type="button" onclick="YardLinkTankCreditPosting_Post();" class="btn btn-primary btn-sm swapChildrenClass hide" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-mail-forward"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.UnPost&nbsp;</b></button>
                </div>
            </div>
            <div class="row text-sm wrapper hidden-print m-t-n-lg swapChildrenClass">

                <div class="col-sm-2 text-left swapChildrenClass m-t-25">
                    <input tabindex="60" type="radio" name="nameJVOrTwoJVs" id="cbIsApproved" onchange="YardLinkTankCreditPosting_LoadingWithPaging();" onclick="HideShowButtons();" checked="checked" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>Approve&nbsp;</b><input tabindex="70" type="radio" name="nameJVOrTwoJVs" id="cbIsUnApproved" onchange="YardLinkTankCreditPosting_LoadingWithPaging();" onfocus="DisableEnterKey(id);" onclick="HideShowButtons();" onkeypress="DisableEnterKey(id);"><b>Un Approve&nbsp;</b>
                </div>
                @*  <div class=" col-sm-1 m-t-md">
                        <button id="btn-Post" tabindex="80" type="button" onclick="YardInvoicePosting_Post();" class="btn btn-primary btn-sm swapChildrenClass hide" @*data-toggle="modal" data-target="#BankModal"@><i class="fa fa-mail-forward"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Post&nbsp;</b></button>
                    </div>
                    <div class=" col-sm-1 m-t-md">
                        <button id="btn-UNPost" tabindex="80" type="button" onclick="YardInvoicePosting_Post();" class="btn btn-primary btn-sm swapChildrenClass hide" @*data-toggle="modal" data-target="#BankModal"><i class="fa fa-mail-forward"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.UnPost&nbsp;</b></button>
                    </div>*@
            </div>
            <!--EOF Region the searh, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive m-t-n-sm" style="font-size:80%;">
                <table id="tblYardLinkTankCreditPosting" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Credit</th>

                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.IssueDate</th>

                            <th>InvoiceTotal</th>
                            <th>Currency</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.InvoiceType</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Client</th>

                            @*<th>@Forwarding.MvcApp.App_Resources.App_Resources.TraJobNo</th>*@

                            <th class="rounded-right hide"></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                            <option value="100">100</option>
                            <option value="200">200</option>
                            <option value="300">300</option>
                            <option value="400">400</option>
                            <option value="500">500</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>
