<!-- ShipLinkRevenueItemsModal  -->
<div class="modal fade" id="ShipLinkRevenueItemsModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="ShipLinkRevenueItemsModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-100">
        <div class="modal-content m-t-n-lg">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h3 class="m-b-none swapChildrenClass">
                    <span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.ShipLinkRevenueItems&nbsp;</span><label id="lblShown" purpose="dynamicLabel" @*to be cleared*@ class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="ShipLinkRevenueItems-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hID" type="hidden" />
                            <!-- start alert message region -->
                            @*<div class="alert alert-danger div-error">
                                    @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                </div>*@
                            <!-- end alert message region -->

                            <select id="slImportExport" class="hide form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false">
                                <option value="Im">@Forwarding.MvcApp.App_Resources.App_Resources.Import</option>
                                <option value="Ex">@Forwarding.MvcApp.App_Resources.App_Resources.Export</option>
                            </select>
                            <select id="slCostCenter" class="hide form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                            <select id="slRevenueItem" class="hide form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                            <select id="slVoyageSubAccount" class="hide form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>


                            <div class="row m-t-n">
                                @*This div is to make the buttons clickable*@
                                <div id="divDetailsBtns" class="col-sm-12 swapChildrenClass" style="">
                                    <div class="col-sm-4">
                                        <label>Voy.Account</label>
                                        <select id="slVoyageAccount" class="form-control input-sm" onchange="ShipLinkRevenueItems_slVoyageAccountChanged();" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="true"></select>
                                    </div>

                                    <div class="col-sm-1 m-t-27">
                                        <button id="btn-AddDetails" class="form-control btn btn-warning btn-sm m-t-n-xs swapChildrenClass" data-toggle="modal" @*data-target="#DetailsModal"*@ onclick="ShipLinkRevenueItems_NewRow();"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>
                                    </div>
                                    <div class="col-sm-1 m-t-27">
                                        <button id="btn-DeleteDetails" type="button" class="form-control btn btn-sm btn-danger m-t-n-xs swapChildrenClass" onclick="ShipLinkRevenueItems_DeleteList();"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>
                                    </div>
                                    <div class="col-sm-4"></div>
                                </div>
                            </div>


                            <div class="col-sm-12 m-t-sm">
                                <section id="secDetails" class="panel panel-default" style="clear:both;">
                                    <div class="table-responsive" style="width:100%; height:450px; font-size:70%; overflow:scroll;">
                                        <table id="tblShipLinkRevenueItems" class="table table-striped b-t b-light text-sm table-hover">
                                            <thead>
                                                <tr class="">
                                                    <th id="HeaderDeleteDetailsID">
                                                        <input id="cbDetailsDeleteHeader" type="checkbox" />
                                                    </th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.Item</th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.Account</th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.SubAccount</th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter</th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.Type</th>
                                                    <th class="rounded-right hide"></th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                    <footer class="panel-footer hide">
                                        <div class="col-sm-12 m-t-n-sm swapChildrenClass">
                                            <div class="col-sm-3 swapChildrenClass"><b>&emsp;@Forwarding.MvcApp.App_Resources.App_Resources.DebitTotal</b><label id="lblDebitTotal" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                            <div class="col-sm-3 swapChildrenClass"><b>&emsp;@Forwarding.MvcApp.App_Resources.App_Resources.CreditTotal</b><label id="lblCreditTotal" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                            <div class="col-sm-3 swapChildrenClass"><b>&emsp;@Forwarding.MvcApp.App_Resources.App_Resources.DebitCreditDifference</b><label id="lblDebitCreditDifference" @*purpose="dynamicLabel"*@ class="static-text-danger"></label></div>
                                            <div class="col-sm-3"></div>

                                        </div>
                                    </footer>
                                </section>
                            </div>
                            <!-- Region Add / Edit Region Form -->
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer m-t-n-lg">
                <button type="button" id="btnSave" class="btn btn-primary btn-sm swapChildrenClass" onclick="YardLinkTankCreditPosting_Post(false);">
                    <span class="glyphicon glyphicon-send"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Post&nbsp;</b>
                </button>
                <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm swapChildrenClass" onclick="YardLinkTankCreditPosting_Post(true);">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;Save Items & Post&nbsp;</b>
                </button>
                <button class="btnClose btn btn-link btn-sm" data-dismiss="modal" aria-hidden="true"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Close</b></button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div>
        </div>
    </div>
</div>
