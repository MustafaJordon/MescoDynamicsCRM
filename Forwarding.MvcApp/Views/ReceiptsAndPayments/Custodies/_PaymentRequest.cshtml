<div id="div-PaymentRequest">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none lblPayment_Custody">
                    @Forwarding.MvcApp.App_Resources.App_Resources.PaymentRequest
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm swapChildrenClass hide"  onclick="GetUserIDFromCustudy();" data-toggle="modal" data-target="#PaymentRequestModal"><i class="fa fa-plus"></i><b>@Forwarding.MvcApp.App_Resources.App_Resources.New</b></button>
                    <button id="btn-FilterOptions" class="btn btn-primary btn-sm" data-toggle="modal" onclick="" data-target="#FilterOptionsModal"><i class="fa fa-filter"></i> @Forwarding.MvcApp.App_Resources.App_Resources.FilterOptions</button>
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-7">
                    <div id="div-search-controls" class="input-group col-sm-9 hide">
                        <input id="txt-Search" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="PaymentRequest_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-3">
                        <button id="btn-Approve" type="button" class="float-right btn btn-primary btn-sm m-r" onclick="PaymentRequest_ApproveList();"><i class="fa fa-check-square-o"></i> @Forwarding.MvcApp.App_Resources.App_Resources.Approve</button>
                    </div>
                   <div class="float-right col-sm-4">
                       <button id="btn-UnApprove" type="button" class="float-right btn btn-danger btn-sm m-r" onclick="PaymentRequest_UnApproveList();"><i class="fa fa-minus-square"></i>UnApprove -فك الاعتماد </button>
                </div>


                    <div class="float-right col-sm-3">
                        <button id="btn-Delete" type="button" class="float-right btn btn-sm btn-danger m-r-n swapChildrenClass hide"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>
                    </div>
                </div>
            </div>
            <!--Region the searh, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblPaymentRequest" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID" style="width:5%;">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Code</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Custody</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</th>
                            <th> @Forwarding.MvcApp.App_Resources.App_Resources.Date</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.TotalEstimatedCost</th>
                            <th class="hideWithCustodySettlement">@Forwarding.MvcApp.App_Resources.App_Resources.TotalActualCost</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</th>
                            <th> @Forwarding.MvcApp.App_Resources.App_Resources.Notes</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Posted</th>
                            <th class="rounded-right"></th>
                            <th class="rounded-right"></th>
                            <th class="rounded-right"></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>


<!-- Add / Edit Region Modal -->
<div class="modal fade" id="PaymentRequestModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="PaymentRequestModalLabel" aria-hidden="true">
    <div class="modal-dialog  modalwidth-100 m-t-n-lg">
        <div class="modal-content">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;</button>*@
                <div class="m-b-none" style="font-size: 24px;">
                    <div class="lblPayment_Custody" style="display: inline;">@Forwarding.MvcApp.App_Resources.App_Resources.PaymentRequest</div> &nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </div>
            </div>   
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="PaymentRequest-form">
                    <fieldset>
                        <div class="form-group pull-in has-error clearfix">
                            <input id="hID" type="hidden" />
                            <!-- start alert message region -->
                            @*<div class="alert alert-danger div-error">
            @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
        </div>*@
                            <!-- end alert message region -->
                            <!-- Region Add / Edit Region Form -->
                            <div class="row swapChildrenClass">
                                <div class="form-group col-sm-2 hide">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Warehouse</label>
                                    <select id="slWarehouse" class="form-control m-b input-sm classDisableForDetails" data-required="false"></select>
                                </div>
                                <div class="form-group col-sm-2">
                                    <label> @Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                    <input type="text" id="txtCode" autocomplete="off" disabled="disabled" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="Auto-Generated" style="text-transform:uppercase;" />
                                </div>
                                <div class="form-group col-sm-4">

                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Custody</label>
                                    <div class="input-group">
                                        <select id="slCustody" onchange="" class="form-control m-b input-sm classDisableForDetails DisableWithCustody" data-required="true"></select>
                                        <span id="btnCopyCustody" title="Copy" class="input-group-addon btn-lightblue" onclick="CopyCustody();" style="padding:5px!important;color:white;cursor:pointer;">&nbsp;<span class="fa fa-filter"></span>&nbsp;</span>
                                    </div>
                                </div>
                                <div class="form-group col-sm-2">
                                    <label>تاريخ الطلب</label>
                                    <input id="txtRequestDate" type="text" readonly="readonly" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="datepicker-input form-control input-sm  DisableWithCustody" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                </div>
                                <div class="form-group col-sm-2 hideWithCustodySettlement hide">
                                    <label>تاريخ التسوية</label>
                                    <input id="txtSettlementDate" type="text" readonly="readonly" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="datepicker-input form-control input-sm  DisableWithCustody" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                </div>

                                <div class="form-group col-sm-2 hide">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Status</label>
                                    <input type="text" id="txtStatus" autocomplete="off" disabled="disabled" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="InProgress" style="text-transform:uppercase;" />
                                </div>
                                <div class="form-group col-sm-2 hide hideWithCustodySettlement">
                                    <label> @Forwarding.MvcApp.App_Resources.App_Resources.TotalActualCost</label>
                                    <input type="text" disabled="disabled" id="txtTotalActualCost" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                                </div>
                                <div class="form-group col-sm-2">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.TotalEstimatedCost</label>
                                    <input type="text" disabled="disabled" id="txtAmount" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                                </div>
                                <div class="form-group col-sm-2 hide  hideWithCustodySettlement">
                                    <label>مبلغ التسوية</label>
                                    <input type="text" disabled="disabled" id="txtSettlementAmount" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                                </div>
                                <div class="form-group col-sm-2">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                                    <select id="slCurrency" class="form-control m-b input-sm DisableWithCustody" data-required="true"></select>
                                </div>
                            </div>
                            <div class="row swapChildrenClass">
                                <div class="form-group col-sm-3">
                                    <label>Service Category</label>
                                    <select id="slServiceCategory" class="form-control m-b input-sm DisableWithCustody" data-required="false"></select>
                                </div>

                                <div class="form-group col-sm-3">
                                    <label>Attach</label>
                                    <input type="text" id="txtAttach" autocomplete="off" class="form-control parsley-validated input-sm DisableWithCustody" data-required="false" maxlength="200" placeholder="Notes" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group col-sm-3 hidden">
                                    <label>Payment</label>
                                    <input type="text" id="txtPayment" autocomplete="off" class="form-control parsley-validated input-sm DisableWithCustody" data-required="false" maxlength="200" placeholder="Notes" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group col-sm-2">
                                    <label>Payment</label>
                                    <select id="slPaymentCombo" class="form-control m-b input-sm DisableWithCustody" data-required="false"></select>
                                </div>
                                <div class="form-group col-sm-2">
                                    <label>IssuedTo</label>
                                    <select id="slIssuedToCmbo" class="form-control m-b input-sm DisableWithCustody" data-required="false"></select>
                                </div>
                                <div class="form-group col-sm-2">
                                    <label>IssuedTo</label>
                                    <input type="text" id="txtIssuedTo" autocomplete="off" class="form-control parsley-validated input-sm DisableWithCustody" data-required="false" maxlength="200" placeholder="Notes" style="text-transform:uppercase" />
                                </div>
                            </div>
                            <div class="row swapChildrenClass">
                                <div class="col-sm-1   swapChildrenClass">
                                    <input tabindex="" class=" DisableWithCustody" type="radio" name="nameCashOrCheck" id="cbIsCash" checked="checked" onchange="" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Cash</b>
                                </div>
                                <div class="col-sm-1  swapChildrenClass">
                                    <input tabindex="" class="DisableWithCustody" type="radio" name="nameCashOrCheck" id="cbIsCheck" onchange="" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b> @Forwarding.MvcApp.App_Resources.App_Resources.Cheque</b>
                                </div>
                               
                                <div class="form-group col-sm-6">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                    <input type="text" id="txtNotes" autocomplete="off" class="form-control parsley-validated input-sm DisableWithCustody" data-required="false" maxlength="200" placeholder="Notes" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group col-sm-1 m-t-27 hide">
                                    <label><input type="checkbox" id="cbIsPosted">&nbsp;Posted</label>
                                </div>
                            </div>
                            <!--
        <div id="divFinalize" class="col-sm-3 m-t-md">
            <button id="btn-Finalize" class="btn btn-lightblue btn-sm m-t-xxs classDisableForPosted" data-toggle="modal" data-target="" onclick="PaymentRequest_Finalize();"><i class="fa fa-hand-o-right"></i>&nbsp;Finalize</button>
            @*<button id="btn-GenerateDefaultLocations" class="btn btn-info btn-sm m-t-xxs" data-toggle="modal" data-target="" onclick="PaymentRequestDetails_GenerateDefaultLocations();"><i class="fa fa-hand-o-down"></i>&nbsp;Default Locations</button>*@
        </div>
    -->
                            <!--Details-->
                            <div id="divPaymentRequestDetailsBtns" class="col-sm-12 m-t" style="clear:both;">
                                <button id="btn-AddPaymentRequestDetails" class="btn btn-warning btn-sm clclassDisableForPosted" data-toggle="modal" d
                                    <div id="divPaymentRequestDetailsBtns" class="col-sm-12 m-t" style="clear:both;">
                                        <button id="btn-AddPaymentRequestDetails" class="btn btn-warning btn-sm classDisableForPosted" data-toggle="modal" data-target="" onclick="Details_NewRow();"><i class="fa fa-plus"></i> @Forwarding.MvcApp.App_Resources.App_Resources.AddDetails</button>
                                        <button id="btn-DeletePaymentRequestDetails" title="Delete Details" type="button" class="float-right btn btn-sm btn-danger classDisableForPosted" onclick="PaymentRequestDetails_DeleteList();"><i class="fa fa-trash-o"></i>@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                                    </div>

                                    <div id="divPaymentRequestDetails" class="col-sm-12 m-t-xs">
                                        <section class="panel panel-default">
                                            <header class="panel-heading font-bold text-ul"> @Forwarding.MvcApp.App_Resources.App_Resources.Details</header>
                                            <!--Region: table header-->
                                            <div class="table-responsive" style="width:100%; height:300px; overflow:scroll;">
                                                <table id="tblPaymentRequestDetails" class="table table-striped b-t b-light text-sm  table-hover ">
                                                    <thead class="swapChildrenClass">
                                                        <tr>
                                                            <th id="HeaderDeletePaymentRequestDetailsID" style="width:5%;">
                                                                <input id="cb-CheckAll-PaymentRequestDetails" type="checkbox" />
                                                            </th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.OperationNo</th>
                                                            <th class="classShowForDepartementHouse hide"> @Forwarding.MvcApp.App_Resources.App_Resources.HouseNumber</th>
                                                            @* <th class=""> رقم الشهادة</th>*@
                                                            <th class="classShowForDepartementTrOr hide"> @Forwarding.MvcApp.App_Resources.App_Resources.TruckingOrders</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ChooseProcessItems</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ChargeTypes</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Supplier</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.TotalEstimatedCost</th>
                                                            <th class="classShowForDepartementCopon hide">@Forwarding.MvcApp.App_Resources.App_Resources.Coupon</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.SupplierInvoiceNumber</th>

                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</th>
                                                            <th class="rounded-right hide"></th>
                                                        </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                    <!--Region: EOF table header-->
                                    <!--Region: table footer-->
                                    <footer class="panel-footer hide">
                                        <div class="col-sm-12 m-t-n-sm swapChildrenClass">
                                            <!--
                        <div class="col-sm-6 swapChildrenClass"><b>&emsp;Row Max Weight(KGM)</b><label id="lblRowMaxWeight" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                        <div class="col-sm-6 text-right swapChildrenClass"><b>&emsp;Row Max Volume(CBM)</b><label id="lblRowMaxVolume" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                    -->
                                        </div>
                                    </footer>
                                    <!--Region: EOF table footer-->
                                </section>
                                                </div>

                            </div>

                            <!--EOF Details-->
                        </div>
                    </fieldset>
                </div>
                <div class="modal-footer m-t-n-lg">
                    <button type="button" id="btnSave" class="btn btn-primary btn-sm classDisableForPosted" onclick="AlarmUsers_SavePaymentRequest();" @*onclick="PaymentRequest_Save(false);"*@>
                        <span class="glyphicon glyphicon-save"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Save
                    </button>
                    <button type="button" id="btnPrint" class="btn btn-lightblue btn-sm" onclick="PaymentRequest_Print(0);">
                        <span class="fa fa-print"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Print
                    </button>
                    <button type="button" id="btnSaveAndAddNew" class="btn btn-primary btn-sm classDisableForPosted hide" onclick="">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                    </button>
                    <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            
        </div>
    </div>
</div>
