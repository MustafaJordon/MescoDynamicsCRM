<div class="modal fade" id="ClientsModal" tabindex="-1" style="z-index: 1100;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="ClientModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-80">
        <div class="modal-content m-t-n-lg">
            <div class="modal-header">
                <h3 class="m-b-none swapChildrenClass">
                    <span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SL_Clients&nbsp;</span><label id="lblShown" purpose="dynamicLabel" @*to be cleared*@ class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode m-t-n-md" id="Client-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hID" type="hidden" />
                            <!--Wizard-->
                            <!-- start alert message region -->
                            <div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>
                            <div class="wizard clearfix">
                                <ul class="steps">
                                    <li data-target="#BasicDataSupplier" id="stepsBasicDataSupplier" class="active"><span class="badge badge-info">1</span>@Forwarding.MvcApp.App_Resources.App_Resources.BasicData</li>
                                    <li data-target="#tabAccounting" id="stepsAccounting" class="complete"><span class="badge badge-info">1</span>@Forwarding.MvcApp.App_Resources.App_Resources.Accounting</li>
                                </ul>
                                <div class="actions" style="display:none;">
                                    <!--Next & Previous buttons-->
                                    <button type="button" class="btn btn-default btn-xs btn-prev" disabled="disabled">@Forwarding.MvcApp.App_Resources.App_Resources.Prev</button>
                                    <button type="button" class="btn btn-default btn-xs btn-next" data-last="Finish">@Forwarding.MvcApp.App_Resources.App_Resources.Next</button>
                                </div>
                            </div>
                            <div class="step-content">
                                <!--Basic Data-->
                                <div class="step-pane active" id="BasicDataSupplier">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    <!-- Add / Edit Basic Data -->
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                        <input type="text" id="txtClientCode" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" data-minlength="1" maxlength="10" placeholder="Generated Automatically" disabled="disabled" style="text-transform:uppercase" />
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                                        <input type="text" id="txtClientName" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Notes" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.EnName</label>
                                        <input type="text" id="txtClientEnglishName" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="25" placeholder="ex. English Name" style="text-transform:uppercase" />
                                    </div>


                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Address</label>
                                        @*<input type="text" id="txtClientaddressName" style="resize:none;" rows="2" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Address" style="text-transform:uppercase" />*@
                                        <textarea id="txtClientaddressName" style="resize:none;" rows="2" class="form-control parsley-validated Edit-input  input-sm"></textarea>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Phone</label>
                                        <input type="text" id="txtrClientPhoneNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="25" placeholder="ex. PhoneNumber" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                        <input type="text" id="txtClientNotes" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Notes" style="text-transform:uppercase" />
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Fax</label>
                                        <input type="text" id="txtClientFax" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Fax" style="text-transform:uppercase" />
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Email</label>
                                        <input type="text" id="txtClientEmail" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Email" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>Payment Bank</label>
                                        <select id="slBank" class="input-sm form-control Edit-input" data-required="false" onchange=""></select>
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label>TaxCard</label>
                                        <input type="text" id="txttaxCard" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Tax" style="text-transform:uppercase" />
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label>FileNo</label>
                                        <input type="text" id="txtFileNo" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. FileNo" style="text-transform:uppercase" />
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label>TaxOffice</label>
                                        <input type="text" id="txtTaxOffice" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. TaxOffice" style="text-transform:uppercase" />
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label>CommercialRegistration</label>
                                        <input type="text" id="txtCommercialRegistration" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. CommercialRegistration" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>ID Number</label>
                                        <input type="text" id="txtIDNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. IDNumber" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>Grace Period</label>
                                        <input type="text" id="txtGracePeriod" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. 123" style="text-transform:uppercase" />
                                    </div>
                                    <!--EOF Add / Edit Basic Data -->
                                </div>


                                <div class="step-pane" id="tabAccounting">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label>ClientsGroup</label>
                                        <select id="slSubAccountGroup" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onchange="FillSlAccountFromGroup('slAccount', 'slSubAccountGroup', 0, 0, 0);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Account</label>
                                        <select id="slAccount" class="form-control m-b" @*onchange="FillSlSubAccount('slSupplierSubAccount', 'slSupplierAccount', 0, 0);"*@ onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                                    </div>

                                    @*<div class="form-group col-sm-6">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.SubAccount</label>
                                            <select id="slClientsSubAccount" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                                        </div>*@
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.SubAccount</label>
                                        <input type="text" id="slClientsSubAccount" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" data-minlength="1" maxlength="10" placeholder="Generated Automatically" disabled="disabled" style="text-transform:uppercase" />
                                    </div>
                                    <div class="hide form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter</label>
                                        <select id="slClientsCostCenter" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                                    </div>
                                    <!--The id of the next div is used to fill the Accounting in the textareas-->
                                    <div id="bodySupplierAccounting"></div>
                                </div>
                                <!--Accounting-->
                                <!--Employee Data-->
                                <div class="step-pane active" id="EmployeeData">

                                    <!-- Add / Edit Employee Data -->
                                    <!--EOF Add / Edit Basic Data -->
                                </div>
                                <!--EOF Employee Data-->
                                <!--ApplicationData-->
                                <div class="step-pane" id="ApplicationData">

                                </div>
                                <!--EOF EmployeeSalary-->
                            </div>
                        </div>
                        <!-- Endregion Add / Edit Region Form -->
                        <div class="modal-footer">
                            <button type="button" id="btnSave" class="btn btn-primary btn-sm swapChildrenClass" onclick="Suppliers_Save(false);">
                                <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Save&nbsp;</b>
                            </button>
                            <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm swapChildrenClass" onclick="Suppliers_Save(true);">
                                <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew&nbsp;</b>
                            </button>
                            <button class="btnClose btn btn-link btn-sm" id="btnClose" data-dismiss="modal" aria-hidden="true"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Close</b></button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
                            @*onclick="Supplier_UnlockRecord();"*@
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </div>
</div>




<div id="div-Client">
    <!-- Region Options & Template  -->
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.Client</h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div id="FilterSec" class="row m-t-sm">


                <div class="form-group  col-sm-2">
                    <div class="col-sm-12">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                        <input id="txtCodeSearch" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated  input-sm" data-required="false" data-minlength="1" maxlength="10" placeholder="ex. 25" style="text-transform:uppercase" />
                    </div>
                </div>

                <div class="form-group  col-sm-2">
                    <div class="col-sm-12">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                        <input id="txtNameSearch" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated  input-sm" data-required="false" data-minlength="1" maxlength="150" placeholder="ex. ahmed" style="text-transform:uppercase" />
                    </div>
                </div>
                <div class="form-group  col-sm-2">
                    <div class="col-sm-12">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.EnName</label>
                        <input id="txtENameSearch" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated  input-sm" data-required="false" data-minlength="1" maxlength="150" placeholder="ex. ahmed" style="text-transform:uppercase" />
                    </div>
                </div>

                <div class="form-group  col-sm-2">
                    <div class="col-sm-12">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                        <input id="txtNotesSearch" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated  input-sm" data-required="false" data-minlength="1" maxlength="210" placeholder="ex.AAA" style="text-transform:uppercase" />
                    </div>
                </div>
                <div class="form-group  col-sm-2">
                    <div class="col-sm-12">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Address</label>
                        <input id="txtAddressSearch" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated  input-sm" data-required="false" data-minlength="1" maxlength="210" placeholder="ex. Alex" style="text-transform:uppercase" />
                    </div>
                </div>
                <div class="form-group  col-sm-2">
                    <label>Group</label>
                    <select id="slClientsSubAccountGroupSearch" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                </div>


                <div class="form-group  col-sm-3" style="margin-top:5px;">
                    <div class="input-group col-sm-12">

                        <button id="btn-search1" class="btn btn-sm btn-lightblue" type="button" onclick="Clients_LoadingWithPaging();"><i class="fa fa-search">&nbsp;Search</i></button>
                        &nbsp;&nbsp;
                        <button id="btn-NewAdd" class="btn btn-warning btn-sm hide" data-toggle="modal" data-target="#ClientsModal"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New</button>
                        &nbsp;
                        <button id="btn-Delete" type="button" class="btn btn-sm btn-danger m-r-n hide"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>

                        <div class="">
                        </div>
                    </div>
                </div>
            </div>

            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm hide" data-toggle="modal" data-target="#ClientsModal"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New</button>
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                    </div>
                </div>

                <div id="div-search-option" class="col-sm-7 hide">
                    <div id="div-search-controls" class="input-group col-sm-4">
                        @*<label>@Forwarding.MvcApp.App_Resources.App_Resources.ClientsGroup</label>*@
                        <select id="slClientsSubAccountGroupSearch" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                    </div>
                    <div id="div-search-controls" class="input-group col-sm-6">
                        <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search Code / Name / Addresss">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="Clients_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-1">
                        <button id="btn-Delete" type="button" class="float-right btn btn-sm btn-danger m-r-n hide"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                    </div>
                </div>
            </div>
            <!--EOF Region the searh, delete, addnew line-->
            <!--Suppliers Modal-->
            <!--EOF Suppliers Modal-->
            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblClients" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th class="th-sortable" data-toggle="class">
                                @Forwarding.MvcApp.App_Resources.App_Resources.Code
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Name
                            </th>

                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Notes
                            </th>

                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.EnName
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Address
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Phone
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Fax
                            </th>


                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Email
                            </th>
                            <th class="rounded-right hide"></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>
