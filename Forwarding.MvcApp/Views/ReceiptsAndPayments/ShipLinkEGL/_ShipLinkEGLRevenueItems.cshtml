<div id="div-JVTypes">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.ShipLinkEGLRevenueItems
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-t-n-sm">
                <div class="form-group  col-sm-2">
                    <label>Item</label>
                    <select id="slItemSearch" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                </div>
                <div class="form-group  col-sm-2">
                    <label>Account</label>
                    <select id="slAccountSearch" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                </div>
                <div class="form-group  col-sm-2">
                    <label>CostCenter</label>
                    <select id="slCostCenterSearch" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                </div>
                <div class="form-group  col-sm-2">
                    <label>Type</label>
                    <select id="slTypeSearch" class="form-control input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false">
                        <option value="0">ALL</option>
                        <option value="Im">IMPORT</option>
                        <option value="Ex">EXPORT</option>

                    </select>
                </div>

            </div>
            <div class="row text-sm wrapper hidden-print m-t-n-sm">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <button id="btn-search1" class="btn btn-sm btn-lightblue" type="button" onclick="ShipLinkEGLRevenueItems_LoadingWithPaging();"><i class="fa fa-search">&nbsp;Search</i></button>
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm swapChildrenClass hide" data-toggle="modal" data-target="#ShipLinkEGLRevenueItemsModal"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm swapChildrenClass hide" data-toggle="modal" data-target="#ShipLinkEGLRevenueItemsModal"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>
                    <button id="btn-Delete" type="button" class="btn btn-sm btn-danger m-r-n swapChildrenClass hide"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>

                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                    </div>
                </div>

                <div id="div-search-option" class="col-sm-7">
                    <div id="div-search-controls" class="hidden input-group col-sm-9">
                        <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="ShipLinkEGLRevenueItems_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    @*<div class="float-right col-sm-1">
                            <button id="btn-Delete" type="button" class="float-right btn btn-sm btn-danger m-r-n swapChildrenClass hide"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>
                        </div>*@
                </div>
            </div>
            <!--Region the searh, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblShipLinkEGLRevenueItems" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Item</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Account</th>
                            <th id="thVoyageSubAccountID" class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.SubAccount</th>
                            <th id="thRevenueSubAccountID20" class="hide">SubAccount20</th>
                            <th id="thRevenueSubAccountID40" class="hide">SubAccount40</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Type</th>
                            <th class="rounded-right hide"></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>


<!-- Add / Edit Region Modal -->
<div class="modal fade" id="ShipLinkEGLRevenueItemsModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="ShipLinkEGLRevenueItemsModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-80">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h3 class="m-b-none swapChildrenClass">
                    <span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.ShipLinkEGLRevenueItems &nbsp;</span><label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="JVTypes-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hID" type="hidden" />
                            <!-- start alert message region -->
                            @*<div class="alert alert-danger div-error">
                                    @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                </div>*@
                            <!-- end alert message region -->
                            <!-- Region Add / Edit Region Form -->
                            <div class="form-group col-sm-3">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Item</label>
                                <select id="slShiplinkItem" class="input-sm form-control Edit-input" data-required="true" onchange=""></select>
                            </div>
                            <div class="form-group col-sm-3">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Account</label>
                                <select id="slRevenueAccount" class="input-sm form-control Edit-input" data-required="true" onchange=""></select>
                            </div>

                            <div class="form-group col-sm-3">
                                <label>SubAccount20</label>
                                <select id="slRevenueSubAccount20" class="input-sm form-control Edit-input" data-required="false" onchange=""></select>
                            </div>
                            <div class="form-group col-sm-3">
                                <label>SubAccount40</label>
                                <select id="slRevenueSubAccount40" class="input-sm form-control Edit-input" data-required="false" onchange=""></select>
                            </div>
                            <div class="form-group col-sm-3">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter</label>
                                <select id="slCostCenter" class="input-sm form-control Edit-input" data-required="false" onchange=""></select>
                            </div>
                            <div class="form-group col-sm-3">
                                <label>Type</label>
                                <select id="slImportExport" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm">
                                    <option value="Im">Import</option>
                                    <option value="Ex">Export</option>
                                </select>
                            </div>
                            <div class="form-group col-sm-6"></div>
                            <select id="slRevenueItem" class="hide form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>

                            @*<div class="col-sm-6 m-t-27 ">
                                    <label><input type="checkbox" id="cbIsReligional" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Religional</label><br />
                                </div>*@
                            <!-- Region Add / Edit Region Form -->
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer">
                <button type="button" id="btnSave" class="btn btn-primary btn-sm swapChildrenClass" onclick=" ShipLinkEGLRevenueItems_Insert(false);">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Save&nbsp;</b>
                </button>
                <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm swapChildrenClass" onclick=" ShipLinkEGLRevenueItems_Insert(true);">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew&nbsp;</b>
                </button>
                <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Close</b></button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div>
        </div>
    </div>
</div>
<!-- Region Options & Template  -->
