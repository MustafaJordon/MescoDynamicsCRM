                        </div>*@
                    
                        <div class="form-group col-sm-3">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Type</label>
                            <select id="slIsCash" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm" data-required="false">
                                <option value="-1">@Forwarding.MvcApp.App_Resources.App_Resources.All</option>
                                <option value="1">@Forwarding.MvcApp.App_Resources.App_Resources.Cash</option>
                                <option value="0">@Forwarding.MvcApp.App_Resources.App_Resources.Bank</option>

                            </select>
                        </div>

                        @*<div class="form-group col-sm-3 m-l-n hide">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.BankAccount</label>
                            <select id="slBankAccount" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                        </div>*@

                        @*<br /><br /><br /><br />*@
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                            <select id="slCurrency" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                        </div>

                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                        <input id="txtToDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="col-sm-5"></div>

                    </div>
            </div>

            <div class="row text-sm wrapper hidden-print m-l m-t-n-sm swapChildrenClass">
                <div class="swapChildrenClass">
                    <div class="form-group col-sm-2 m-l-n hide">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                        <input id="txtFromDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
