<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="ReceiptsAndPayments" href="javascript:void(0);" onclick="LoadViews('ReceiptsAndPaymentsApprovingAndPosting');">@Forwarding.MvcApp.App_Resources.App_Resources.ReceiptsAndPaymentsGroup
    </a></li>
    <li><a id="ReceiptsAndPaymentsMasterData" href="javascript:void(0);" onclick="LoadViews('ReceiptsAndPaymentsApprovingAndPosting');">@Forwarding.MvcApp.App_Resources.App_Resources.RecAndPayApprovingAndPosting
    </a></li>
    <li id="liPost_Unpost_Voucher" class="active"></li>
</ul>
<!-- end breadcrumb  -->

<!--Region the searh, delete, addnew-->
<div id="div-Voucher">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 id="h3Post_Unpost_Voucher" class="m-b-none">
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">
                <div class="col-sm-2 hide">
                    <select id="slSearchTax1" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                </div>
                <div class="col-sm-2 hide">
                    <select id="slSearchTax2" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                </div>
                <div class="col-sm-3">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.CashOrCharge</label>
                    <select id="slSearchVoucherType" tabindex="5" class="form-control input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false">
                        <option value="10" selected="selected">@Forwarding.MvcApp.App_Resources.App_Resources.CashReceivingVoucher</option>
                        <option value="20">@Forwarding.MvcApp.App_Resources.App_Resources.CashIssueVoucher</option>
                        <option value="30">@Forwarding.MvcApp.App_Resources.App_Resources.ChequeReceivingVoucher</option>
                        <option value="40">@Forwarding.MvcApp.App_Resources.App_Resources.ChequeIssueVoucher</option>
                    </select>
                </div>
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                    <input id="txtSearchCode" tabindex="10" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="50" placeholder="" style="text-transform:uppercase"/>
                </div>
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                    <input id="txtSearchFrom" tabindex="20" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                </div>
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                    <input id="txtSearchTo" tabindex="30" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                </div>
                <div class="col-sm-3">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Safe</label>
                    <select id="slSearchSafe" tabindex="40" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                </div>
            </div>
            <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">
                <div class="swapChildrenClass m-t-n">
                    <div class="col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.ChargedPerson</label>
                        <input id="txtSearchChargedPerson" tabindex="50" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="50" placeholder="" data-required="false" style="text-transform:uppercase"/>
                    </div>
                    <div class="col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Amount</label>
                        <input id="txtSearchTotal" tabindex="60" type="text" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" @*placeholder="value from 0.00001 to 99999.99999"*@ />
                    </div>
                    <div class="col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                        <select id="slSearchCurrency" tabindex="70" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                    </div>
                    <div class="col-sm-2 hide">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                        <input id="txtSearchNotes" tabindex="80" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="100" placeholder="" data-required="false" style="text-transform:uppercase"/>
                    </div>
                    <div class="col-sm-2 hide">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.CashOrCharge</label>
                        <select id="slSearchCashOrCharge" tabindex="90" class="form-control input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false">
                            <option value="0">@Forwarding.MvcApp.App_Resources.App_Resources.SelectFromMenu</option>
                            <option value="10">@Forwarding.MvcApp.App_Resources.App_Resources.Cash</option>
                            <option value="20">@Forwarding.MvcApp.App_Resources.App_Resources.Charge</option>
                        </select>
                    </div>
                    <div class="col-sm-2 hide">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Status</label>
                        <select id="slSearchStatus" tabindex="100" class="form-control input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false">
                            <option value="0">@Forwarding.MvcApp.App_Resources.App_Resources.SelectFromMenu</option>
                            <option value="10">@Forwarding.MvcApp.App_Resources.App_Resources.Approved</option>
                            <option value="20">@Forwarding.MvcApp.App_Resources.App_Resources.NotApproved</option>
                        </select>
                    </div>
                    <div class="col-sm-1 m-t-27 showForPosting hide">
                        <label class="swapChildrenClass"><input tabindex="107" type="checkbox" id="cbIsJVDate" onchange="Post_Unpost_Voucher_cbIsJVDateChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><span>@Forwarding.MvcApp.App_Resources.App_Resources.JVDate</span></label>
                    </div>
                    <div class="col-sm-2 m-t-md showForPosting hide">
                        @*<label>@Forwarding.MvcApp.App_Resources.App_Resources.JVDate</label>*@
                        <input id="txtJVDate" disabled="disabled" tabindex="109" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="/*background-color:white;*/ cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class=" col-sm-3 hideForPosting"></div>
                    <div class=" col-sm-3 swapChildrenClass m-t-md">
                        <button id="btn-Search" tabindex="110" type="button" onclick="Post_Unpost_Voucher_LoadingWithPaging();" class="btn btn-lightblue btn-sm swapChildrenClass" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-search"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Search&nbsp;</b></button>
                        <button id="btn-Post" tabindex="120" type="button" onclick="Post_Unpost_Voucher_PostOrUnpostList(1);" class="btn btn-warning btn-sm swapChildrenClass hide" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-mail-forward"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.PostingVouchers&nbsp;</b></button>
                        <button id="btn-UnPost" tabindex="130" type="button" onclick="Post_Unpost_Voucher_PostOrUnpostList(0);" class="btn btn-warning btn-sm swapChildrenClass hide" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-undo"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.UnpostingVouchers&nbsp;</b></button>
                    </div>
                </div>
            </div>
            <!--EOF Region the searh, delete, addnew-->

            <!--Region: table header-->
            <div class="table-responsive m-t-n" style="font-size:80%;">
                <table id="tblVoucher" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Code</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.VoucherDate</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Safe</th>
                            <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.Notes</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Amount</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ChargedPerson</th>
                            <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.Cash</th>
                            <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.Approved</th>
                            <th class="rounded-right "></th>
                            <th class="rounded-right hide"></th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm"> @Forwarding.MvcApp.App_Resources.App_Resources.Showing 
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>
<!--Region the searh, delete, addnew-->
