                                        <select id="slTrucker" onchange="" class="form-control m-b input-sm classDisableForDetails DisableWithCustody" data-required="false"></select>
                                    </div>
                                </div>
                            </div>
                                <div class="row swapChildrenClass">
                                    <div class="form-group col-sm-2 ">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.HouseNumber</label>
                                        <select id="slHouseNumber" class="form-control m-b input-sm " data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-2 ">
                                        <label>TR Supplier(Containers)</label>
                                        <select id="slTruckingOrders" class="form-control m-b input-sm " data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-2">
                                        <label>تاريخ الطلب</label>
                                        <input id="txtRequestDate" type="text" readonly="readonly" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="datepicker-input form-control input-sm  DisableWithCustody" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                    </div>
                                    <div class="form-group col-sm-2 hideWithCustodySettlement hide">
                                        <label>تاريخ التسوية</label>
                                        <input id="txtSettlementDate" type="text" readonly="readonly" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="datepicker-input form-control input-sm  DisableWithCustody" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                    </div>
                                    <div class="form-group col-sm-2 hide">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Status</label>
                                        <input type="text" id="txtStatus" autocomplete="off" disabled="disabled" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="InProgress" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-2 hide hideWithCustodySettlement">
                                        <label> @Forwarding.MvcApp.App_Resources.App_Resources.TotalActualCost</label>
                                        <input type="text" disabled="disabled" id="txtTotalActualCost" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                                    </div>
                                    <div class="form-group col-sm-2 hide">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.TotalEstimatedCost</label>
                                        <input type="text" disabled="disabled" id="txtAmount" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                                    </div>
                                    <div class="form-group col-sm-2 hide  ">
                                        <label>مبلغ التسوية</label>
                                        <input type="text" disabled="disabled" id="txtSettlementAmount" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                                    </div>
                                    <div class="form-group col-sm-2 hide">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                                        <select id="slCurrency" class="form-control m-b input-sm DisableWithCustody" data-required="true"></select>
                                    </div>
                                </div>
                            <div class="row swapChildrenClass">
                                <div class="form-group col-sm-3 hidden">
                                    <label>Payment</label>
                                    <input type="text" id="txtPayment" autocomplete="off" class="form-control parsley-validated input-sm DisableWithCustody" data-required="false" maxlength="200" placeholder="Notes" style="text-transform:uppercase" />
                                </div>
                            </div>
                            <div class="row swapChildrenClass">
                                <div class="col-sm-1  m-t-27 swapChildrenClass">
                                    <input tabindex="" class=" DisableWithCustody" type="radio" name="nameCashOrCheck" id="cbIsCash" checked="checked" onchange="" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Cash</b>
                                </div>
                                <div class="col-sm-1 m-t-27 swapChildrenClass">
                                    <input tabindex="" class="DisableWithCustody" type="radio" name="nameCashOrCheck" id="cbIsCheck" onchange="" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b> @Forwarding.MvcApp.App_Resources.App_Resources.Cheque</b>
                                </div>
                               
                                <div class="form-group col-sm-6">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                    <input type="text" id="txtNotes" autocomplete="off" class="form-control parsley-validated input-sm DisableWithCustody" data-required="false" maxlength="200" placeholder="Notes" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group col-sm-1 m-t-27 hide">
                                    <label><input type="checkbox" id="cbIsPosted">&nbsp;Posted</label>
                                </div>
                            </div>
                            <!--
        <div id="divFinalize" class="col-sm-3 m-t-md">
            <button id="btn-Finalize" class="btn btn-lightblue btn-sm m-t-xxs classDisableForPosted" data-toggle="modal" data-target="" onclick="PaymentRequestIntegration_Finalize();"><i class="fa fa-hand-o-right"></i>&nbsp;Finalize</button>
            @*<button id="btn-GenerateDefaultLocations" class="btn btn-info btn-sm m-t-xxs" data-toggle="modal" data-target="" onclick="PaymentRequestIntegrationDetails_GenerateDefaultLocations();"><i class="fa fa-hand-o-down"></i>&nbsp;Default Locations</button>*@
        </div>
    -->
                            <!--Details-->
                            <div id="divPaymentRequestIntegrationDetailsBtns" class="col-sm-12 m-t" style="clear:both;">
                                    <div id="divPaymentRequestIntegrationDetailsBtns" class="col-sm-12 m-t" style="clear:both;">
                                        <button id="btn-AddPaymentRequestIntegrationDetails" class="btn btn-warning btn-sm classDisableForPosted" data-toggle="modal" data-target="" onclick="Details_NewRow();"><i class="fa fa-plus"></i> @Forwarding.MvcApp.App_Resources.App_Resources.AddDetails</button>
                                        <button id="btn-DeletePaymentRequestIntegrationDetails" title="Delete Details" type="button" class="float-right btn btn-sm btn-danger classDisableForPosted" onclick="PaymentRequestIntegrationDetails_DeleteList();"><i class="fa fa-trash-o"></i>@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                                    </div>

                                    <div id="divPaymentRequestIntegrationDetails" class="col-sm-12 m-t-xs">
                                        <section class="panel panel-default">
                                            <header class="panel-heading font-bold text-ul"> @Forwarding.MvcApp.App_Resources.App_Resources.Details</header>
                                            <!--Region: table header-->
                                            <div class="table-responsive" style="width:100%; height:300px; overflow:scroll;">
                                                <table id="tblPaymentRequestIntegrationDetails" class="table table-striped b-t b-light text-sm  table-hover ">
                                                    <thead class="swapChildrenClass">
                                                        <tr>
                                                            <th id="HeaderDeletePaymentRequestIntegrationDetailsID" style="width:5%;">
                                                                <input id="cb-CheckAll-PaymentRequestIntegrationDetails" type="checkbox" />
                                                            </th>
                                                            <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.OperationNo</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ChargeTypes</th>
                                                            <th class="classShowForDepartementHouse hide"> @Forwarding.MvcApp.App_Resources.App_Resources.HouseNumber</th>
                                                            @* <th class=""> رقم الشهادة</th>*@
                                                            <th class="classShowForDepartementTrOr hide"> @Forwarding.MvcApp.App_Resources.App_Resources.TruckingOrders</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ChooseProcessItems</th>
                                                
                                                            <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.Supplier</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.TotalEstimatedCost</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</th>
                                                            <th class=" hide">@Forwarding.MvcApp.App_Resources.App_Resources.Coupon</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.SupplierInvoiceNumber</th>

                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</th>
                                                            <th class="rounded-right hide"></th>
                                                        </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                    <!--Region: EOF table header-->
                                    <!--Region: table footer-->
                                    <footer class="panel-footer ">
                                        <div class="col-sm-12 m-t-n-sm swapChildrenClass">
                                        <div class="col-sm-12 text-right swapChildrenClass">
                                            <b>&emsp;Estimated Cost:</b><label id="lblCostByCurrency" @*purpose="dynamicLabel"*@ class="static-text-blue"></label>
                                         </div>
                                        </div>
                                    </footer>
                                    <!--Region: EOF table footer-->
                                </section>
                                                </div>

                            </div>

                            <!--EOF Details-->
                        </div>
                    </fieldset>
                </div>
                <div class="modal-footer m-t-n-lg">
                    <button type="button" id="btnSave" class="btn btn-primary btn-sm classDisableForPosted" onclick="AlarmUsers_SavePaymentRequestIntegration();" @*onclick="PaymentRequestIntegration_Save(false);"*@>
                        <span class="glyphicon glyphicon-save"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Save
                    </button>
                    <button type="button" id="btnPrint" class="btn btn-lightblue btn-sm" onclick="PaymentRequestIntegration_Print(0);">
                        <span class="fa fa-print"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Print
                    </button>
                    <button type="button" id="btnSaveAndAddNew" class="btn btn-primary btn-sm classDisableForPosted hide" onclick="">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                    </button>
                    <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            
        </div>
    </div>
</div>
