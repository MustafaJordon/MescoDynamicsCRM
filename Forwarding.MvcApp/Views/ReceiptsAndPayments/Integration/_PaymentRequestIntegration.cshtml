                                        <select id="slClosingOperation" onchange="" class="form-control m-b input-sm classDisableForDetails DisableWithCustody" data-required="false"></select>
                                    </div>
                                </div>
                                <div class="form-group col-sm-2">
                                    <input tabindex="" class=" DisableWithCustody" type="radio" name="nameType" id="cbIsTrucker" checked="checked" onchange="" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Trucker</b>
