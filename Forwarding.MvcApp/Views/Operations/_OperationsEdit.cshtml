<div class="modal fade" id="OperationComplaintsModal" tabindex="-1" style="z-index:1000;" @*data-keyboard="false"*@ data-backdrop="false" role="dialog" aria-labelledby="OperationComplaintsModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-90">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true" title="Close">
                    &times;
                </button>
                <h3 class="m-b-none swapChildrenClass">
                    @Forwarding.MvcApp.App_Resources.App_Resources.OperationComplaints&nbsp;<label id="lblOperationComplaintsShown" purpose="dynamicLabel" class="static-text-primary"></label>
                    @*<button type="button" class="close" title="Close" data-dismiss="modal" onclick="MAWBStockSelect_ResetFunctionsNames();" aria-hidden="true">&times;</button>*@
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="OperationComplaints-form">
                    <fieldset>
                        <button id="btn-NewAdd" class="btn btn-warning btn-sm swapChildrenClass" onclick="Complaint_ClearAllControls();" data-toggle="modal" data-target="#ComplaintModal"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>

                        <div class="table-responsive">
                            <table id="tblComplaint" class="table table-striped b-t b-light text-sm  table-hover">
                                <thead>
                                    <tr>
                                        <th id="ID" style="width:5%;">
                                            <input id="cb-CheckAll" type="checkbox" />
                                        </th>
                                        <th>@Forwarding.MvcApp.App_Resources.App_Resources.Code</th>
                                        <th>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</th>
                                        <th>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</th>
                                        <th class="rounded-right"></th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer">
                <button type="button" id="btnOperationComplaints" class="btn btn-primary btn-sm" onclick="Operations_OperationComplaintsSave();">
                    <span class="glyphicon glyphicon-save"></span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Save
                </button>
                @*<button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm" onclick="MAWBStockSelect_Insert(true);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew</button>*@
                <button class="btnClose btn btn-link btn-sm" title="Close" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div>
        </div>
    </div>
</div>

<!-- Add / Edit Region Modal -->
<div class="modal fade" id="ComplaintModal" tabindex="-1" data-keyboard="true" style="z-index: 2000;" data-backdrop="false" role="dialog" aria-labelledby="ComplaintModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-80">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h3 class="m-b-none swapChildrenClass">
                    <span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Complaint &nbsp;</span><label id="lblShown" purpose="dynamicLabel" class="static-text-primary hide"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="Complaint-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hComplaintID" type="hidden" />

                            <div class="row col-sm-12">
                                <div class="form-group col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                    <input type="text" id="txtComplaintCode" autocomplete="off" disabled="disabled" class="form-control parsley-validated input-sm" data-required="false" maxlength="50" placeholder="Auto-Generated" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group col-sm-5" style="">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</label>
                                    <select id="slCustomer" class="form-control m-b input-sm" data-required="true"><option value=""><--Select--></option></select>
                                </div>
                                <div class="form-group col-sm-4" style="">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</label>
                                    <select id="slOperationForHeader" class="form-control m-b input-sm" onchange="" data-required="false"><option value=""><--Select--></option></select>
                                </div>
                            </div>
                            <div class="row col-sm-12">
                                <div class="form-group col-sm-3">
                                    <button id="btn-NewAddDetails" class="btn btn-warning btn-sm swapChildrenClass m-t-sm" onclick="ComplaintDetails_ClearAllControls(); OpenCRM_ComplaintDetailsModal();" data-toggle="modal" data-target="#"><i class="fa fa-plus"></i><b>&nbsp;Add Details</b></button>
                                </div>
                                <div class="float-right col-sm-2">
                                    <button id="btn-DeleteDetails" type="button" class="float-right btn btn-sm btn-danger m-t-sm m-r-n swapChildrenClass" onclick="ComplaintDetails_DeleteList();"><i class="fa fa-trash-o"></i><b>&nbsp;Delete&nbsp;</b></button>
                                </div>
                            </div>

                            <div class="row col-sm-12">

                                <div class="table-responsive">
                                    <table id="tblCRM_ComplaintDetails" class="table table-striped b-t b-light text-sm  table-hover">
                                        <thead>
                                            <tr>
                                                <th id="ID_CRM_ComplaintDetails">
                                                    <input id="cb-CheckAll_ComplaintDetails" type="checkbox">
                                                </th>
                                                @*<th>
                                                        Operation
                                                    </th>*@
                                                <th>
                                                    Status
                                                </th>
                                                <th>
                                                    Complaint Name
                                                </th>
                                                <th>
                                                    Complaint Date
                                                </th>
                                                <th class="hide">
                                                    Response Date
                                                </th>
                                                <th>
                                                    Sales Rep
                                                </th>
                                                <th class="hide">
                                                    Sales Rep 2
                                                </th>

                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer">
                <button type="button" id="btnSaveComplaint" class="btn btn-primary btn-sm swapChildrenClass" onclick="Complaint_Save(false);">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Save&nbsp;</b>
                </button>
                <button type="button" id="btnSaveandNewComplaint" class="btn btn-primary btn-sm swapChildrenClass" onclick="Complaint_Save(true);">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew&nbsp;</b>
                </button>
                <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Close</b></button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="CRM_ComplaintDetailsModal" tabindex="-1" style="z-index: 3000;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="CRM_ContactPersonsModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-70">
        @*<div class="modal-content modalwidth-100">*@
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="m-b-none">
                    Complaint Details&nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary TheClientName"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="CRM_FollowUp-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error  row">
                            <input id="hIDDetails" type="hidden" />
                            <!-- start alert message region -->
                            <div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>
                            <!-- end alert message region -->
                            <!-- Region Add / Edit Region Form -->
                            <div class="form-group col-sm-4 hide">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</label>
                                <select id="slOperation" class="form-control m-b input-sm" data-required="false"><option value=""><--Select--></option></select>
                            </div>
                            <div class="form-group col-sm-3 StatusComplaintDetails ">
                                <label>Status</label>
                                <select id="slStatus" class="input-sm form-control" data-required="false" onchange=""></select>
                            </div>
                            <div class="form-group col-sm-3">
                                <label>Complaint</label>
                                <select id="slComplaint" class="input-sm form-control" data-required="true" onchange=""></select>
                            </div>
                            <div class="form-group col-sm-1 m-t-sm">
                                <button id="btn-NotifyComplaint" class="btn btn-warning btn-sm swapChildrenClass m-t-sm" type="button" onclick="ShowReceptionists();"><b>&nbsp;Notify</b></button>
                            </div>
                            <div class="row m-l-n-sm">
                                <div class=" form-group col-sm-6">
                                    <label>Complaint Description</label>
                                    <textarea id="txtComplaintDetails" maxlength="1000" class="form-group col-sm-12 textDivGoodsDescription" style="text-transform:none; height:100px;"></textarea>
                                </div>
                                <div class="form-group col-sm-3 hide">
                                    <label>Complaint</label>
                                    <textarea id="txtComplaintName" autocomplete="off" type="text" class="form-control parsley-validated input-sm" data-required="true" maxlength="500" placeholder="" style="text-transform:uppercase; height:100px;"></textarea>
                                </div>
                                <div class="col-sm-3">
                                    <label>Complaint Date</label>
                                    <input id="txtComplaintDate" disabled type="text" onfocus="DisableEnterKey(id);" data-required="false" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control input-sm" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                </div>

                                <div class="form-group col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.SalesRep</label>
                                    <select id="slComplaintDetailsSalesRep" disabled class="input-sm form-control" data-required="false" onchange=""></select>
                                </div>

                            </div>

                            @*<div class="form-group col-sm-6">
                                    <label>Response Description</label>
                                    <textarea id="txtResponseDetails" maxlength="1000" class="form-group col-sm-12 textDivGoodsDescription" style="text-transform:none; height:100px;"></textarea>
                                </div>

                                <div class="col-sm-3">
                                    <label>Response Date</label>
                                    <input id="txtResponseDate" type="text" onfocus="DisableEnterKey(id);" data-required="false" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control input-sm" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                </div>
                                <div class="col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.SalesRep 2</label>
                                    <select id="slComplaintDetailsSalesRep2" class="input-sm form-control" data-required="false" onchange=""></select>
                                </div>*@

                            <div class="row col-sm-12">
                                <div class="form-group col-sm-3">
                                    <button id="btn-NewAddDetailsResponses" class="btn btn-warning btn-sm swapChildrenClass m-t-sm" onclick="ComplaintDetailsResponses_ClearAllControls(); OpenCRM_ComplaintDetailsResponsesModal();" data-toggle="modal" data-target="#"><i class="fa fa-plus"></i><b>&nbsp;Add Details</b></button>
                                </div>
                                <div class="float-right col-sm-2">
                                    <button id="btn-DeleteDetailsResponses" type="button" class="float-right btn btn-sm btn-danger m-t-sm m-r-n swapChildrenClass" onclick="ComplaintDetailsResponses_DeleteList();"><i class="fa fa-trash-o"></i><b>&nbsp;Delete&nbsp;</b></button>
                                </div>
                            </div>

                            <div class="row col-sm-12">

                                <div class="table-responsive">
                                    <table id="tblCRM_ComplaintDetailsResponses" class="table table-striped b-t b-light text-sm  table-hover">
                                        <thead>
                                            <tr>
                                                <th id="ID_CRM_ComplaintDetails">
                                                    <input id="cb-CheckAll_ComplaintDetailsResponses" type="checkbox">
                                                </th>
                                                <th>
                                                    Sales Rep
                                                </th>
                                                <th>
                                                    Status
                                                </th>
                                                <th>
                                                    Response Date
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>


                        </div>

                        <div class="form-group pull-in  has-error  row">

                            <div class="form-group   col-sm-3">
                                <input id="hstatue" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="hidden" class="form-control   input-sm" placeholder="" data-required="false" style="text-transform:uppercase" />
                                <input id="hPreCode" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="hidden" class="form-control   input-sm" placeholder="" data-required="false" style="text-transform:uppercase" />

                            </div>

                        </div>
                        <div class="form-group pull-in  has-error   row">
                            @*<input id="hID" type="hidden" />*@
                            <!-- end alert message region -->
                            <!-- Region Add / Edit Region Form -->
                            <div id="divreason" class="form-group has-error  hide  col-sm-9">
                                <div class="col-sm-12">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Reason</label>
                                    <textarea id="txtReason" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" cols="3" class="form-control parsley-validated  input-sm" data-required="true" placeholder="" style="text-transform:uppercase;resize:none;"></textarea>
                                </div>
                            </div>

                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer">
                <button type="button" id="btnSave2" class="btn btn-primary btn-sm" onclick="ComplaintDetails_Insert(false);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
                </button>
                <button type="button" id="btnSaveandNew2" class="btn btn-primary btn-sm" onclick="CRM_FollowUp_Insert(true);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                </button>

                <button class="btnClose btn btn-link btn-sm" id="btnClose2" @*onclick="CRM_ContactPersons_UnlockRecord();"*@ data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->

            </div>
        </div>
    </div>
</div>

