<div class="modal fade" id="InvoiceModal" tabindex="-1" style="z-index: 1040;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="InvoiceModalLabel"
     aria-hidden="true">
    <div class="modal-dialog modalwidth-80">
        <div class="modal-content">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;</button>*@
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.Invoice &nbsp;<label id="lblInvoiceShown" purpose="dynamicLabel" class="static-text-primary"></label>
            </h3>
        </div>
        <div class="modal-body">
            <!-- Region Add / Edit Invoice Form -->
            <div class="span7 form vertical-mode" id="Invoice-form">
                <fieldset>
                    <div class="form-group pull-in  has-error clearfix">
                        @*<input id="hInvoiceID" type="hidden" />*@
                        <!-- start alert message region -->
                        <div class="alert alert-danger div-error">
                            @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                        </div>
                        <!-- end alert message region -->
                        <div class="form-group col-sm-6">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</label>
                            <select id="slInvoiceOperations" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" onchange="Invoices_InvoiceOperationChanged('slInvoiceOperations');" class="form-control input-sm" data-required="true"></select>
                        </div>
                        <div class="form-group col-sm-6">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.BillTo</label>
                            <select id="slInvoicePartner" name="slInvoicePartner" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" onchange="Invoices_PartnerChanged('slInvoiceAddressTypes', id, 'txtInvoiceIssueDate', 'txtInvoiceDueDate', 'slInvoicePaymentTerms');" class="form-control input-sm" data-required="true"><option value=""><--Select--></option></select>
                        </div>
                        <div class="form-group col-sm-6">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.AddressType</label>
                            <select id="slInvoiceAddressTypes" name="slInvoiceAddressTypes" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm" data-required="true"><option value=""><--Select--></option></select>
                        </div>
                        <div class="form-group col-sm-6">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.InvoiceType</label>
                            <select id="slInvoiceTypes" name="slInvoicTypes" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm" data-required="true"><option value=""><--Select--></option></select>
                        </div>
                        <div class="form-group col-sm-6">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.InvoiceNumber</label>
                            <input type="text" id="txtInvoiceNumber" disabled="disabled" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="false" data-minlength="1" maxlength="15" placeholder="Generated Automatically" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-6 hide">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.CustomerReference</label>
                            <input type="text" id="txtInvoiceCustomerReference" class="form-control parsley-validated input-sm" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" data-required="false" data-minlength="0" maxlength="10" placeholder="Ref No." style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-6">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                            <select id="slInvoiceCurrency" disabled="disabled" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" onchange="(false);" class="form-control input-sm" data-required="true"></select>
                        </div>
                        <div class="form-group col-sm-6 hide">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.ExchangeRate</label>
                            <input type="text" id="txtInvoiceMasterDataExchangeRate" disabled="disabled" class="form-control parsley-validated input-sm" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" onkeypress="CheckValueIsDecimal(id);" data-required="true" maxlength="9" placeholder="Exchange Rate" style="text-transform:uppercase" />
                        </div>


                        <div class="form-group col-sm-2 hide classShowForTaxOnHeader">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.VAT</label>
                            <select id="slInvoiceTax" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" onchange="Invoices_SetInvoiceAmount('tblReceivables','Invoice','ReceivableSaleAmount');" class="form-control input-sm" data-required="false"><option value=""><--Select--></option></select>
                        </div>
                        <div class="form-group col-sm-2 hide classShowForTaxOnHeader">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Perc</label>
                            <input type="text" id="txtInvoiceTaxPercentage" disabled="disabled" class="form-control parsley-validated input-sm" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" onkeypress="CheckValueIsDecimal(id);" data-required="true" maxlength="4" placeholder="Tax (%)" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-2 hide classShowForTaxOnHeader">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.VATAmt</label>
                            <input type="text" id="txtInvoiceTaxAmount" disabled="disabled" class="form-control parsley-validated input-sm" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" onkeypress="CheckValueIsDecimal(id);" data-required="true" maxlength="7" placeholder="Tax Amount" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-2 hide classShowForTaxOnHeader">
                            @*<label>@Forwarding.MvcApp.App_Resources.App_Resources.TaxDiscount</label>*@
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.DeductedTax</label>
                            <select id="slInvoiceDiscount" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" onchange="Invoices_SetInvoiceAmount('tblReceivables','Invoice','ReceivableSaleAmount');" class="form-control input-sm" data-required="false"></select>
                        </div>
                        <div class="form-group col-sm-2 hide classShowForTaxOnHeader">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Perc</label>
                            <input type="text" id="txtInvoiceDiscountPercentage" disabled="disabled" class="form-control parsley-validated input-sm" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" onkeypress="CheckValueIsDecimal(id);" data-required="true" maxlength="4" placeholder="Discount (%)" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-2 hide classShowForTaxOnHeader">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.DiscAmt</label>
                            <input type="text" id="txtInvoiceDiscountAmount" disabled="disabled" class="form-control parsley-validated input-sm" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" onkeypress="CheckValueIsDecimal(id);" data-required="true" maxlength="7" placeholder="Discount Amount" style="text-transform:uppercase" />
                        </div>

                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.SpecialDisc.</label>
                            <input type="text" id="txtInvoiceFixedDiscount" class="form-control parsley-validated input-sm" onchange="Invoices_SetInvoiceAmount('tblReceivables','Invoice','ReceivableSaleAmount');" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" onkeypress="CheckValueIsDecimal(id);" data-required="false" maxlength="7" placeholder="Special Discount" style="text-transform:uppercase" />
                        </div>

                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.PaymentTerm</label>
                            <select id="slInvoicePaymentTerms" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" onchange="Invoices_SetDueDate('txtInvoiceIssueDate' ,'txtInvoiceDueDate' ,id);" class="form-control input-sm" data-required="true"></select>
                        </div>
                        <div class="form-group col-sm-2 classShowForGBL hide">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.TransactionType</label>
                            <select id="slTransactionTypes" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" onchange="" class="form-control input-sm" data-required="false"></select>
                        </div>
                        <div class="form-group col-sm-2 hide classShowForTaxOnHeader">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.AmtwithoutTax</label>
                            <input type="text" id="txtInvoiceAmountWithoutVAT" disabled="disabled" class="form-control parsley-validated input-sm" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" onkeypress="CheckValueIsDecimal(id);" data-required="true" maxlength="7" placeholder="Amount w/o VAT" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Total</label>
                            <input type="text" id="txtInvoiceAmount" disabled="disabled" class="form-control parsley-validated input-sm" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" onkeypress="CheckValueIsDecimal(id);" data-required="true" maxlength="3" placeholder="0.00" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.InvoiceDate</label>
                            <input id="txtInvoiceIssueDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text; background-color:white;" class="datepicker-input form-control input-sm" data-required="true" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.DueDate</label>
                            <input id="txtInvoiceDueDate" type="text" disabled="disabled" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control input-sm" data-required="true" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                        </div>
                        <div class="form-group col-sm-2 hide classShowForTank">
                            <label>Tank</label>
                            <select id="slInvoiceOperationTank" class="form-control input-sm" data-required="false"><option value=""><--Select--></option></select>
                        </div>

                        @*<div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.ReportType</label>
                                <select id="slInvoiceReportTypes" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm" data-required="true"></select>
                            </div>*@
                    </div>
                </fieldset>
            </div>
            <!-- Endegion Add / Edit Country Form -->
        </div>
        <div class="modal-footer">
            <!--sherif-->
            <button type="button" id="btnSaveInvoice" class="btn btn-primary btn-sm" onclick="Invoices_Insert(false);">
                <span class="glyphicon glyphicon-save"></span>&nbsp;Create
            </button>
            @*<button type="button" id="btnSaveandNewInvoice" class="btn btn-primary btn-sm" onclick="Invoices_Insert(true);">
                <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew</button>*@
            <button class="btnClose btn btn-link btn-sm" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button>
        </div>
    </div>
</div>
</div>

<!--EditInvoiceModal-->
