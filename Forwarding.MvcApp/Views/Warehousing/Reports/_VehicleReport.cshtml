<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="liGroupName" href="javascript:void(0);" onclick="LoadViews('VehicleReportTab')">@Forwarding.MvcApp.App_Resources.App_Resources.VehicleModule
    </a></li>
    @*<li><a id="liTabName" href="javascript:void(0);" onclick="LoadViews('VehicleReportTab')">@Forwarding.MvcApp.App_Resources.App_Resources.VehicleReport
    </a></li>*@
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.VehicleReport</li>
</ul>
<!-- end breadcrumb  -->
 
<div id="div-VehicleReport">
    <!-- Region Options & Template  -->
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <div id="hExportedTable" class="hide"></div>
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.FilterBy</h3>
                @*<h3 class="m-b-none">Best Print Using Internet Explorer</h3>*@
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-l">
                <div id="div-main-options">
                    <!--Region Filter-->

                    <div class="form-group col-sm-2">
                        <label>Chassis Number</label>
                        <input type="text" id="txtChassisNumber" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="50" placeholder="" style="text-transform:uppercase">
                    </div>
                    <div class="form-group col-sm-2">
                        <label>Dispatch Number</label>
                        <input type="text" id="txtDispatchNumber" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="50" placeholder="" style="text-transform:uppercase">
                    </div>
                    <div class="form-group col-sm-2">
                        <label>Operation Number</label>
                        <input type="text" id="txtOperationNumber" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="50" placeholder="" style="text-transform:uppercase">
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</label>
                        <select id="slCustomer" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>
                    <div class="form-group col-sm-2">
                        <label>Doc Number</label>
                        <input type="text" id="txtDocNumber" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="50" placeholder="" style="text-transform:uppercase">
                    </div>
                    <div class="form-group col-sm-2 classVehicleTracking">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.ActionType</label>
                        <select id="slVehicleActionType" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>

                    <div class="form-group col-sm-2 classVehicleTracking" style="clear:both;">
                        <label>From Action Date</label>
                        <input id="txtFromActionDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="form-group col-sm-2 classVehicleTracking">
                        <label>To Action Date</label>
                        <input id="txtToActionDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>

                    <div class="form-group col-sm-2 classVehicleCharge hide" style="clear:both;">
                        <label>From Receive Date</label>
                        <input id="txtFromReceiveDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="form-group col-sm-2 classVehicleCharge hide">
                        <label>To Receive Date</label>
                        <input id="txtToReceiveDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>

                    <div class="form-group col-sm-2 classVehicleCharge hide">
                        <label>From Pickup Date</label>
                        <input id="txtFromPickupDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="form-group col-sm-2 classVehicleCharge hide">
                        <label>To Pickup Date</label>
                        <input id="txtToPickupDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>

                    <div class="form-group col-sm-2 classVehicleAging hide">
                        <label>In/Out</label>
                        <select id="slVehicleInOrOut" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm">
                            <option value=""><--All--></option>
                            <option value="IN">IN</option>
                            <option value="OUT">OUT</option>
                        </select>
                    </div>
                    <div id="VehicleReportCharge" class="hide"></div>
                    <div class="col-sm-2 m-t-27 hide">
                        <label> <input type="checkbox" id="cbOperationsWOInvoices" onchange="OperationsStatistics_cbIsWorkFlowChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp; Oper. w/o Inv. </label>
                        <br><label> <input type="checkbox" id="cbIsWorkFlow" onchange="OperationsStatistics_cbIsWorkFlowChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp; Work Flow </label>
                    </div>

                    <div class="col-sm-12" style="clear:both;">
                        <label class="m-t-sm"> <input type="radio" name="nameReportOption" checked="checked" onchange="ReportOptionChanged();" id="cbIsVehicleTracking" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"> Vehicles Tracking</label>
                        <label class="m-l-lg m-t-sm"> <input type="radio" name="nameReportOption" id="cbIsVehicleCharge" onchange="ReportOptionChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"> Vehicles Charges Details</label>
                        <label class="m-l-lg m-t-sm"> <input type="radio" name="nameReportOption" id="cbIsVehicleAging" onchange="ReportOptionChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"> Vehicles Aging</label>
                        <label class="m-l-lg m-t-sm hide classShowForGBL"> <input type="radio" name="nameReportOption" id="cbIsOracleData" onchange="ReportOptionChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"> Oracle Data retrieved</label>
                    </div>

                    <div class="col-sm-5 m-t-xs" data-toggle="buttons">
                        <button id="btn-print" type="button" class="btn btn-lightblue btn-sm" onclick="VehicleReport_Print('Print');"><i class="fa fa-print"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print</button>
                        <button id="btn-ExportToExcel" type="button" onclick="VehicleReport_Print('Excel');" class="btn btn-sm btn-primary"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>
                        <button id="btn-SelectColumns" type="button" class="btn btn-warning btn-sm hide" onclick="VehicleReport_SelectColumns();"><i class="fa fa-print"></i><b>&nbsp;Add / Remove Columns</b></button>
                    </div>
                    <!--EOF Region Filter-->
                </div>
            </div>
            <!--EOF Region the searh, delete, addnew line-->


        </section>
    </div>
</div>

<!--ModalSelectColumns-->
<div class="modal fade" id="ModalSelectColumns" tabindex="-1" style="z-index: 3000;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="ModalSelectColumnsLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-90">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="m-b-none">
                    &nbsp;<label id="lblShownItems" purpose="dynamicLabel" class="static-text-primary">Select columns to show:</label>
                </h3>
            </div>
            <div class="modal-body">
                <!-- Region SelectColumns Form -->
                <div class="span7 form vertical-mode" id="SelectColumns-form">
                    <fieldset>
                        @*<div class="row">
                                <div id="div-search-controls" class="input-group m-l col-sm-5">
                                    <input id="txtSearchItems" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase"  type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search" />
                                    <span class="input-group-btn">
                                        <button id="btn-SearchItems" class="btn btn-sm btn-primary" type="button"><i class="fa fa-search"></i></button>
                                    </span>
                                </div>
                            </div><br />*@

                        <div id="divSelectColumns">
                            <div class="col-sm-2">
                                <label><input type="checkbox" id="cbOperation" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;Operation</label>
                            </div>
                            <div class="col-sm-2">
                                <label><input type="checkbox" id="cbReceiveDate" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;ReceiveDate</label>
                            </div>
                            <div class="col-sm-2">
                                <label><input type="checkbox" id="cbPickupDate" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;PickupDate</label>
                            </div>
                            <div class="col-sm-2">
                                <label><input type="checkbox" id="cbDays" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;Days</label>
                            </div>
                            <div class="col-sm-2">
                                <label><input type="checkbox" id="cbDispatchNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;DispatchNumber</label>
                            </div>
                            <div class="col-sm-2">
                                <label><input type="checkbox" id="cbCustomer" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;Customer</label>
                            </div>

                        </div>
                    </fieldset>
                </div>
                <!-- Endegion SelectColumns Form -->
            </div>
            <div class="modal-footer">
                <!--sherif-->
                @*<button type="button" id="btnSelectColumnsApply" class="btn btn-primary btn-sm">
                    <span class="glyphicon glyphicon-save"></span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Apply</button>*@

                <button id="btn-print-FromSelect" type="button" class="btn btn-lightblue btn-sm" onclick="VehicleReport_Print('Print');"><i class="fa fa-print"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print</b></button>
                <button id="btn-ExportToExcel-FromSelect" type="button" onclick="VehicleReport_Print('Excel');" class="btn btn-sm btn-primary"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>

                <button class="btn btn-link btn-sm" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button>
            </div>
        </div>
    </div>
</div>
<!--EOF ModalSelectColumns-->
