<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="liGroupName" href="javascript:void(0);" onclick="LoadViews('WarehousingMasterData')">@Forwarding.MvcApp.App_Resources.App_Resources.MasterData
    </a></li>
    <li><a id="liTabName" href="javascript:void(0);" onclick="LoadViews('WarehousingMasterData')">@Forwarding.MvcApp.App_Resources.App_Resources.WarehousingMasterData
    </a></li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.Warehouses</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-Warehouse">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.Warehouses
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm swapChildrenClass" data-toggle="modal" data-target="#WarehouseModal"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-7">
                    <div id="div-search-controls" class="input-group col-sm-9">
                        <input id="txt-Search" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase"  type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="Warehouse_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-1">
                        <button id="btn-Delete-hide" type="button" class="float-right btn btn-sm btn-danger m-r-n swapChildrenClass hide"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>
                    </div>
                </div>
            </div>
            <!--Region the searh, delete, addnew-->

            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblWarehouse" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID" style="width:5%;">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Code
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Name
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Phones
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.MainWarehouse
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.WarehouseType
                            </th>
                            <th class="rounded-right hide"></th>
                        </tr>
                    </thead>
            <tbody></tbody>
            </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm"> @Forwarding.MvcApp.App_Resources.App_Resources.Showing 
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>


<!-- Add / Edit Region Modal -->
<div class="modal fade" id="WarehouseModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="WarehouseModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-70">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;</button>
                <h3 class="m-b-none swapChildrenClass"><span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Warehouse &nbsp;</span><label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="Warehouse-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hID" type="hidden" />
                            <!-- start alert message region -->
                            @*<div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>*@
                            <!-- end alert message region -->

                            <!-- Region Add / Edit Region Form -->
                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                <input type="text" id="txtCode" autocomplete="off" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="" style="text-transform:uppercase"/>
                            </div>
                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                                <input type="text" id="txtName" autocomplete="off" class="form-control parsley-validated input-sm" data-required="true" maxlength="200" placeholder="" style="text-transform:uppercase"/>
                            </div>
                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Phone</label>
                                <input type="text" id="txtPhone" autocomplete="off" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="" style="text-transform:uppercase"/>
                            </div>
                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Fax</label>
                                <input type="text" id="txtFax" autocomplete="off" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="" style="text-transform:uppercase"/>
                            </div>
                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Address</label>
                                <input type="text" id="txtAddress" autocomplete="off" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="" style="text-transform:uppercase"/>
                            </div>
                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                <input type="text" id="txtNotes" autocomplete="off" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="" style="text-transform:uppercase"/>
                            </div>
                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.MainWarehouse</label>
                                <select id="slMainWarehouse"  class="form-control m-b input-sm" data-required="false"></select>
                            </div>
                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.WarehouseType</label>
                                <select id="slWarehouseType" class="form-control m-b input-sm" data-required="false">
                                    <option value="">Select Warehouse Type</option>
                                    <option value=1>3PL</option> 
                                    <option value=2>Container Freight Station</option>
                                    <option value=3>Container Yard</option>

                                </select>
                            </div>
                           
                            <!-- Region Add / Edit Region Form -->
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer">
                <button type="button" id="btnSave" class="btn btn-primary btn-sm swapChildrenClass" onclick="Warehouse_Insert(false);">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Save&nbsp;</b></button>
                <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm swapChildrenClass hide" onclick="Warehouse_Insert(true);">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew&nbsp;</b></button>
                <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true" ><b>@Forwarding.MvcApp.App_Resources.App_Resources.Close</b></button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div> 
        </div>
    </div>
</div>
<!-- Region Options & Template  -->
