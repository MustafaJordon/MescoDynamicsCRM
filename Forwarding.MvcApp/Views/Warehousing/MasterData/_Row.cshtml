<div id="div-Row">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.Rows
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm swapChildrenClass hide" data-toggle="modal" data-target="#RowModal"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-7">
                    <div id="div-search-controls" class="input-group col-sm-9">
                        <input id="txt-Search" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase"  type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="Row_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-1">
                        <button id="btn-Delete" type="button" class="float-right btn btn-sm btn-danger m-r-n swapChildrenClass hide"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>
                    </div>
                </div>
            </div>
            <!--Region the searh, delete, addnew-->

            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblRow" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID" style="width:5%;">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Name
                            </th>
                            <th class="rounded-right" style="width:10%;"></th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm"> @Forwarding.MvcApp.App_Resources.App_Resources.Showing 
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>


<!-- Add / Edit Region Modal -->
<div class="modal fade" id="RowModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="RowModalLabel" aria-hidden="true">
    <div class="modal-dialog  modalwidth-100 m-t-n-lg">
        <div class="modal-content">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;</button>*@
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.Row &nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="Row-form">
                    <fieldset>
                        <div class="form-group pull-in has-error clearfix">
                            <input id="hID" type="hidden" />
                            <!-- start alert message region -->
                            @*<div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>*@
                            <!-- end alert message region -->

                            <!-- Region Add / Edit Region Form -->
                            
                            <div class="form-group col-sm-2">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Warehouse</label>
                                <select id="slWarehouse" onchange="Row_WarehouseChanged();" class="form-control m-b input-sm classDisableForLocationExistence" disabled data-required="true"></select>
                            </div>
                            <div class="form-group col-sm-2">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Area</label>
                                <select id="slArea" class="form-control m-b input-sm" data-required="true"></select>
                            </div>                            
                            <div class="form-group col-sm-2">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                                <input type="text" id="txtName" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="true" maxlength="200" placeholder="" style="text-transform:uppercase;"/>
                            </div>
                            <div class="form-group col-sm-2">
                                <label>No Of Levels</label>
                                <input type="text" id="txtNumberOfLevelsPerRow" autocomplete="off" onkeypress="CheckValueIsInteger(id);" onfocus="CheckValueIsInteger(id);" class="form-control parsley-validated input-sm"  data-required="true" maxlength="3" placeholder="" />
                            </div>
                            <div class="form-group col-sm-2">
                                <label>No Of Trays</label>
                                <input type="text" id="txtNumberOfTraysPerLevel" autocomplete="off" onkeypress="CheckValueIsInteger(id);" onfocus="CheckValueIsInteger(id);" class="form-control parsley-validated input-sm" data-required="true" maxlength="3" placeholder="" />
                            </div>

                            <div class="form-group col-sm-1" style="clear:both;">
                                <label>Loc.Length(M)</label>
                                <input type="text" id="txtLength" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                            </div>
                            <div class="form-group col-sm-1">
                                <label>Loc.Width(M)</label>
                                <input type="text" id="txtWidth" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                            </div>
                            <div class="form-group col-sm-1 hide">
                                <label>Length.Unit</label>
                                <select id="slHeaderLengthUnit" class="form-control m-b input-sm" data-required="false"></select>
                            </div>
                            <div class="form-group col-sm-1">
                                <label>MaxWt/Loc(KG)</label>
                                <input type="text" id="txtMaxWeight" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                            </div>
                            <div class="form-group col-sm-1 hide">
                                <label>Wght.Unit</label>
                                <select id="slWeightUnit" class="form-control m-b input-sm" data-required="false"></select>
                            </div>
                            <div class="form-group col-sm-1">
                                <label>MaxVol/Loc(CBM)</label>
                                <input type="text" id="txtMaxVolume" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                            </div>
                            <div class="form-group col-sm-1 hide">
                                <label>Cubic Unit</label>
                                <select id="slVolumeUnit" class="form-control m-b input-sm" data-required="false"></select>
                            </div>

                            <div class="form-group col-sm-2 hide">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Status</label>
                                <select id="slLocationStatus" class="form-control m-b input-sm" data-required="false"></select>
                            </div>
                            <div class="form-group col-sm-2">
                                <label>Pickup Method</label>
                                <select id="slPickupMethod" class="form-control m-b input-sm" data-required="false"></select>
                            </div>
                            <div id="divApplyToLocations" class="col-sm-4 m-t-md">
                                <button id="btn-ApplyToLocations" class="btn btn-lightblue btn-sm m-t-xxs" data-toggle="modal" data-target="" onclick="RowLocation_ApplyToLocations();"><i class="fa fa-hand-o-down"></i>&nbsp;Apply To Locations</button>
                                <button id="btn-GenerateDefaultLocations" class="btn btn-info btn-sm m-t-xxs" data-toggle="modal" data-target="" onclick="RowLocation_GenerateDefaultLocations();"><i class="fa fa-hand-o-down"></i>&nbsp;Default Locations</button>
                                <button id="btn-ExportToExcel" class="btn btn-primary btn-sm m-t-xxs" data-toggle="modal" data-target="" onclick="RowLocation_Export();"><i class="fa fa-table"></i>&nbsp;Export to Excel</button>
                                <button id="btn-ImportReceiveDetailsSerial" class="btn btn-warning btn-sm m-t-xxs" data-toggle="modal" data-target="" onclick="RowLocation_Import();"><i class="fa fa-table"></i>&nbsp;Import from Excel</button>

                                <input id="btnAddFromExcel" type="file" title="Import Items from Excel 97-2003" class="btn-info btn-sm m-t-xxs hide" onchange="onFileSelected(event);" />
                            </div>

                            <!--Details-->
                                    <div id="divRowLocationBtns" class="col-sm-12 m-t" style="clear:both;">
                                        <button id="btn-AddRowLocation" class="btn btn-warning btn-sm" data-toggle="modal" data-target="" onclick="RowLocation_ClearAllControls();"><i class="fa fa-plus"></i>&nbsp;Add Location</button>
                                        <button id="btn-DeleteRowLocation" title="Delete Location" type="button" class="float-right btn btn-sm btn-danger" onclick="RowLocation_DeleteList();"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                                    </div>

                                    <div id="divRowLocation" class="col-sm-12 m-t-xs">
                                        <section class="panel panel-default">
                                            <header class="panel-heading font-bold text-ul">Row Locations</header>
                                            <!--Region: table header-->
                                            <div class="table-responsive" style="width:100%; height:300px; overflow:scroll;">
                                                <table id="tblRowLocation" class="table table-striped b-t b-light text-sm  table-hover ">
                                                    <thead>
                                                        <tr>
                                                            <th id="HeaderDeleteRowLocationID" style="width:5%;">
                                                                <input id="cb-CheckAll-RowLocation" type="checkbox" />
                                                            </th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Code</th>
                                                            <th>Length(M)</th>
                                                            <th>Width(M)</th>
                                                            <th>MaxWgt(KGM)</th>
                                                            <th>Max.CBM</th>
                                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Status</th>
                                                            <th>Pickup Method</th>
                                                            <th>Level</th>
                                                            <th>Tray</th>
                                                            <th class="rounded-right hide"></th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                    </tbody>
                                                </table>
                                            </div>
                                            <!--Region: EOF table header-->
                                            <!--Region: table footer-->
                                            <footer class="panel-footer">
                                        <div class="col-sm-12 m-t-n-sm swapChildrenClass">
                                            <div class="col-sm-6 swapChildrenClass"><b>&emsp;Row Max Weight(KGM)</b><label id="lblRowMaxWeight" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                            <div class="col-sm-6 text-right swapChildrenClass"><b>&emsp;Row Max Volume(CBM)</b><label id="lblRowMaxVolume" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                        </div>
                                    </footer>
                                            <!--Region: EOF table footer-->
                                        </section>
                                    </div>                            
                                    
                            <!--EOF Details-->
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer m-t-n-lg">
                <button type="button" id="btnSave" class="btn btn-primary btn-sm" onclick="">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save</button>
                <button type="button" id="btnSaveAndAddNew" class="btn btn-primary btn-sm" onclick="">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew</button>
                <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true" >@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div> 
        </div>
    </div>
</div>
<!-- Region Options & Template  -->

