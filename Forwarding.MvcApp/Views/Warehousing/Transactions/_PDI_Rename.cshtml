<!--PickupDetailsModal-->
<div class="modal fade" id="PickupDetailsModal" tabindex="-1" style="z-index: 1050;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="PickupDetailsModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-90">
        <div class="modal-content m-t-n-lg">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;</button>*@
                <h3 class="m-b-none">
                    PDI Details&nbsp;<label id="lblPickupDetailsShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <div class="modal-body">
                <!-- Region Add / Edit PickupDetails Form -->
                <div class="span7 form vertical-mode" id="PickupDetails-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hPickupDetailsID" type="hidden" />
                            <!-- start alert message region -->
                            @*<div class="alert alert-danger div-error">
                                    @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                </div>*@
                            <!-- end alert message region -->
                            <div class="form-group col-sm-5">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Product</label>
                                <div class="input-group">
                                    <select id="slPickupDetailsPurchaseItem" onchange="PickupDetails_PurchaseItemChanged();" class="form-control m-b input-sm" data-required="true"></select>
                                    <span title="Reload" class="input-group-addon" onclick="PickupDetails_FillModalTables();" style="background-color:green;padding:5px!important;color:white;cursor:pointer;"><span class="fa fa-refresh"></span></span>
                                </div>
                            </div>
                            @*<div class="form-group col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Receive</label>
                                    <select id="slPickupDetailsReceive" class="form-control m-b input-sm" data-required="true"></select>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Location</label>
                                    <select id="slPickupDetailsLocation" class="form-control m-b input-sm" data-required="true"></select>
                                </div>*@
                            <div class="form-group col-sm-4">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                <input type="text" id="txtPickupDetailsNotes" autocomplete="off" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="Notes" style="text-transform:uppercase" />
                            </div>
                            <div class="form-group col-sm-2 hide">
                                <label>Demanded.Qty</label>
                                <input type="text" id="txtPickupDetailsDemandedQuantity" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                            </div>
                            <div class="form-group col-sm-2">
                                <label>Picked.Qty</label>
                                <input type="text" disabled="disabled" id="txtPickupDetailsPickedQuantity" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                            </div>
                            @*<div class="form-group col-sm-3">
                                    <label>Qty Unit</label>
                                    <select id="slPickupDetailsQuantityUnit" class="form-control m-b input-sm" data-required="true"></select>
                                </div>*@
                            @*<div class="col-sm-6 m-t-27 ">
                                    <label><input type="checkbox" id="cbIsReligional" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Religional</label><br />
                                </div>*@







                            @*<div id="divPickupDetailsLocationBtns" class="col-sm-12 m-t" style="clear:both;">
                                    <button id="btn-AddToPickupDetailsLocation" class="btn btn-lightblue btn-sm classDisableForFinalized" data-toggle="modal" data-target="" onclick="PickupDetailsLocation_AddToPickup();"><i class="fa fa-shopping-cart"></i>&nbsp;Add To Pickup</button>
                                    <button id="btn-DeletePickupDetailsLocation" title="Delete Details" type="button" class="float-right btn btn-sm btn-danger classDisableForFinalized" onclick="PickupDetailsLocation_DeleteList();"><i class="fa fa-trash-o"></i>&nbsp;Delete</button>
                                </div>
                            *@
                            <div id="divReceiveDetails" class="col-sm-7 m-t-xs" style="clear:both;">
                                <section class="panel panel-default">
                                    <header class="panel-heading font-bold text-ul">Available Stock</header>
                                    <!--Region: table header-->
                                    <div class="table-responsive" style="width:100%; height:390px; overflow:scroll;">
                                        <table id="tblReceiveDetails" class="table table-striped b-t b-light text-sm  table-hover ">
                                            <thead>
                                                <tr>
                                                    <th id="HeaderDeleteReceiveDetailsID" style="width:5%;">
                                                        <input id="cb-CheckAll-ReceiveDetails" type="checkbox" />
                                                    </th>
                                                    <th>Location</th>
                                                    <th>Rec.Code</th>
                                                    <th>BarCode</th>
                                                    <th class="hide">PalletID</th>
                                                    <th>U.Q.</th>
                                                    <th>Avail.Qty</th>
                                                    <th>Qty To Pick</th>
                                                    <th class="rounded-right hide"></th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                    <!--Region: EOF table header-->
                                    <!--Region: table footer-->
                                    <footer class="panel-footer hide">
                                        <div class="col-sm-12 m-t-n-sm swapChildrenClass">
                                            <!--
                                                <div class="col-sm-6 swapChildrenClass"><b>&emsp;Row Max Weight(KGM)</b><label id="lblRowMaxWeight" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                                <div class="col-sm-6 text-right swapChildrenClass"><b>&emsp;Row Max Volume(CBM)</b><label id="lblRowMaxVolume" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                            -->
                                        </div>
                                    </footer>
                                    <!--Region: EOF table footer-->
                                </section>
                            </div>

                            <div id="divPickupDetailsLocation" class="col-sm-5 m-t-xs">
                                <section class="panel panel-default">
                                    <header class="panel-heading font-bold text-ul">Picked Quantities</header>
                                    <!--Region: table header-->
                                    <div class="table-responsive" style="width:100%; height:390px; overflow:scroll;">
                                        <table id="tblPickupDetailsLocation" class="table table-striped b-t b-light text-sm  table-hover ">
                                            <thead>
                                                <tr>
                                                    <th id="HeaderDeletePickupDetailsLocationID" style="width:5%;">
                                                        <input id="cb-CheckAll-PickupDetailsLocation" type="checkbox" />
                                                    </th>
                                                    <th>Location</th>
                                                    <th>Rec.Code</th>
                                                    <th>Picked.Qty</th>
                                                    <th class="rounded-right"></th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                    <!--Region: EOF table header-->
                                    <!--Region: table footer-->
                                    <footer class="panel-footer hide">
                                        <div class="col-sm-12 m-t-n-sm swapChildrenClass">
                                            <!--
                                                <div class="col-sm-6 swapChildrenClass"><b>&emsp;Row Max Weight(KGM)</b><label id="lblRowMaxWeight" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                                <div class="col-sm-6 text-right swapChildrenClass"><b>&emsp;Row Max Volume(CBM)</b><label id="lblRowMaxVolume" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                            -->
                                        </div>
                                    </footer>
                                    <!--Region: EOF table footer-->
                                </section>
                            </div>


                        </div>
                    </fieldset>
                </div>
                <!-- End Region Add / Edit EditCharge Form -->
            </div>
            <div class="modal-footer m-t-n-lg">
                <button type="button" id="btnSavePickupDetails" class="btn btn-primary btn-sm classDisableForFinalized" onclick="PickupDetails_Save(false);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
                </button>
                <button type="button" id="btnSaveAndNewPickupDetails" class="btn btn-primary btn-sm hide classDisableForFinalized" onclick="PickupDetails_Save(true);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                </button>
                <button class="btnClose btn btn-link btn-sm" id="btnPickupDetailsClose" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div>
        </div>
    </div>
</div>
<!--EOF PickupDetailsModal-->
<!--ReceiveDetailsSerialModal-->
<div class="modal fade" id="ReceiveDetailsSerialModal" tabindex="-1" style="z-index: 1050;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="ReceiveDetailsSerialModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-40">
        <div class="modal-content m-t-n-lg">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h3 class="m-b-none">
                    Serials&nbsp;<label id="lblReceiveDetailsSerialShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <div class="modal-body">
                <!-- Region Add / Edit ReceiveDetailsSerial Form -->
                <div class="span7 form vertical-mode" id="ReceiveDetailsSerial-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <!-- start alert message region -->
                            @*<div class="alert alert-danger div-error">
                                    @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                </div>*@
                            <input id="hPickupDetailsLocationID" type="hidden" />
                            <input id="hReceiveDetailsID" type="hidden" />
                            <!-- end alert message region -->
                            <div class="form-group col-sm-3 m-t-n">
                                <label>Quantity</label>
                                <input type="text" id="txtReceiveDetailsSerialQuantity" disabled="disabled" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                            </div>
                            <div id="divSerialsBtns" class="form-group col-sm-9 m-t-xs hide">
                                <button id="btn-DeleteReceiveDetailsSerial" title="Delete selected serials" type="button" class="float-right btn btn-sm btn-danger classDisableForFinalized" onclick="ReceiveDetailsSerial_DeleteFromPickup();"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                                <button id="btn-AddReceiveDetailsSerial" title="Add serial" class="btn btn-warning btn-sm float-right classDisableForFinalized" data-toggle="modal" data-target="" onclick="ReceiveDetailsSerial_NewRow();"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Add</button>
                            </div>
                            <div class="form-group col-sm-12 m-t-lg">
                                <section class="panel panel-default" style="clear:both;">
                                    @*<header class="panel-heading font-bold text-ul">Serials</header>*@
                                    <!--Region: table header-->
                                    <div class="table-responsive" style="width:100%; height:450px; overflow:scroll;">
                                        <table id="tblReceiveDetailsSerial" class="table table-striped b-t b-light text-sm  table-hover ">
                                            <thead>
                                                <tr>
                                                    <th id="HeaderDeleteReceiveDetailsSerialID" style="width:5%;">
                                                        <input id="cb-CheckAll-ReceiveDetailsSerial" type="checkbox" />
                                                    </th>
                                                    <th>Serial</th>
                                                    <th class="rounded-right hide"></th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                    <!--Region: EOF table header-->
                                    <!--Region: table footer-->
                                    <footer class="panel-footer hide">
                                        <div class="col-sm-12 m-t-n-sm swapChildrenClass">
                                            <!--
                                                <div class="col-sm-6 swapChildrenClass"><b>&emsp;Row Max Weight(KGM)</b><label id="lblRowMaxWeight" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                                <div class="col-sm-6 text-right swapChildrenClass"><b>&emsp;Row Max Volume(CBM)</b><label id="lblRowMaxVolume" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                            -->
                                        </div>
                                    </footer>
                                    <!--Region: EOF table footer-->
                                </section>
                            </div>
                        </div>
                    </fieldset>
                </div>
                <!-- End Region Add / Edit EditCharge Form -->
            </div>
            <div class="modal-footer m-t-n-lg">
                <button id="btn-GenerateDefaultSerial" class="btn btn-lightblue btn-sm classDisableForFinalized hide" data-toggle="modal" data-target="" onclick="ReceiveDetailsSerial_GenerateDefaultSerial();"><i class="fa fa-hand-o-down"></i>&nbsp;Generate Default Serials</button>
                <button type="button" id="btnSaveReceiveDetailsSerial" class="btn btn-primary btn-sm classDisableForFinalized" onclick="ReceiveDetailsSerial_Save(false);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
                </button>
                <button type="button" id="btnSaveAndNewReceiveDetailsSerial" class="btn btn-primary btn-sm classDisableForFinalized hide" onclick="ReceiveDetailsSerial_Save(true);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                </button>
                <button class="btnClose btn btn-link btn-sm" id="btnReceiveDetailsSerialClose" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div>
        </div>
    </div>
</div>
<!--EOF ReceiveDetailsSerialModal-->
