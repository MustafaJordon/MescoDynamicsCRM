<div id="div-WHInvoice">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.Invoices
            </h3>
        </header>
        <!--Region the searh, delete, addnew-->
        <div class="row text-sm wrapper hidden-print ">
            <div id="div-main-options" class="col-sm-5 m-b-xs">
                <button id="btn-NewAdd" class="btn btn-warning btn-sm swapChildrenClass hide" data-toggle="modal" data-target="#WHInvoiceModal"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>
                <button id="btn-FilterOptions" class="btn btn-primary btn-sm" data-toggle="modal" onclick="" data-target="#FilterOptionsModal"><i class="fa fa-filter"></i> Filter Options</button>
                <div id="div-option-buttons" class="btn-group hide">
                    <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                    <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                    <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                </div>
            </div>
            <div id="div-search-option" class="col-sm-7">
                <div id="div-search-controls" class="input-group col-sm-9 hide">
                    <input id="txt-Search" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="">
                    <span class="input-group-btn">
                        <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="WHInvoice_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                    </span>
                </div>
                <div class="float-right col-sm-1">
                    <button id="btn-Delete" type="button" class="float-right btn btn-sm btn-danger m-r-n swapChildrenClass hide"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>
                </div>
            </div>
        </div>
        <!--Region the searh, delete, addnew-->
        <!--Region: table header-->
        <div class="table-responsive">
            <table id="tblWHInvoice" class="table table-striped b-t b-light text-sm  table-hover">
                <thead>
                    <tr>
                        <th id="ID" style="width:5%;">
                            <input id="cb-CheckAll" type="checkbox" />
                        </th>
                        <th>@Forwarding.MvcApp.App_Resources.App_Resources.Code</th>
                        <th>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</th>
                        <th>@Forwarding.MvcApp.App_Resources.App_Resources.InvoiceDate</th>
                        <th>@Forwarding.MvcApp.App_Resources.App_Resources.Amount</th>
                        <th>@Forwarding.MvcApp.App_Resources.App_Resources.Cur</th>
                        <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.Posted</th>
                        <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.PostDate</th>
                        <th>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</th>
                        <th class="rounded-right"></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
        <!--Region: table header-->
        <!--Region: footer-->
        <footer class="panel-footer">
            <div class="row">
                <div class="col-sm-4 hidden-xs">
                    <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                        <option value="10">10</option>
                        <option value="25">25</option>
                        <option value="50">50</option>
                    </select>
                    <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                        @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                    </button>
                </div>
                <div id="div-Text-Total" class="col-sm-4 text-center">
                    <small class="text-muted inline m-t-sm m-b-sm">
                        @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                        <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                        <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                    </small>
                </div>
                <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
            </div>
        </footer>
        <!--Region: footer-->
    </section>
</div>
</div>


<!-- Add / Edit Region Modal -->
<div class="modal fade" id="WHInvoiceModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="WHInvoiceModalLabel" aria-hidden="true">
    <div class="modal-dialog  modalwidth-100 m-t-n-lg">
        <div class="modal-content">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;</button>*@
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.Invoice &nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
            </h3>
        </div>
        <!-- Region Add / Edit Region Form -->
        <div class="modal-body">
            <div class="span7 form vertical-mode" id="WHInvoice-form">
                <fieldset>
                    <div class="form-group pull-in has-error clearfix">
                        <input id="hID" type="hidden" />
                        <!-- start alert message region -->
                        @*<div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>*@
                        <!-- end alert message region -->
                        <!-- Region Add / Edit Region Form -->
                        <div class="form-group col-sm-2 hide">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Warehouse</label>
                            <select id="slWarehouse" class="form-control m-b input-sm classDisableForDetails" data-required="true"></select>
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                            <input type="text" id="txtCode" autocomplete="off" disabled="disabled" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="Auto-Generated" style="text-transform:uppercase;" />
                        </div>
                        <div class="form-group col-sm-4">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</label>
                            <select id="slCustomer" disabled="disabled" onchange="WHInvoice_slCustomerChanged();" class="form-control m-b input-sm classDisableForDetails" data-required="true"></select>
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.InvoiceDate</label>
                            <input id="txtInvoiceDate" type="text" readonly="readonly" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="datepicker-input form-control input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                        </div>

                        <div class="form-group col-sm-2 hide">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Status</label>
                            <input type="text" id="txtStatus" autocomplete="off" disabled="disabled" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="InProgress" style="text-transform:uppercase;" />
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Amount</label>
                            <input type="text" disabled="disabled" id="txtAmount" autocomplete="off" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Cur</label>
                            <select id="slCurrency" class="form-control m-b input-sm" data-required="true"></select>
                        </div>

                        <div class="form-group col-sm-2 hide">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.ExchangeRateAbbrev</label>
                            <input type="text" id="txtExchangeRate" autocomplete="off" disabled="disabled" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" placeholder="0.0" />
                        </div>
                        <div class="form-group col-sm-6">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                            <input type="text" id="txtNotes" autocomplete="off" class="form-control parsley-validated input-sm" data-required="false" maxlength="200" placeholder="Notes" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-1 m-t-27 hide">
                            <label><input type="checkbox" id="cbIsPosted">&nbsp;Posted</label>
                        </div>
                        <!--
                            <div id="divFinalize" class="col-sm-3 m-t-md">
                                <button id="btn-Finalize" class="btn btn-lightblue btn-sm m-t-xxs classDisableForPosted" data-toggle="modal" data-target="" onclick="WHInvoice_Finalize();"><i class="fa fa-hand-o-right"></i>&nbsp;Finalize</button>
                                @*<button id="btn-GenerateDefaultLocations" class="btn btn-info btn-sm m-t-xxs" data-toggle="modal" data-target="" onclick="WHInvoiceDetails_GenerateDefaultLocations();"><i class="fa fa-hand-o-down"></i>&nbsp;Default Locations</button>*@
                            </div>
                        -->
                        <!--Details-->
                        <div id="divWHInvoiceDetailsBtns" class="col-sm-12 m-t" style="clear:both;">
                            <button id="btn-AddWHInvoiceDetails" class="btn btn-warning btn-sm classDisableForPosted" data-toggle="modal" data-target="" onclick="WHInvoiceDetails_FillModalControls(0);"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.AddDetails</button>
                            <button id="btn-DeleteWHInvoiceDetails" title="Delete Details" type="button" class="float-right btn btn-sm btn-danger classDisableForPosted" onclick="WHInvoiceDetails_DeleteList();"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                        </div>

                        <div id="divWHInvoiceDetails" class="col-sm-12 m-t-xs">
                            <section class="panel panel-default">
                                <header class="panel-heading font-bold text-ul">Invoice Details</header>
                                <!--Region: table header-->
                                <div class="table-responsive" style="width:100%; height:300px; overflow:scroll;">
                                    <table id="tblWHInvoiceDetails" class="table table-striped b-t b-light text-sm  table-hover ">
                                        <thead>
                                            <tr>
                                                <th id="HeaderDeleteWHInvoiceDetailsID" style="width:5%;">
                                                    <input id="cb-CheckAll-WHInvoiceDetails" type="checkbox" />
                                                </th>
                                                <th>@Forwarding.MvcApp.App_Resources.App_Resources.Item</th>
                                                <th>@Forwarding.MvcApp.App_Resources.App_Resources.Receive</th>
                                                <th>@Forwarding.MvcApp.App_Resources.App_Resources.Pickup</th>
                                                <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.Contract</th>
                                                <th>Space/Pal</th>
                                                <th>@Forwarding.MvcApp.App_Resources.App_Resources.Days</th>
                                                <th>@Forwarding.MvcApp.App_Resources.App_Resources.Rate</th>
                                                <th>@Forwarding.MvcApp.App_Resources.App_Resources.Amount</th>
                                                <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.Notes</th>
                                                <th class="rounded-right hide"></th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                                <!--Region: EOF table header-->
                                <!--Region: table footer-->
                                <footer class="panel-footer hide">
                                    <div class="col-sm-12 m-t-n-sm swapChildrenClass">
                                        <!--
                                            <div class="col-sm-6 swapChildrenClass"><b>&emsp;Row Max Weight(KGM)</b><label id="lblRowMaxWeight" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                            <div class="col-sm-6 text-right swapChildrenClass"><b>&emsp;Row Max Volume(CBM)</b><label id="lblRowMaxVolume" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                        -->
                                    </div>
                                </footer>
                                <!--Region: EOF table footer-->
                            </section>
                        </div>

                        <!--EOF Details-->
                    </div>
                </fieldset>
            </div>
        </div>
        <!-- Endregion Add / Edit Region Form -->
        <div class="modal-footer m-t-n-lg">
            <button type="button" id="btnSave" class="btn btn-primary btn-sm classDisableForPosted" onclick="WHInvoice_Save(false);">
                <span class="glyphicon glyphicon-save"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Save
            </button>
            <button type="button" id="btnPrint" class="btn btn-lightblue btn-sm" onclick="WHInvoice_Print(0);">
                <span class="fa fa-print"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Print
            </button>
            <button type="button" id="btnSaveAndAddNew" class="btn btn-primary btn-sm classDisableForPosted hide" onclick="">
                <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
            </button>
            <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
        </div>
    </div>
</div>
</div>
