<div class="modal fade" id="WHInvoiceDetailsModal" tabindex="-1" style="z-index: 1050;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="WHInvoiceDetailsModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-60">
        <div class="modal-content m-t-n-lg">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;</button>*@
                <h3 class="m-b-none">
                    Invoice Details&nbsp;<label id="lblWHInvoiceDetailsShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <div class="modal-body">
                <!-- Region Add / Edit WHInvoiceDetails Form -->
                <div class="span7 form vertical-mode" id="WHInvoiceDetails-form">
