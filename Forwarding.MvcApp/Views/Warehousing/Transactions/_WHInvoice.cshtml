                            </div>
                            <div class="form-group col-sm-3">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Receive</label>
                                <select id="slWHInvoiceDetailsReceive" class="form-control m-b input-sm" data-required="false"></select>
                            </div>
                            <div class="form-group col-sm-3">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Pickup</label>
