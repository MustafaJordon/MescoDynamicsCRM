<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="Accounting" href="javascript:void(0);" onclick="LoadViews('AccountingReports')">@Forwarding.MvcApp.App_Resources.App_Resources.Accounting
    </a></li>
    <li><a id="AccountingReports" href="javascript:void(0);" onclick="LoadViews('AccountingReports')">@Forwarding.MvcApp.App_Resources.App_Resources.Reports
    </a></li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.SubAccountLedger</li>
</ul>
<!-- end breadcrumb  -->


<div id="div-SubAccountLedger">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.SubAccountLedger 
                </h3>
            </header>
            <div class="row text-sm wrapper hidden-print m-t-n-sm">
                <div class="swapChildrenClass">
                    <div class="input-group col-sm-2 hide">
                        <label>Sub-Accounts</label>
                        <select id="slSubAccount" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"><option value=""><--Select--></option></select>
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                        <input id="txtFromDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                        <input id="txtToDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                    </div>

                    <div class="input-group col-sm-2 hide">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Status</label>
                        <select id="slStatus" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm">
                            <option value="0">@Forwarding.MvcApp.App_Resources.App_Resources.All</option>
                            <option value="1">@Forwarding.MvcApp.App_Resources.App_Resources.Posted</option>
                            <option value="2">@Forwarding.MvcApp.App_Resources.App_Resources.UnPosted</option>
                        </select>
                    </div>
                    <div class="input-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Group</label>
                        <select id="slSubAccountGroup" onchange="SubAccountLedger_slSubAccountGroupChanged();" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>
                    <div class="input-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.BankTemplate</label>
                        <select id="slBankTemplate" onchange="" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>
                    <div class="input-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                        <select id="slCurrency" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>
                    <div class="input-group col-sm-2 m-t-10">
                        <label class="swapChildrenClass"><input type="checkbox" id="cbIsAccrual" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;Accrual Only(Client st)&nbsp;</label>
                        <label class="swapChildrenClass"><input type="checkbox" id="cbSuppressForZeroes" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SuppressForZeroes&nbsp;</label><br />
                        <label class="swapChildrenClass"><input type="checkbox" id="cbHeaderLogo" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Header&nbsp;</label>
                      </div>
                </div>
            </div>
            <div class="row text-sm wrapper hidden-print m-t-n-md">
                <div class="swapChildrenClass">
                    <div class="col-sm-2 swapChildrenClass m-t-xs">
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_show();" id="cbIsDetails" checked="checked" onfocus="    DisableEnterKey(id);" onkeypress="    DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Detailed&nbsp;</b>
                    </div>                                                 
                    <div class="col-sm-2 swapChildrenClass m-t-xs">        
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_show();" id="cbIsGroupByCostCenter" onfocus="    DisableEnterKey(id);" onkeypress="    DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.CostCenterBySubAccount&nbsp;</b>
                    </div>                                                 
                    <div class="col-sm-2 swapChildrenClass m-t-xs">       
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_show();" id="cbIsByCostCenter" onfocus="    DisableEnterKey(id);" onkeypress="    DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.CostCenterByAccount&nbsp;</b>
                    </div>                                                
                    <div class="col-sm-2 swapChildrenClass m-t-xs">       
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_show();" id="cbIsByCurrency" onfocus="    DisableEnterKey(id);" onkeypress="    DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.ByCurrency&nbsp;</b>
                    </div>                                                 
                    <div class="col-sm-2 swapChildrenClass m-t-xs">      
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_hide();" id="cbIsClientStatmentEn" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.ClientStatmentEn&nbsp;</b>
                    </div>                                                 
                    <div class="col-sm-2 swapChildrenClass m-t-xs">        
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_hide();" id="cbIsClientStatmentAr" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.ClientStatmentAr&nbsp;</b>
                    </div>
                </div>
            </div>
            <div class="row text-sm wrapper hidden-print m-t-n-md">
                <div class="swapChildrenClass">
                    
                    <div class="col-sm-2 swapChildrenClass m-t-xs">
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_hide();" id="cbIsAgentStatmentEn" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.AgentStatmentEn&nbsp;</b>
                    </div>
                    <div class="col-sm-2 swapChildrenClass m-t-xs">
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_hide();" id="cbIsAgentStatmentAr" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.AgentStatmentAr&nbsp;</b>
                    </div>                                                
                    <div class="col-sm-2 swapChildrenClass m-t-xs">       
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_hide();" id="cbIsCostCenterSummary" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.CostCenterSummary&nbsp;</b>
                    </div>                                                 
                    <div class="col-sm-2 swapChildrenClass m-t-xs ">      
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_hide();" id="cbIsCostCenterProfit" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.CostCenterProfit&nbsp;</b>
                    </div>                                              
                    <div class="col-sm-2 swapChildrenClass m-t-xs">       
                        <input tabindex="" type="radio" name="nameRdGroup" onchange="BranchLedger_hide();" id="cbIsSupplierStatementEn" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SupplierStatementEn&nbsp;</b>
                    </div>
                    <div class="input-group col-sm-2 hide m-t-xs">
                        <label class=""><input type="checkbox" id="cbShowSubAccount" onchange="BranchLedger_hide();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.ShowSubAccounts&nbsp;</label><br />
                    </div>
                    <div class="col-sm-2 swapChildrenClass  m-t-xs">
                        <input tabindex="" type="radio" name="nameRdGroup"  id="cbIsGroupByBranch" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" onchange="BranchLedger_show();"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.ByBranch&nbsp;</b>
                    </div>
                    <div class="col-sm-9">
                        <button id="btn-ExportToExcel" type="button" onclick="SubAccountLedger_Print('Excel');" class="btn btn-sm btn-lightblue"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>
                        <button id="btn-Print" type="button" onclick="SubAccountLedger_Print('Print');" class="btn btn-sm btn-info"><i class="fa fa-print"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print&nbsp;</b></button>
                          
                                <div class="input-group col-sm-6">
                                    <input type="text" id="txtToEmail" placeholder=" Send Email To " class="form-control input-sm">
                                    <span id="btn-SendEmail" title="Send PDF Report as Email " class="input-group-addon" onclick="SubAccountLedger_Print('Email');" style="background-color:green;padding:5px!important;color:white;cursor:pointer;"><span class="fa fa-envelope">&nbsp; @Forwarding.MvcApp.App_Resources.App_Resources.Send</span>&nbsp;</span>
                                </div>
                            
                    </div>
                </div>
            </div>
                <div class="row text-sm wrapper hidden-print m-t-n-md">
                    <section id="secSubAccount" class="m-l col-sm-3 panel panel-default" style="clear:both;">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllSubAccounts" onchange="SubAccountLedger_cbCheckAllSubAccountsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllSubAccounts&nbsp;</u></label><br />
                        </div>
                        <div id="divCbSubAccount" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>
                    <section id="secAccount" class="m-l col-sm-3 panel panel-default">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllAccounts" onchange="SubAccountLedger_cbCheckAllAccountsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllAccounts&nbsp;</u></label><br />
                        </div>
                        <div id="divCbAccount" style="width:107%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>
                    <section id="secCostCenter" class="m-l col-sm-2 panel panel-default">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllCostCenters" onchange="SubAccountLedger_cbCheckAllCostCentersChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllCostCenters&nbsp;</u></label><br />
                        </div>
                        <div id="divCbCostCenter" style="width:107%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>
                    <section id="secBranch" class="m-t-xs m-l col-sm-3 panel panel-default ">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllBranch" onchange="BranchLedger_cbCheckAllBranchChanged();" onfocus=" DisableEnterKey(id);" onkeypress=" DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllBranch&nbsp;</u></label><br />
                        </div>
                        <div id="divCbBranch" class="swapCheckBoxesClass" style="width:107%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>

                    <!--EOF Region Filter-->
                    <div id="hExportedTable" class="hide"></div>
                    <div id="hExportdiv" class="hide"></div>
                </div>


                <footer class="panel-footer hide">
                    <div class="row">
                        <div class="col-sm-4 hidden-xs">
                            <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                                <option value="10">10</option>
                                <option value="25">25</option>
                                <option value="50">50</option>
                            </select>
                            <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                                @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                            </button>
                        </div>
                        <div id="div-Text-Total" class="col-sm-4 text-center">
                            <small class="text-muted inLine m-t-sm m-b-sm">
                                @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                                <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                                <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                            </small>
                        </div>
                        <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                    </div>
                </footer>
</section>
    </div>
</div>
