<style>
    .table th, table td {
        position: center;
        width: 200px;
    }
</style>
<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="Accounting" href="javascript:void(0);" onclick="LoadViews('AccountingMasterData')">
            @Forwarding.MvcApp.App_Resources.App_Resources.Accounting
        </a>
    </li>
    <li>
        <a id="AccountingMasterData" href="javascript:void(0);" onclick="LoadViews('AccountingMasterData')">
            @Forwarding.MvcApp.App_Resources.App_Resources.MasterData
        </a>
    </li>
<li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.A_Investmentcostcenters</li>
</ul>
<!-- end breadcrumb  -->
<section class="panel panel-default">
    <div id="div-SL_Invoices">
        <!-- Add / Edit Region Modal --> <!--data-keyboard="false" data-backdrop="false" to prevent modal closure on pressing escape and pressing outside the modal-->

        <div class="modal fade" id="SL_InvoicesModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="SL_InvoicesModalLabel" aria-hidden="true">

        </div>
        <!-- Region Options & Template  -->

        <div id="div-Data">
            <div id="div-Options">
                <div id="div-SL_Invoices" class="radius-top-left-right">
                </div>
            </div>
            <div id="div-search-controls " class="input-group col-sm-9 hide">
                <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search Store Name / Account Name">
                <span class="input-group-btn">
                    <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="SL_Invoices_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                </span>
            </div>


        </div>

        <div class="modal-header">
            <h3 class="m-b-none">
                @Forwarding.MvcApp.App_Resources.App_Resources.A_Investmentcostcenters&nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
            </h3>
        </div>
        <!-- Region Add / Edit Region Form -->
        <div class="modal-body">
            <div class="span7 form vertical-mode" id="SL_Invoices-form">
                <fieldset>
                    <div class="form-group pull-in  clearfix">
                        <input id="hID" type="hidden" />
                        <!-- start alert message region -->
                        <div class="alert alert-danger div-error">
                            @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                        </div>
                        <!-- end alert message region -->
                        <!-- Region Add / Edit Region Form -->
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group has-error clearfix col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter 1</label>
                                    <input id="txtCostCenterID1" type="text" class="form-control parsley-validated  input-sm" data-required="false" data-minlength="1" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group has-error clearfix col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter</label>
                                    <select id="slCostCenterID" class="input-sm form-control Edit-input" data-required="true" onchange=""></select>
                                </div>

                                <div class="form-group has-error clearfix col-sm-3 hide">
                                    <label>A_CostCenterGroup</label>
                                    <select id="slCostCenterGroupsID" class="input-sm form-control Edit-input" data-required="true" onchange=""></select>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group has-error clearfix col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter 2</label>
                                    <input id="txtCostCenterID2" type="text" class="form-control parsley-validated  input-sm" data-required="false" data-minlength="1" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group has-error clearfix col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter 2</label>
                                    <select id="slCostCenterID2" class="input-sm form-control Edit-input" data-required="true" onchange=""></select>
                                </div>

                                <label class="hide">AddCostCenter</label><br />
                                <button id="btn-NewAddCostCenter" class="btn btn-warning btn-sm hide" data-toggle="modal" data-target=""><i class="fa fa-plus"></i>&nbsp;AddCostCenter</button>

                            </div>

                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group has-error clearfix col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter 3</label>
                                    <input id="txtCostCenterID3" type="text" class="form-control parsley-validated  input-sm" data-required="false" data-minlength="1" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group has-error clearfix col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter 3</label>
                                    <select id="slCostCenterID3" class="input-sm form-control Edit-input" data-required="true" onchange=""></select>
                                </div>

                                <label class="hide">AddCostCenter</label><br />
                                <button id="btn-NewAddCostCenter" class="btn btn-warning btn-sm hide" data-toggle="modal" data-target=""><i class="fa fa-plus"></i>&nbsp;AddCostCenter</button>

                            </div>

                        </div>

                    </div>
                </fieldset>
            </div>


            @*taaaaaaabs*@
            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#tabItems"><b hidden>Items</b> <span hidden id="lblTotalItems">0.00</span> [<b hidden>I:</b> <span hidden id="lblTotalItems_I">0.00</span> <b>Total:</b>&nbsp;<span id="lblTotalItems_S">0.00</span>&nbsp;] </a> </li>
            </ul>

            <div class="tab-content">
                <div id="tabItems" class="tab-pane fade in active">
                    @* Iteeeeeeems*@
                    <div class="row">
                        <a href="#" onclick="AddNewItemsRow();" style="border-radius:25px;margin-left:20px" class="btn btn-lightblue Edit-btn float-left m-sm"><span class="fa fa-plus "></span>&nbsp; Add Details</a>
                    </div>
                    <div class="table-responsive" style="height:240px;overflow:scroll;">
                        <table id="tblItems" class="table b-t b-light text-sm  table-hover">
                            <thead>
                                <tr @*style="display:block"*@>
                                    <th></th>
                                    <th></th>
                                    <th class="hidden">Cost Center ID</th>
                                    @*<th class="hidden">Store</th>
                                    <th class="hidden">Qty in store</th>
                                    <th class="hidden">Unit Price</th>
                                    <th class="hidden">Discount</th>*@
                                    <th>Cost Center Name</th>
                                    <th>Part</th>
                                    <th>Part Ratio</th>
                                    <th>Value</th>
                                    @*<th class="hidden">Price</th>
                                    <th class="hidden">Notes</th>
                                    <th class="hidden">Tax Ratio</th>*@
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody @*style="display:block;height:300px;overflow:scroll;"*@></tbody>
                        </table>
                    </div>




                    @* Iteeeeeeems*@
                </div>

            </div>

        </div>
        <!-- Endregion Add / Edit Region Form -->
        <div class="modal-footer">
            <button type="button" id="btnSave" class="btn btn-primary btn-sm Edit-btn" onclick="SL_Invoices_Save(false);">
                <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
            </button>

            <button type="button" id="btnPrint2" class="btn btn-lightblue btn-sm PrintTtansaction hide" onclick="PrintInvoice(0);">
                <span class="fa fa-print"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Print
            </button>



            <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm hide" onclick="SL_Invoices_Save(true);">
                <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
            </button>
            <button class="hidden btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->

        </div>

        <!--Region: footer-->
        <footer class="hidden panel-footer">
            <div class="row">
                <div class="col-sm-4 hidden-xs">
                    <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                        <option value="10">10</option>
                        <option value="25">25</option>
                        <option value="50">50</option>
                    </select>
                    <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                        @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                    </button>
                </div>
                <div id="div-Text-Total" class="col-sm-4 text-center">
                    <small class="text-muted inline m-t-sm m-b-sm">
                        @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                        <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                        <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                    </small>
                </div>
                <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>



            </div>
        </footer>
        <select id="hidden_slItems" style="visibility:hidden" class="input-sm form-control" data-required="false" onchange=""></select>
        <select id="hidden_slServices" style="visibility:hidden" class="input-sm form-control" data-required="false" onchange=""></select>

    </div>
</section>  