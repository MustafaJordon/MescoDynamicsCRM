<!-- CostCentersModal -->
<div class="modal fade " id="CostCentersModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="CostCentersModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-50">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;</button>
                <h3 class="m-b-none swapChildrenClass"><span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.CostCenters&nbsp;</span><label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <div class="modal-body">
                <!-- Region Add / Edit Region Form -->
                <div class="span7 form vertical-mode" id="Region-form">
                    <fieldset>
                        <div class="form-group pull-in has-error clearfix">
                            <input id="hID" type="hidden" />
                            <input id="hActionType" type="hidden" />
                            <input id="hParentID" type="hidden" />
                            <input id="hLevel" type="hidden" />
                            <input id="hRealCode" type="hidden" />
                            <!-- start alert message region -->
                            @*<div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>*@
                            <!-- end alert message region -->
                            <div class="swapChildrenClass">
                                <div class="form-group col-sm-5">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                    <input id="txtCode" tabindex="10" disabled="disabled" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated input-sm"  data-required="true" maxlength="" placeholder="Generated Automatically" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group col-sm-7">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                                    <input type="text" id="txtName" tabindex="20" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="true" maxlength="50" placeholder="" style="text-transform:uppercase"/>
                                </div>
                            </div>

                            <div class="swapChildrenClass hide">
                                <div class="form-group col-sm-9">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenterType</label>
                                    <select tabindex="60" id="slCostCenterType" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control m-b input-sm" data-required="false"></select>
                                </div>
                                <div class="col-sm-3"></div>
                            </div>
                            
                            <div class="swapChildrenClass hide" style="clear:both;">
                                <div class="col-sm-3 m-t-xs ">
                                    <label class="swapChildrenClass"><input tabindex="60" type="checkbox" id="cbIsClosed" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Closed&nbsp;</span></label><br />
                                </div>
                                <div class="form-group col-sm-9 m-t-sm"></div>
                            </div>

                        </div>
                    </fieldset>
                </div>
                <!-- Endegion Add / Edit Region Form -->
            </div>
            <div class="modal-footer m-t-n">
                <button type="button" id="btnSave" class="btn btn-primary btn-sm swapChildrenClass">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Save&nbsp;</b></button>
                <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm swapChildrenClass hide">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew&nbsp;</b></button>
                <button class="btnClose btn btn-link btn-sm" data-dismiss="modal" aria-hidden="true"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Close</b></button>
            </div>
        </div>
    </div>
</div>
<!--EOF CostCentersModal -->
