<div id="div-ForeignCurrencyRevaluation">


    <div id="div-Data">
       
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.ForeignCurrencyRevaluation
            </h3>
        </header>


            <div class="span7 form vertical-mode" id="ForeignCurrencyRevaluation-form">
             <fieldset class="">
                 <div style="margin-left:25%; margin-right:25%;" class="form-group  col-sm-6 ">
                     <div class="row">
                         <div class="form-group  col-sm-3">
                             <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                             <input id="txtDateFrom" tabindex="20" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.SelectFromMenu" />
                         </div>
                         <div class="form-group col-sm-6 ">
                             <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                             <input id="txtDateTo" tabindex="30" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.SelectFromMenu" />
                         </div>

                         <div class="form-group col-sm-3 ">
                             <label>@Forwarding.MvcApp.App_Resources.App_Resources.ExchangeRate</label>
                             <input id="txtExchangeRate" tabindex="70" type="text" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" @*placeholder="value from 0.00001 to 99999.99999"*@ />
                         </div>
                     </div>

                     <div class="row">
                     <div class="form-group col-sm-3 ">
                         <label>@Forwarding.MvcApp.App_Resources.App_Resources.JVDate</label>
                         <input id="txtJVDate" tabindex="30" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.SelectFromMenu" />
                     </div>
                     <div class="form-group col-sm-3 ">
                         <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                         <select id="slCurrency" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                     </div>
                     <div class="form-group col-sm-6">
                         <div class="col-sm-12">
                             <label>@Forwarding.MvcApp.App_Resources.App_Resources.ForeignCurrencyAccount</label>
                             <select id="slForeignCurrencyAccountID" class="input-sm form-control" onchange=""></select>
                         </div>
                     </div>


                         </div>
                    


                     <div class="row">
                         <div class="form-group col-sm-12">
                             <div class="col-sm-12">
                                 <label style="text-align: left;">@Forwarding.MvcApp.App_Resources.App_Resources.SubAccount</label>
                                 <select id="slSubaccount" class="input-sm form-control" onchange=""></select>
                             </div>
                         </div>
                     </div>

                         @*<div class="form-group col-sm-6 ">
                         <label></label>
                        </div>
                             <br>*@

                         <div class="form-group col-sm-12 m-t-lg">
                                 <div class="">
                                     <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllAccounts" onchange="AccountLedger_cbCheckAllAccountsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllAccounts&nbsp;</u></label><br />
                                 </div>
                                 <div id="div_Accounts" style="border:solid 3px black!important; height:300px; overflow-y:scroll;">
                                     <ul id="ul_Accounts"></ul>
                                 </div>
                         </div>

                         <div class="form-group  col-sm-12">
                             <div class="form-group">
                                 <button type="button" id="btnSave" class="btn btn-danger btn-sm float-right" onclick="CreateJV();">
                                     <span class="fa fa-retweet">@Forwarding.MvcApp.App_Resources.App_Resources.CreateJV</span>@*&nbsp;Create JV*@
                                 </button>

                             </div>
                         </div>
                     </div>

              </fieldset>
                </div>




                <footer class="panel-footer">
                    <div class="row">

                    </div>
                </footer>
                <!--Region: footer-->
            </section>
        </div>
    </div>
