<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="liOperAcc" href="javascript:void(0);">
            @Forwarding.MvcApp.App_Resources.App_Resources.Accounting
        </a>
    </li>
    <li>
        <a id="liApprovals" href="javascript:void(0);">
            @Forwarding.MvcApp.App_Resources.App_Resources.AccountingTransactions
        </a>
    </li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.UnapprovingPayableAllocations</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-UnapprovingAllocations">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none"> @Forwarding.MvcApp.App_Resources.App_Resources.UnapprovingPayableAllocations</h3>
            </header>
            <!--Region search, delete, addnew-->
            <div id="div-search-option" class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">
                <div class="swapChildrenClass">
                    <div class="form-group  col-sm-2" style="">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.PartnerType</label>
                        <div class="input-group">
                            <select id="slPartnerType" class="input-sm form-control" data-required="false" onchange=""></select>
                            <span id="btnCopyStores" title="Copy" class="input-group-addon btn-lightblue" onclick="FillParteners();" style="padding:5px!important;color:white;cursor:pointer;">&nbsp;<span class="fa fa-filter"></span>&nbsp;</span>
                        </div>
                    </div>

                    <div class="form-group  col-sm-2" style="">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Partner</label>
                        <div class="">
                            <select id="slPartner" class="input-sm form-control" data-required="false" onchange=""></select>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                        <input id="txtSearchFrom" tabindex="20" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                    </div>

                    <div class="col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                        <input id="txtSearchTo" tabindex="30" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                    </div>

                    <div id="" class="col-sm-3 m-t-xs">
                        <br />
                        <span class="">
                            <button id="btn-search" title="Search" class="btn btn-sm btn-primary" type="button" onclick="UnapprovingAllocations_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                        <button id="btn-UnPost" type="button" onclick="UnapprovingAllocations_UnpostList();" class="btn btn-danger btn-sm swapChildrenClass" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-backward"></i><b>&nbsp; @Forwarding.MvcApp.App_Resources.App_Resources.UnApproveSelected &nbsp;</b></button>

                    </div>
                    <div class="float-right col-sm-2 hide">
                        <button id="btn-Delete-Remove" type="button" class="float-right btn btn-sm btn-danger m-r-n swapChildrenClass hide"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>
                    </div>
                </div>
            </div>
            <div class="row">
                <div id="div-search-controls" class="col-sm-3">
                    <select id="hiddenslCurrecies" style="visibility:hidden;" class="input-sm form-control" data-required="false" onchange=""></select>
                </div>
            </div>
            <!--EOF Region search, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive m-t-n-sm">
                <table id="tblUnapprovingAllocations" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Partner</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Amount</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ExchangeRate</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.CreationDate</th>

                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>

