<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="SC" href="javascript:void(0);" onclick="LoadViews('SC')">@Forwarding.MvcApp.App_Resources.App_Resources.SC
    </a></li>
    <li><a id="SC_Maintenance" href="javascript:void(0);" onclick="LoadViews('SC_Maintenance')">@Forwarding.MvcApp.App_Resources.App_Resources.SC_Maintenance
    </a></li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.MaterialIssueVouchersFollowUp</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-AccountLedger">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.MaterialIssueVouchersFollowUp 
                </h3>
            </header>
            <div class="row text-sm wrapper hidden-print m-t-n-sm">
                <div class="swapChildrenClass">
                    <!--Region Filter-->
                    <div class="form-group col-sm-3">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                        <input id="txtFromDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                    </div>
                    <div class="form-group col-sm-3">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                        <input id="txtToDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                    </div>
                    <div class="form-group col-sm-3">
                    </div>
                    <div class="form-group col-sm-3">
                    </div>

                </div>
            </div>
            <div class="row text-sm wrapper hidden-print m-t-n-md">
                <div class="col-sm-12 swapChildrenClass">
                    <div class="col-sm-2  m-t-xs">
                        <input tabindex="" type="radio" name="nameReportType" id="1" checked="checked" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Details&nbsp;</b>
                        @*<input tabindex="" type="radio" name="nameReportType" id="2" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.PriceDetails&nbsp;</b>*@
                    </div>
                    <div class="col-sm-2  m-t-xs hide">
                        @*<input tabindex="" type="radio" name="nameReportType" id="1" checked="checked" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TransactionDetails&nbsp;</b>*@
                        <input tabindex="" type="radio" name="nameReportType" id="2" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.PriceDetails&nbsp;</b>
                    </div>
                    <div class="col-sm-8">
                        <button id="btn-ExportToExcel" type="button" onclick="SC_Reports_Print('Excel');" class="btn btn-sm btn-lightblue"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>
                        <button id="btn-Print" type="button" onclick="SC_Reports_Print('Print');" class="btn btn-sm btn-info"><i class="fa fa-print"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print&nbsp;</b></button>
                    </div>

                    @*<div class=" col-sm-4">
                    </div>*@
                </div>
                    
                    
                
            </div>
            <div class="row text-sm wrapper hidden-print m-t-n-md">
                <div class="swapChildrenClass">
                    <section id="secStores" class="m-t-xs  col-sm-2 panel panel-default">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllStores" onchange="cbCheckAllStoresChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllStores&nbsp;</u></label><br />
                        </div>
                        <div id="divCbStores" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>
                    <section id="secItems" class="m-t-xs  col-sm-2 panel panel-default">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllItems" onchange="cbCheckAllItemsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllItems&nbsp;</u></label><br />
                        </div>
                        <div id="divCbItems" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>
                    <section id="secBranches" class="m-t-xs  col-sm-2 panel panel-default">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllBranches" onchange="cbCheckAllBranchesChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;All Branches&nbsp;</u></label><br />
                        </div>
                        <div id="divCbBranches" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>
                    <section id="secDepartments" class="m-t-xs  col-sm-2 panel panel-default">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllDepartments" onchange="cbCheckAllDepartmentsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;All Departments&nbsp;</u></label><br />
                        </div>
                        <div id="divCbDepartments" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>
                    <section id="secTrailers" class="m-t-xs  col-sm-2 panel panel-default">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllTrailers" onchange="cbCheckAllTrailersChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;All Trailers&nbsp;</u></label><br />
                        </div>
                        <div id="divCbTrailers" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>
                    <section id="secEquipments" class="m-t-xs  col-sm-2 panel panel-default">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllEquipments" onchange="cbCheckAllEquipmentsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;All Equipments&nbsp;</u></label><br />
                        </div>
                        <div id="divCbEquipments" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>
                    <section id="secSuppliers" class="m-t-xs  col-sm-2 panel panel-default">
                        <div class="">
                            <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllSuppliers" onchange="cbCheckAllSuppliersChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;All Suppliers&nbsp;</u></label><br />
                        </div>
                        <div id="divCbSuppliers" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                        </div>
                    </section>








                    <section id="" class=" col-sm-1"></section>
                    <!--EOF Region Filter-->
                    <div id="hExportedTable" class="hide"></div>
                </div>
            </div>

            <footer class="panel-footer hide">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inLine m-t-sm m-b-sm"> @Forwarding.MvcApp.App_Resources.App_Resources.Showing 
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
        </section>
    </div>
</div>
