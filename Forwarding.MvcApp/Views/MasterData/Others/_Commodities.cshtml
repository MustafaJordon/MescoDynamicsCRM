                                        <th class="rounded-right"></th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                        <!--Region: table footer-->
                        <footer class="panel-footer"></footer>
                        <!--Region: EOF table footer-->
                        <!--Region: EOF table header-->
                    </section>
                </div>
            </fieldset>
        </div>
    </div>
    <div class="modal-footer swapChildrenClass">
        @*<button tabindex="1200" id="btnSaveDocsIn" type="button" class=" hide btn btn-primary btn-sm swapChildrenClass">
                <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Tourism.MvcApp.App_Resources.App_Resources.Save&nbsp;</b></button>
            <button tabindex="2010" id="btnSaveandNewDocsIn" type="button" class="hide btn btn-primary btn-sm swapChildrenClass">
                <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Tourism.MvcApp.App_Resources.App_Resources.SaveAndNew&nbsp;</b></button>*@
        <button tabindex="1210" id="btnCloseDocsIn" class="btnClose btn btn-link btn-sm" onclick="" data-dismiss="modal" aria-hidden="true"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Close</b></button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
    </div>
</div>
</div>
</div>
<!--EOF DocsIn-->
