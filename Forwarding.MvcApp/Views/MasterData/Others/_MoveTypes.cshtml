<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="liGroupName" href="javascript:void(0);">@Forwarding.MvcApp.App_Resources.App_Resources.MasterData
    </a></li>
    <li><a id="liTabName" href="javascript:void(0);">@Forwarding.MvcApp.App_Resources.App_Resources.Others
    </a></li>
    <li id="liFormName" class="active">@Forwarding.MvcApp.App_Resources.App_Resources.MoveTypes</li>
</ul>
