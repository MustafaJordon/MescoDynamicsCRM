<!-- start breadcrumb  -->
@*<script src="~/Scripts/utilities/bootstrap-hijri-datepicker.js"></script>*@
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="MasterData" href="javascript:void(0);" onclick="LoadViews('CustomsClearance')">@Forwarding.MvcApp.App_Resources.App_Resources.MasterData
    </a></li>
    <li><a id="CustomsClearance" href="javascript:void(0);" onclick="LoadViews(id)">@Forwarding.MvcApp.App_Resources.App_Resources.CustomsClearance
    </a></li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.Authorizations</li>
</ul>
<!-- end breadcrumb  -->


<div id="div-Network">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.Authorizations
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm hide" data-toggle="modal" data-target="#AuthorizationsModal"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New</button>
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-7">
                    <div id="div-search-controls" class="input-group col-sm-9">
                        <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase"  type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search Authorization Number / registration Number / Owner Number">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="Authorizations_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-1">
                        <button id="btn-Delete" type="button" onclick="Authorizations_DeleteList();" class="float-right btn btn-sm btn-danger m-r-n hide"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                    </div>
                </div>
            </div>
            <!--Region the searh, delete, addnew-->

            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblAuthorizations" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>رقم التفويض </th>
                            <th class="">العميل</th>
                            <th class="">رقم السجل</th>
                            <th class="">تاريخ انتهاء السجل</th>
                            <th class="">رقم المالك</th>
                            <th class="">تاريخ البداية</th>
                            <th class="">تاريخ الانتهاء</th>

                            <th class="rounded-right hide"></th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
            <!--Region: table header-->

            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm"> @Forwarding.MvcApp.App_Resources.App_Resources.Showing 
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>

<!-- Add / Edit Region Modal -->
<div class="modal fade" id="AuthorizationsModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="AuthorizationsLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-60">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.Authorizations &nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="Network-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hID" type="hidden" />
                            <!-- start alert message region -->
                            <div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>
                            <!-- end alert message region -->

                            <!-- Region Add / Edit Region Form -->
                            <div class="row col-sm-12">
                                <div class="form-group col-sm-6">
                                    <label>Authorization Number - رقم التفويض</label>
                                    <input id="txtAuthorizationNumber" autocomplete="off" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="200" placeholder="Authorization Number" />
                                </div>
                                <div class="form-group col-sm-6">
                                    <label>Customer - العميل</label>
                                    <select id="slCustomers" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);"></select>
                                </div>
                            </div>
                            <div class="row col-sm-12">
                                <div class="form-group col-sm-6">
                                    <label>registration Number - رقم السجل</label>
                                    <input id="txtregistrationNumber" autocomplete="off" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="200" placeholder="registration Number" />
                                </div>
                                <div class="form-group col-sm-6">
                                    <label>registration End Date - تاريخ انتهاء السجل</label>
                                    @*<input id="date_registration_EndDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="true" data-date-format="dd/mm/yyyy" placeholder="Select Date" />*@
                                    <input type='text' class="form-control input-sm" id="date_registration_EndDate" autocomplete="off" placeholder="registration End Date"/>
                                </div>
                            </div>
                            <div class="row col-sm-12">
                                @*<div class="form-group col-sm-6">
                                    <label>registration End Date Islamic</label>
                                    <input id="date_registration_EndDate_Islamic" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="true" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                </div>*@
                                <div class="form-group col-sm-6">
                                    <label>Owner Number - رقم المالك</label>
                                    <input id="txtOwnerNumber" autocomplete="off" type="number" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="200" placeholder="Owner Number" />
                                </div>
                                <div class="form-group col-sm-6">
                                    <label>Start Date - تاريخ البداية</label>
                                    @*<input id="date_StartDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="true" data-date-format="dd/mm/yyyy" placeholder="Select Date" />*@
                                    <input type='text' class="form-control input-sm" autocomplete="off" id="date_StartDate" placeholder="Start Date" />
                                </div>
                            </div>
                            <div class="row col-sm-12">
                              
                                @*<div class="form-group col-sm-6">
                                    <label>Start Date Islamic</label>
                                    <input id="date_StartDate_Islamic" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="true" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                </div>*@
                            </div>
                            <div class="row col-sm-12">
                                <div class="form-group col-sm-6">
                                    <label>Notes - ملاحظات</label>
                                    <input id="txtNotes" autocomplete="off" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="true" maxlength="200" placeholder="Notes" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group col-sm-6">
                                    <label>End Date - تاريخ الانتهاء</label>
                                    @*<input id="date_EndDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="true" data-date-format="dd/mm/yyyy" placeholder="Select Date" />*@
                                    <input type='text' class="form-control input-sm" autocomplete="off" id="date_EndDate" placeholder="End Date" />
                                </div>
                                @*<div class="form-group col-sm-6">
                                    <label>End Date Islamic</label>
                                    <input id="date_EndDate_Islamic" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="true" data-date-format="dd/mm/yyyy" placeholder="Select Date Islamic" />
                                </div>*@
                            </div>
                            <div class="row col-sm-12">
                               

                                <div class="form-group col-sm-6 hide">
                                    <input id="inputFileUpload" type="file" multiple="multiple" class="btn-lightblue btn-sm m-t-xs " />
                                    <a id="ImageName" onclick="ShowImage();" href=""></a>
                                </div></div>

                                <!-- Region Add / Edit Region Form -->
                            </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer m-t-n">
                <button type="button" id="btnSave" class="btn btn-primary btn-sm" onclick="Authorizations_Insert(false);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save</button>
                <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm" onclick="Network_Insert(true);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew</button>
                <button class="btnClose btn btn-link btn-sm" id="btnClose" data-dismiss="modal" aria-hidden="true" >@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div> 
        </div>
    </div>
</div>
<!-- Region Options & Template  -->

    