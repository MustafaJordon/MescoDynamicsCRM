<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="MasterData" href="javascript:void(0);" onclick="LoadViews('CustomsClearance')">@Forwarding.MvcApp.App_Resources.App_Resources.MasterData
    </a></li>
    <li><a id="CustomsClearance" href="javascript:void(0);" onclick="LoadViews(id)">@Forwarding.MvcApp.App_Resources.App_Resources.CustomsClearance
    </a></li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.CustomsItems</li>
</ul>
<!-- end breadcrumb  -->


<div id="div-CustomsItems">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.CustomsItems
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm hide" data-toggle="modal" data-target="#CustomsItemsModal"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New</button>
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-7">
                    <div id="div-search-controls" class="input-group col-sm-9">
                        <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase"  type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search Code / Description / Tariff">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="CustomsItems_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-1">
                        <button id="btn-Delete" type="button" onclick="CustomsItems_DeleteList();" class="float-right btn btn-sm btn-danger m-r-n hide"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                    </div>
                </div>
            </div>
            <!--Region the searh, delete, addnew-->

            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblCustomsItems" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>التذييلات </th>
                            <th class="">الوصف بالعربية</th>
                            <th class="">الوصف بالانجليزية</th>
                            <th class="">رمز التعريفة</th>
                            <th class="">النسبة</th>
                            <th class="rounded-right hide"></th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
            <!--Region: table header-->

            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm"> @Forwarding.MvcApp.App_Resources.App_Resources.Showing 
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>

<!-- Add / Edit Region Modal -->
<div class="modal fade" id="CustomsItemsModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="CustomsItemsLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-60">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.CustomsItems &nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="CustomsItems-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hID" type="hidden" />
                            <!-- start alert message region -->
                            <div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>
                            <!-- end alert message region -->

                            <!-- Region Add / Edit Region Form -->
                            <div class="form-group col-sm-6">
                                <label>التذييلات</label>
                                <input id="txtCode" autocomplete="off" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="true" maxlength="200" placeholder="Code" style="text-transform:uppercase"/>
                            </div>
                            <div class="form-group col-sm-6">
                                <label>Ar Description - الوصف بالعربية</label>
                                <input id="txtArDescription" autocomplete="off" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="true" maxlength="200" placeholder="Ar Description" style="text-transform:uppercase" />
                            </div>
                            <div class="form-group col-sm-6">
                                <label>En Description - الوصف بالانجليزية</label>
                                <input id="txtEnDescription" autocomplete="off" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="true" maxlength="200" placeholder="En Description" style="text-transform:uppercase" />
                            </div>
                            <div class="form-group col-sm-6">
                                <label>Tariff Code - رمز التعريفة</label>
                                <input id="txtTariffCode" autocomplete="off" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="true" maxlength="200" placeholder="Tariff Code" style="text-transform:uppercase" />
                            </div>
                            <div class="form-group col-sm-6">
                                <label>Discount - نسبة الخصم</label>
                                <input id="txtDiscount" autocomplete="off" type="number" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="true" maxlength="200" placeholder="Discount" style="text-transform:uppercase" />
                            </div>
                            <div class="form-group col-sm-6 hide">
                                <input id="inputFileUpload" type="file" class="btn-lightblue btn-sm m-t-md" />
                                <a id="ImageName" onclick="ShowImage();" href=""></a>
                            </div>

                                @*<div class="form-group col-sm-2 m-t-md">
                <label><input type="checkbox" id="cbIsOcean" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" />&nbsp;<i class= 'fa fa-anchor ocean-icon-style'/>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.IsOcean</label><br />
                <label><input type="checkbox" id="cbIsAir" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" />&nbsp;<i class= 'fa fa-plane air-icon-style'/>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.IsAir</label><br />
                <label><input type="checkbox" id="cbIsInland" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" />&nbsp;<i class= 'fa fa-truck inland-icon-style'/>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.IsInland</label>
            </div>
            <div class="form-group col-sm-2 m-t-md">
                <label><input type="checkbox" id="cbIsImport" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" />&nbsp;<i class= 'fa fa-arrow-circle-down import-icon-style'/>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Import</label><br />
                <label><input type="checkbox" id="cbIsExport" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" />&nbsp;<i class= 'fa fa-external-link export-icon-style'/>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Export</label><br />
                <label><input type="checkbox" id="cbIsDomestic" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" />&nbsp;<i class= 'fa fa-home domestic-icon-style'/>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Domestic</label><br />
            </div>
            <div class="col-sm-6 m-t">
                <label><input type="checkbox" id="cbIsInactive" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.IsInactive</label><br />
            </div>*@
                                <!-- Region Add / Edit Region Form -->
                            </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer m-t-n">
                <button type="button" id="btnSave" class="btn btn-primary btn-sm" onclick="CustomsItems_Insert(false);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save</button>
                <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm" onclick="CustomsItems_Insert(true);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew</button>
                <button class="btnClose btn btn-link btn-sm" id="btnClose" data-dismiss="modal" aria-hidden="true" >@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div> 
        </div>
    </div>
</div>
<!-- Region Options & Template  -->

    