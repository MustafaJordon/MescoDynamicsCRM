@*@{ Culture = UICulture = ViewBag.CutlureID; }*@
<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="MasterData" href="javascript:void(0);" onclick="LoadViews('Locations')">@Forwarding.MvcApp.App_Resources.App_Resources.MasterData
    </a></li>
    <li><a id="Others" href="javascript:void(0);" onclick="LoadViews(id)">@Forwarding.MvcApp.App_Resources.App_Resources.Others
    </a></li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.Rates </li>
</ul>
<!-- end breadcrumb  -->
<div id="div-RateRegions">
    <!-- Add / Edit Rate Modal -->
    <div class="modal fade" id="RateRegionsModal" tabindex="-1"  style="z-index:200;" data-keyboard="false" data-backdrop="false" role="dialog" aria-labelledby="CityModalLabel" aria-hidden="true">
        <div class="modal-dialog modalwidth-70">
            <div class="modal-content">
                <div class="modal-header">
                    @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        &times;</button>*@
                    <h3 class="m-b-none">Regions From-To&nbsp;<label id="lblCityShown" purpose="dynamicLabel" class="static-text-primary"></label>
                    </h3>
                </div>
                <div class="modal-body">
                    <!-- Country Add / Edit Rate Form -->
                    <div class="span7 form vertical-mode" id="RateRegions-form">
                        <fieldset>
                            <div class="row">

                                <div class="col-sm-12 form-group pull-in  has-error clearfix">
                                    <input id="hRateRegionsID" type="hidden" />
                                    <!-- start alert message Country -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message Country -->
                                    @*<div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                        <input id="txtCode" type="text" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" data-required="true" data-minlength="3" maxlength="3" placeholder="ex. EGY" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                                        <input type="text" id="txtName" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" data-required="true" maxlength="50" placeholder="ex. Egypt" style="text-transform:uppercase" />
                                    </div>*@
                                    <select id="slRegionBase" name="slRegionBase" data-required="false" class="form-control m-b hide" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);"></select>

                                    <div class="form-group col-sm-3">
                                        <label>City-From</label>
                                        <select id="slCityFrom" name="slCityFrom" onchange="GetRegionsInCity('From');"   data-required="true" class="form-control m-b"  onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="    DisableBackspaceKey(id);"></select>
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Region-From</label>
                                        <select id="slRegionFrom" name="slRegionFrom" class="form-control m-b" onfocus="DisableBackspaceKey(id);" data-required="true"  onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);"></select>
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Cost</label>
                                        <input type="number" id="txtCost" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" data-required="false"  onkeypress="DisableEnterKey(id);" maxlength="50" placeholder="500" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Selling</label>
                                        <input type="number" id="txtSelling" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" data-required="true"  onkeypress="DisableEnterKey(id);"  maxlength="50" placeholder="1000" style="text-transform:uppercase" />
                                    </div>

                                </div>
                                <div class="col-sm-12 form-group pull-in  has-error clearfix">
                                        @*<div class="form-group col-sm-3">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.LocalName</label>
                                            <input type="text" id="txtLocalName" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" data-required="false" maxlength="50" placeholder="ex. القاهرة" style="text-transform:uppercase" />
                                        </div>*@
                                      
                                        <div class="form-group col-sm-3">
                                            <label>City-To</label>
                                            <select id="slCityTo" name="slCityTo" data-required="true" onchange="GetRegionsInCity('To');" class="form-control m-b" onfocus="    DisableBackspaceKey(id);" onkeydown="    DisableBackspaceKey(id);" onkeypress="    DisableBackspaceKey(id);"></select>
                                        </div>
                                        <div class="form-group col-sm-3">
                                            <label>Region-To</label>
                                            <select id="slRegionTo" name="slRegionTo" data-required="true" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);"></select>
                                        </div>
                                    <div class="form-group col-sm-3">
                                        <label>Package Type</label>
                                        <select id="slPackageTypeID" name="slPackageTypeID" class="form-control m-b" onfocus="DisableBackspaceKey(id);" data-required="false" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);"></select>
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Quantity</label>
                                        <input type="number" id="txtQuantity" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" data-required="false" maxlength="50" placeholder="1000" style="text-transform:uppercase" />
                                    </div>
                                    </div>
                                    <div class="col-sm-12 form-group pull-in  has-error clearfix">

                                        <div class="form-group col-sm-6">
                                            <label>Remarks</label>
                                            <textarea type="number" id="txtRemarksRateRegions" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" data-required="false" placeholder="remarks"></textarea>
                                        </div>
                                    </div>
                            </div>
                        </fieldset>
                    </div>
                    <!-- Endegion Add / Edit Rate Form -->
                </div>
                <div class="modal-footer"><!--sherif-->
                    <button type="button" id="btnSaveRateRegions" class="btn btn-primary btn-sm" onclick="RateRegions_Insert_Update(false);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save</button>
                    <button type="button" id="btnSaveandNewRateRegions" class="btn btn-primary btn-sm" onclick="RateRegions_Insert_Update(true);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew</button>
                    <button class="btnClose btn btn-link btn-sm" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="RateModal" tabindex="-1" style="z-index:100;" data-keyboard="false" data-backdrop="false" role="dialog" aria-labelledby="CityModalLabel" aria-hidden="true">
        <div class="modal-dialog modalwidth-90">
            <div class="modal-content">
                <div class="modal-header">
                    @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        &times;</button>*@
                    <h3 class="m-b-none">
                        Regions From-To&nbsp;<label id="lblCityShown" purpose="dynamicLabel" class="static-text-primary"></label>
                    </h3>
                </div>
                <div class="modal-body">
                    <!-- Country Add / Edit Rate Form -->
                    <div class="span7 form vertical-mode" id="RateRegions-form">
                        <fieldset>
                            <div class="row">
                              
                                <div class="row col-sm-12 form-group pull-in  has-error clearfix">
                                    <input id="hRateID" type="hidden" />
                                    <!-- start alert message Country -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message Country -->
                                    <div class="form-group col-sm-2">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                            <input id="txtCode" type="text" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" data-required="false" maxlength="3" placeholder="ex. Rate1" style="text-transform:uppercase" />
                                        </div>
                                        <div class="form-group col-sm-3">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                                            <input type="text" id="txtName" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" data-required="true" maxlength="50" placeholder="ex. Rate1" style="text-transform:uppercase" />
                                        </div>
                                        @*<div class="form-group col-sm-3">
                                            <label>City-From</label>
                                            <select id="slCityFrom" name="slCityFrom" onchange="GetRegionsInCity('From');" class="form-control m-b" onfocus="    DisableBackspaceKey(id);" onkeydown="    DisableBackspaceKey(id);" onkeypress="    DisableBackspaceKey(id);"></select>
                                        </div>
                                        <div class="form-group col-sm-3">
                                            <label>Region-From</label>
                                            <select id="slRegionFrom" name="slRegionFrom" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);"></select>
                                        </div>*@
                                    <div class="form-group col-sm-2">
                                        <label>From Date</label>
                                        <div>
                                            <input id="txtRateFromDate" tabindex="30" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="From Date">
                                        </div>
                                    </div>
                                    <div class="form-group col-sm-2">
                                        <label> To Date</label>
                                        <div>
                                            <input id="txtRateToDate" tabindex="30" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="To Date ">
                                        </div>
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Remarks</label>
                                        <textarea type="number" id="txtRemarks" class="form-control parsley-validated" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" data-required="false" placeholder="remarks"></textarea>
                                    </div>
                                </div>
                                <div class="row col-sm-12">
                                   
                                </div>
                                <div class="row col-sm-12 text-sm wrapper hidden-print ">
                                    <div id="div-main-optionsRateRegions" class="col-sm-3 m-b-xs">
                                        <button id="btn-NewAddRateRegions" class="btn btn-warning btn-sm" onclick="ClearRateRegions();" data-toggle="modal" @*data-target="#RateRegionsModal"*@><i class="fa fa-plus"></i>&nbsp;New</button>

                                    </div>
                                    <div id="div-search-option" class="col-sm-9">
                                        <div id="div-search-controls" class="input-group col-sm-9">
                                            <input id="txt-SearchRateRegions" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search region">
                                            <span class="input-group-btn">
                                                <button id="btn-searchRateRegions" class="btn btn-sm btn-primary" type="button" onclick="RateRegions_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                                            </span>
                                        </div>
                                        <div class="float-right col-sm-3">
                                            <button id="btn-DeleteRateRegions" onclick="DeleteRateRegions();" type="button" class="float-right btn btn-sm btn-danger m-r-n"><i class="fa fa-trash-o"></i>&nbsp;Delete</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-responsive">
                                    <table id="tblRateRegions" class="table table-striped b-t b-light text-sm  table-hover">
                                        <thead>
                                            <tr>
                                                <th id="ID">
                                                    <input id="cb-CheckAll" type="checkbox" />
                                                </th>
                                                <th>Region From</th>
                                                <th>Region To</th>
                                                <th>PackageTypeName</th>
                                                <th>Quantity</th>
                                                <th>Cost</th>
                                                <th>Selling</th>
                                 
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            <footer class="panel-footer">
                                <div class="row">
                                    <div class="col-sm-4 hidden-xs">
                                        <select id="select-page-size-Regions" class="input-sm form-control input-s-sm inline">
                                            <option value="10">10</option>
                                            <option value="25">25</option>
                                            <option value="50">50</option>
                                        </select>
                                        <button id="btn-applypaging-Regions" class="btn btn-sm btn-default" type="button">
                                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                                        </button>
                                    </div>
                                    <div id="div-Text-Total-Regions" class="col-sm-4 text-center">
                                        <small class="text-muted inline m-t-sm m-b-sm">
                                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                                            <span id="spn-first-page-row-Regions"></span>- <span id="spn-last-page-row-Regions"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                                            <span id="spn-total-count-Regions"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                                        </small>
                                    </div>
                                    <div id="div-Pager-Regions" class="col-sm-4 text-right text-center-xs float-right"></div>
                                </div>
                            </footer>
                            </div>
                        </fieldset>
                    </div>
                    <!-- Endegion Add / Edit Rate Form -->
                </div>
                <div class="modal-footer">
                    <!--sherif-->
                    <button type="button" id="btnSave" class="btn btn-primary btn-sm" onclick="Rates_Insert_Update(false);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
                    </button>
                    @*<button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm" onclick="Rates_Insert(true);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                    </button>*@
                    <button class="btnClose btn btn-link btn-sm" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Country Options & Template  -->
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.Rates  @*Rates*@
                </h3>
            </header>
            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm hide" onclick="ClearRateModal();" data-toggle="modal" data-target="#RateModal"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New</button>
                    
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-7">
                    <div id="div-search-controls" class="input-group col-sm-9">
                        <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase"  type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search Rate">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="Rates_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-1">
                        <button id="btn-Delete" type="button" class="float-right btn btn-sm btn-danger m-r-n hide"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                    </div>
                </div>
            </div>
            <div class="table-responsive">
                <table id="tblRates" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th class="th-sortable" data-toggle="class">@Forwarding.MvcApp.App_Resources.App_Resources.Code
                            </th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Name
                            </th>
                            <th>From Date
                            </th>
                            <th>To Date</th>
                            <th>Remarks</th>
                           @* <th class="rounded-right"></th>*@
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm"> @Forwarding.MvcApp.App_Resources.App_Resources.Showing 
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
        </section>
    </div>
</div>

