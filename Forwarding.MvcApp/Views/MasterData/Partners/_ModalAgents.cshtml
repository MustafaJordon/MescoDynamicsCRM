                                        <input type="text" id="txtAgentVATNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" data-minlength="0" maxlength="10" placeholder="Vat No." style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        @*<label>@Forwarding.MvcApp.App_Resources.App_Resources.BankName</label>*@
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.BankName</label>
                                        <input type="text" id="txtAgentBankName" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" maxlength="100" placeholder="ex. CIB" style="text-transform:uppercase"/>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.CommercialRegister</label>
                                        <input type="text" id="txtAgentIBANNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" maxlength="100" placeholder="ex. Commercial Register" style="text-transform:uppercase" />
                                    </div>                                        
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.PaymentTerm</label>
                                        <select id="slAgentPaymentTerms" name="slAgentPaymentTerms" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control m-b" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6 hide">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.BankAddress</label>
                                        <input type="text" id="txtAgentBankAddress" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="500" placeholder="ex. Address" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6 hide">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.InvoiceCurrency</label>
                                        <select id="slAgentCurrencies" name="slAgentCurrencies" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control m-b" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6 hide">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Swift</label>
                                        <input type="text" id="txtAgentSwift" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Swift Code" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6 hide">
