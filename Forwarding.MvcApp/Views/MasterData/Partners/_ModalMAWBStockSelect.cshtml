<!--MAWBStockSelectModal-->
    <!-- This modal is opened from Master Air Operations to select MAWB -->
    <div class="modal fade" id="MAWBStockSelectModal" tabindex="-1" style="z-index:2000;" data-keyboard="false" data-backdrop="false" role="dialog" aria-labelledby="MAWBStockSelectModalLabel" aria-hidden="true">
        <div class="modal-dialog modalwidth-50">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.MAWBStock &nbsp;<label id="lblMAWBStockSelectShown" purpose="dynamicLabel" class="static-text-primary"></label>
                        <button type="button" class="close" title="Close" data-dismiss="modal" onclick="MAWBStockSelect_ResetFunctionsNames();" aria-hidden="true">&times;</button>
                    </h3>
                </div>
                <!-- Region Add / Edit Region Form -->
                <div class="modal-body">
                    <div class="span7 form vertical-mode" id="MAWBStockSelect-form">
                        <fieldset>                                        
                            <div id="bodyMAWBStockSelect">
                                <div class="col-sm-2 m-l-n">
                                    <button id="btn-NewAddMAWBStockSelect" class="btn btn-warning btn-sm" data-toggle="modal" data-target="#MAWBStockModal" onclick="MAWBStockSelect_GenerateModal_ClearAllControls();"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.AddMAWBs</button>
                                </div>
                                <div class="col-sm-3 m-l">
                                    <input id="txtMAWBStockSelectSearch" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase; border-color:#cccccc;"  type="text" class="input-sm form-control" maxlength="8" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search">
                                </div>
                                <button id="btn-MAWBStockSelectSearch" class="btn btn-sm btn-primary m-l-n" type="button"><i class="fa fa-search"></i></button>
                                <button id="btn-DeleteMAWBStockSelect" type="button" class="float-right btn btn-sm btn-danger" onclick="MAWBStockSelect_Delete();"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                                <br /> <br />
                                <section class="panel panel-default" style="overflow: scroll; width: auto; height: auto;">
                                    <!--Region: table header-->
                                    <div class="table-responsive">
                                        <table id="tblMAWBStockSelect" class="table table-striped b-t b-light text-sm  table-hover">
                                            <thead>
                                                <tr>
                                                    <th id="HeaderDeletetblMAWBStockSelectID">
                                                        <input id="cb-CheckAll-MAWBStockSelect" type="checkbox" />
                                                    </th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.Number
                                                    </th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.InsertionDate
                                                    </th>
                                                    <th class="hidden">@Forwarding.MvcApp.App_Resources.App_Resources.Notes
                                                    </th>
                                                    <th class="rounded-right hide"></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                            </tbody>
                                        </table>
                                    </div>
                                    <!--Region: EOF table header-->
                                    <!--Region: table footer-->
                                    <footer class="panel-footer">

                                    </footer>
                                    <!--Region: EOF table footer-->
                                </section>
                                            
                                <div class="row">
                                    <input type="number" id="select-page-size" value="10" class="hide" />
                                    <div id="div-Text-Total-Modal" class="col-sm-4 text-center">
                                        <small class="text-muted inline m-t-sm m-b-sm"> @Forwarding.MvcApp.App_Resources.App_Resources.Showing 
                                            <span id="spn-first-page-row-Modal"></span>- <span id="spn-last-page-row-Modal"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                                            <span id="spn-total-count-Modal"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                                        </small>
                                    </div>
                                    <div id="div-Pager-Modal" class="col-sm-4 text-right text-center-xs "></div>
                                </div>
                    
                                @*<div id="divMAWBStockSelectSummary" class="form-group float-right m-r-lg"> <!--i need those Divs for not to start from a new line-->
                                    @Forwarding.MvcApp.App_Resources.App_Resources.NumberOfMAWBs &nbsp;<label id="lblTotalNumberOfMAWBs" purpose="dynamicLabel" class="static-text-blue"></label>
                                </div>*@
                            </div>
                        </fieldset>
                    </div>
                </div>
                <!-- Endregion Add / Edit Region Form -->
               <div class="modal-footer">
                    @*<button type="button" id="btnSave" class="btn btn-primary btn-sm" onclick="MAWBStockSelect_Insert(false);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save</button>
                    <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm" onclick="MAWBStockSelect_Insert(true);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew</button>*@
                    <button class="btnClose btn btn-link btn-sm" id="btnClose" title="Close" onclick="MAWBStockSelect_ResetFunctionsNames();" data-dismiss="modal" aria-hidden="true" >@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
                </div> 
            </div>
        </div>
    </div>
<!--EOF MAWBStockSelectModal-->