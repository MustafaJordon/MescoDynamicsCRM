<!--CustomersTemp Modal-->
<div class="modal fade" id="CustomerTempModal" tabindex="-1" style="z-index: 1045;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="CustomerTempModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-90">
        <div class="modal-content m-t-n-lg">
            <div class="modal-header">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.CustomerTemp &nbsp;<label id="lblCustomersTemphown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="CustomerTemp-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hCustomerTempID" type="hidden" />
                            <!--Wizard-->
                            <div class="wizard clearfix">
		                        <ul class="steps"> 
			                        <li data-target="#BasicDataCustomerTemp" id="stepsBasicDataCustomerTemp" class="active"><span class="badge badge-info">1</span>@Forwarding.MvcApp.App_Resources.App_Resources.BasicData</li> 
			                    </ul> 
		                        <div class="actions" style="display:none;"> <!--Next & Previous buttons-->
			                        <button type="button" class="btn btn-default btn-xs btn-prev" disabled="disabled">@Forwarding.MvcApp.App_Resources.App_Resources.Prev</button> 
			                        <button type="button" class="btn btn-default btn-xs btn-next" data-last="Finish">@Forwarding.MvcApp.App_Resources.App_Resources.Next</button> 
		                        </div> 
	                        </div> 
	                        <div class="step-content">
                                <!--Basic Data-->
		                        <div class="step-pane active" id="BasicDataCustomerTemp">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    <!-- Add / Edit Basic Data -->
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                        <input type="text" id="txtCustomerTempCode" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" data-minlength="1" maxlength="10" placeholder="Generated Automatically" disabled="disabled" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                                        <input type="text" id="txtCustomerTempName" disabled="disabled" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="true" maxlength="50" placeholder="ex. SEA WINGS USA" style="text-transform:uppercase"/>
                                    </div>                                        
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.LocalName</label>
                                        <input type="text" id="txtCustomerTempLocalName" disabled="disabled" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. SEA WINGS USA" style="text-transform:uppercase"/>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Address</label>
                                        <input type="text" id="txtCustomerTempAddress" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="250" placeholder="ex. Address" style="text-transform:uppercase" />
                                        @*<textarea id="txtCustomerTempAddress" maxlength="500" rows="3"  class="form-control col-sm-12 textDivOperations" style="text-transform:uppercase;"></textarea>*@
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>Phones & Faxes</label>
                                        <input id="txtCustomerTempPhonesAndFaxes" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="250" placeholder="Phones & Faxes" style="text-transform:uppercase"/>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Email</label>
                                        <input id="txtCustomerTempEmail" name="Email" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="250" placeholder="Email" style="text-transform:uppercase"/>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                        <input type="text" id="txtCustomerTempNotes" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="250" placeholder="ex. Notes" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.VATNumber</label>
                                        <input type="text" id="txtCustomerTempVATNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" data-minlength="0" maxlength="10" placeholder="Vat No." style="text-transform:uppercase" />
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.CommercialRegister</label>
                                        <input type="text" id="txtCustomerTempIBANNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" maxlength="100" placeholder="ex. Commercial Register" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6 m-t">
                                        <label><input type="checkbox" id="cbCustomerTempIsInactive" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.IsInactive</label><br />
                                    </div>
                                                
                                    <!--EOF Add / Edit Basic Data -->
		                        </div> 
                                <!--EOF Basic Data-->


	                        </div>
                            <!--EOF Wizard-->
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer m-t-n">
                <button type="button" id="btnSaveCustomerTemp" class="btn btn-primary btn-sm">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save</button>
                <button type="button" id="btnSaveandNewCustomerTemp" class="btn btn-primary btn-sm">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew</button>
                <button class="btnClose btn btn-link btn-sm" id="btnCustomerTempClose" @*onclick="CustomersTemp_UnlockRecord();"*@ data-dismiss="modal" aria-hidden="true" >@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div> 
        </div>
    </div>
</div>
<!--EOF CustomersTemp Modal-->
