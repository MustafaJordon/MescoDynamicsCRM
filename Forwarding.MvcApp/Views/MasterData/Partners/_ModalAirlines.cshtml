                                                        <span id="spn-first-page-row-Modal"></span>- <span id="spn-last-page-row-Modal"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                                                        <span id="spn-total-count-Modal"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                                                    </small>
                                                </div>
                                                <div id="div-Pager-Modal" class="col-sm-4 text-right text-center-xs "></div>
                                            </div>
                    
                                            @*<div id="divMAWBStockSummary" class="form-group float-right m-r-lg"> <!--i need those Divs for not to start from a new line-->
                                                @Forwarding.MvcApp.App_Resources.App_Resources.NumberOfMAWBs &nbsp;<label id="lblTotalNumberOfMAWBs" purpose="dynamicLabel" class="static-text-blue"></label>
                                            </div>*@
                                        </div>
		                            </div> 
                                <!--EOF MAWBStock-->
                                
                                <!--Payables-->
                                <div class="step-pane" id="tabPayables">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    <!--The id of the next div is used to fill the MAWBStock -->
                                    <div id="divAirPayablesBtns" class="col-sm-12" style="clear:both;">
                                        <button id="btn-AddAirLinePayables" class="btn btn-warning btn-sm m-l-n" data-toggle="modal" data-target="#SelectChargesModal" onclick="PayablesAirLine_GetAvailableCharges();"><i class="fa fa-plus"></i>&nbsp;Add Payables</button>
                                        <button id="btn-DeleteAirLinePayable" type="button" class="float-right btn btn-sm btn-danger m-t-n-xs " onclick="AirLinePayables_DeleteList();"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
