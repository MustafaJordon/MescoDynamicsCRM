                                            <div class="table-responsive" style="width:100%; height:100%; overflow:scroll;">
                                                <table id="tblAirLinePayables" class="table table-striped b-t b-light text-sm  table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th id="HeaderDeleteAWBPayableID">
                                                                <input id="cb-CheckAll-Payables" type="checkbox" />
                                                            </th>
                                                            <th>
                                                                @Forwarding.MvcApp.App_Resources.App_Resources.ChargeType
                                                            </th>
                                                            <th>
                                                                @Forwarding.MvcApp.App_Resources.App_Resources.Defaults
                                                            </th>
                                                            <th class="rounded-right hide"></th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                            </div>
                                            @*<footer class="panel-footer"></footer>*@
                                            <!--Region: EOF table footer-->
                                        </section>

