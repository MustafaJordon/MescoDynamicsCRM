<!--Suppliers Modal-->
<div class="modal fade" id="SupplierModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="SupplierModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-90 m-t-n-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.Supplier &nbsp;<label id="lblSupplierShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="Supplier-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hSupplierID" type="hidden" />
                            <!--Wizard-->
                            <div class="wizard clearfix">
                                <ul class="steps">
                                    <li data-target="#BasicDataSupplier" id="stepsBasicDataSupplier" class="active"><span class="badge badge-info">1</span>@Forwarding.MvcApp.App_Resources.App_Resources.BasicData</li>
                                    <li data-target="#BillingSupplier" id="stepsBillingSupplier" class="complete"><span class="badge badge-info">2</span>@Forwarding.MvcApp.App_Resources.App_Resources.Billing</li>
                                    <li data-target="#tabAddressesSupplier" id="stepsAddressesSupplier" class="complete"><span class="badge badge-info">3</span>@Forwarding.MvcApp.App_Resources.App_Resources.Addresses</li>
                                    <li data-target="#tabContactsSupplier" id="stepsContactsSupplier" class="complete"><span class="badge badge-info">4</span>@Forwarding.MvcApp.App_Resources.App_Resources.Contacts</li>
                                    <li data-target="#tabPartnersBanks" id="stepsPartnersBanksSupplier" class="complete hide"><span class="badge badge-info">5</span>@Forwarding.MvcApp.App_Resources.App_Resources.Banks</li>

                                    <li data-target="#tabAccounting" id="stepsAccounting" class="complete hide classAccountingOption"><span class="badge badge-info">$</span>@Forwarding.MvcApp.App_Resources.App_Resources.Accounting</li>
                                    @*------------*@
                                    <li data-target="#tabUploadFiles" id="stepsUploadFilesSupplier" class="complete hide classHideOutsidePartners"><span class="badge badge-info">↑</span>Upload</li>
                                    @*------------*@
                                    <li data-target="#tabSL_SalesMan" id="stepsSL_SalesMan" class="complete hide classSL_SalesManOption"><span class="badge badge-info">$</span>@Forwarding.MvcApp.App_Resources.App_Resources.SL_SalesMan</li>


                                </ul> 
		                        <div class="actions" style="display:none;"> <!--Next & Previous buttons-->
			                        <button type="button" class="btn btn-default btn-xs btn-prev" disabled="disabled">@Forwarding.MvcApp.App_Resources.App_Resources.Prev</button> 
			                        <button type="button" class="btn btn-default btn-xs btn-next" data-last="Finish">@Forwarding.MvcApp.App_Resources.App_Resources.Next</button> 
		                        </div> 
	                        </div> 
                            <div class="step-content">
                                <!--Basic Data-->
                                <div class="step-pane active" id="BasicDataSupplier">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    <!-- Add / Edit Basic Data -->
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                        <input type="text" id="txtSupplierCode" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" data-minlength="1" maxlength="10" placeholder="Generated Automatically" disabled="disabled" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                                        <input type="text" id="txtSupplierName" disabled="disabled" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="true" maxlength="50" placeholder="ex. SEA WINGS USA" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.LocalName</label>
                                        <input type="text" id="txtSupplierLocalName" disabled="disabled" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. SEA WINGS USA" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Website</label>
                                        <input type="text" id="txtSupplierWebsite" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" name="Website" data-required="false" data-minlength="0" maxlength="150" placeholder="ex. www.SEAWINGSUSA.com" onchange="Suppliers_SetWebSiteHref();" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                        <input type="text" id="txtSupplierNotes" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Notes" style="text-transform:uppercase" />
                                    </div>

                                    <div class="form-group col-sm-6 hide classShowForGBL" >
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Sites</label>
                                        <input type="text" id="txtSites" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Sites" style="text-transform:uppercase" />
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <a id="btnSupplierVisitWebsite" class="btn btn-info btn-sm m-t-25" target="_blank">
                                            <span class="fa fa-hand-o-right"></span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.VisitWebsite
                                        </a>
                                    </div>
                                    <div class="form-group col-sm-6 m-t">
                                        <label><input type="checkbox" id="cbSupplierIsInactive" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.IsInactive</label><br />
                                    </div>

                                    <!--EOF Add / Edit Basic Data -->
                                </div>
                                <!--EOF Basic Data-->
                                <!--Billing-->
                                <div class="step-pane" id="BillingSupplier">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    <!-- Add / Edit Billing -->
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.VATNumber</label>
                                        <input type="text" id="txtSupplierVATNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" data-minlength="0" maxlength="10" placeholder="Vat No." style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.TaxID</label>
                                        <input type="text" id="txtSupplierSwift" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" maxlength="50" placeholder="Tax ID" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.CommercialRegister</label>
                                        <input type="text" id="txtSupplierIBANNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" maxlength="100" placeholder="Commercial Registration" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6 ">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.BankName</label>
                                        <input type="text" id="txtSupplierBankName" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="ex. CIB" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.BankAccountNumber</label>
                                        <input type="text" id="txtSupplierBankAccountNumber" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="ex. Account No." style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.BankAddress</label>
                                        <input type="text" id="txtSupplierBankAddress" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="500" placeholder="ex. Address" style="text-transform:uppercase" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.PaymentTerm</label>
                                        <select id="slSupplierPaymentTerms" name="slSupplierPaymentTerms" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control m-b" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.InvoiceCurrency</label>
                                        <select id="slSupplierCurrencies" name="slSupplierCurrencies" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control m-b" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.TaxeType</label>
                                        <select id="slSupplierTaxeTypes" name="slSupplierTaxeTypes" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control m-b" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6 m-t-27">
                                        <label><input type="checkbox" id="cbSupplierIsConsolidatedInvoice" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TaxExempted</label><br />
                                    </div>
                                    <!--EOF Add / Edit Billing -->
                                </div>
                                <!--EOF Billing-->
                                <!--Addresses-->
                                <div class="step-pane" id="tabAddressesSupplier">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    @* The next line is to call the addresses modal from the new button *@
                                    <button id="btn-NewAddAddress" class="m-l btn btn-warning btn-sm" data-toggle="modal" data-target="#AddressModal" onclick="Addresses_ClearAllControls();"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.NewAddress</button>
                                    <br /><br />
                                    <!--The id of the next div is used to fill the addresses in the textareas-->
                                    <div id="bodySupplierAddresses"></div>
                                </div>
                                <!--EOFAddresses-->
                                <!--Contacts-->
                                <div class="step-pane" id="tabContactsSupplier">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    @* The next line is to call the Contacts modal from the new button *@
                                    <button id="btn-NewAddContact" class="m-l btn btn-warning btn-sm" data-toggle="modal" data-target="#ContactModal" onclick="Contacts_ClearAllControls();"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.NewContact</button>
                                    <br /><br />
                                    <!--The id of the next div is used to fill the Contacts in the textareas-->
                                    <div id="bodySupplierContacts"></div>
                                </div>
                                <!--EOF Contacts-->
                                <!--Banks-->
                                <div class="step-pane" id="tabPartnersBanks">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    @* The next line is to call the addresses modal from the new button *@
                                    <button id="btn-NewAddBanks" class="m-l btn btn-warning btn-sm" data-toggle="modal" data-target="#PartnersBanksModal" onclick="PartnersBanks_ClearAllControls();"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New  Banks</button>
                                    <br /><br />
                                    <!--The id of the next div is used to fill the addresses in the textareas-->
                                    <div id="bodySupplierPartnersBanks"></div>
                                </div>
                                <!--EOFBanks-->
                                <!--Accounting-->
                                <div class="step-pane" id="tabAccounting">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.SupplierGroup</label>
                                        <select id="slSupplierSubAccountGroup" class="form-control m-b" onchange="FillSlAccountFromGroup('slSupplierAccount', 'slSupplierSubAccountGroup', 'slSupplierSubAccount', 0, 0);" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Account</label>
                                        <select id="slSupplierAccount" class="form-control m-b" @*onchange="FillSlSubAccount('slSupplierSubAccount', 'slSupplierAccount', 0, 0);"*@ onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.SubAccount</label>
                                        <select id="slSupplierSubAccount" class="form-control m-b" disabled="disabled" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.CostCenter</label>
                                        <select id="slSupplierCostCenter" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                                    </div>
                                    <!--The id of the next div is used to fill the Accounting in the textareas-->
                                    <div id="bodySupplierAccounting"></div>
                                </div>
                                <!--Accounting-->
                                <!--UploadFiles-->
                                <div class="step-pane" id="tabUploadFiles">
                                    <!--Upload Region Controls-->
                                    <input id="inputFileUpload_Suppliers" type="file" multiple="multiple" class="btn-primary btn-sm " />
                                    <div id="divUpload" class="m-t">
                                        <input id="btnUploadFile" onclick="GeneralUpload_UploadFile();" class="btn btn-warning btn-sm" type="button" value="Upload Files" />
                                        <progress></progress>
                                    </div>
                                    <button id="btn-DeleteDocsIn" type="button" class="float-right btn btn-sm btn-danger m-t-n-lg swapChildrenClass" onclick="GeneralUpload_DeleteList();"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>

                                    <!--End Upload Region Controls-->

                                    <section class="panel panel-default m-t">
                                        <!--Region: table header-->
                                        <div class="table-responsive">
                                            <table id="tblUploadedFiles_Suppliers" class="table table-striped b-t b-light text-sm  table-hover">
                                                <thead>
                                                    <tr class="swapChildrenClass">
                                                        <th id="HeaderDeleteUploadedFiles_Suppliers" style="width:5%;">
                                                            <input id="cb-CheckAll-UploadedFiles_Suppliers" type="checkbox" />
                                                        </th>
                                                        <th>
                                                            @Forwarding.MvcApp.App_Resources.App_Resources.Serial
                                                        </th>
                                                        <th>
                                                            @Forwarding.MvcApp.App_Resources.App_Resources.FileName
                                                        </th>
                                                        <th class="rounded-right"></th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                        <!--Region: table footer-->
                                        <footer class="panel-footer"></footer>
                                        <!--Region: EOF table footer-->
                                        <!--Region: EOF table header-->
                                    </section>
                                </div>
                                <div class="step-pane" id="tabSL_SalesMan">
                                    <!-- start alert message region -->
                                    <div class="alert alert-danger div-error">
                                        @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                    </div>
                                    <!-- end alert message region -->
                                    <!--The id of the next div is used to fill the AgentNetwork -->
                                    <div id="bodySupplierNetwork">
                                        <div class="col-sm-2 m-l-n">
                                            <button id="btn-NewAddSupplierSL_SalesMan" class="btn btn-warning btn-sm" data-toggle="modal" data-target="#SupplierSL_SalesManModal" onclick="SupplierSL_SalesMan_ClearAllControls();"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Add</button>
                                        </div>
                                        <div class="col-sm-3 m-l">
                                            <input id="txtSupplierSL_SalesManSearch" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase; border-color:#cccccc;" type="text" class="input-sm form-control" maxlength="8" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search SalesMan">
                                        </div>
                                        <button id="btn-SupplierSL_SalesManSearch" class="btn btn-sm btn-primary m-l-n" type="button" onclick="SupplierSL_SalesMan_LoadingWithPagingForModal();"><i class="fa fa-search"></i></button>
                                        <button id="btn-DeleteSupplierSL_SalesMan" type="button" class="float-right btn btn-sm btn-danger" onclick="SupplierSL_SalesMan_Delete();"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                                        <br /> <br />
                                        <section class="panel panel-default" style="overflow: scroll; width: auto; height: 330px;">
                                            <!--Region: table header-->
                                            <div class="table-responsive">
                                                <table id="tblSupplierSL_SalesMan" class="table table-striped b-t b-light text-sm  table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th style="width:5%;" id="HeaderDeletetblSupplierSL_SalesManID">
                                                                <input id="cb-CheckAll-SupplierNetwork" type="checkbox" />
                                                            </th>
                                                            <th>
                                                                @Forwarding.MvcApp.App_Resources.App_Resources.SL_SalesMan
                                                            </th>
                                                            <th>
                                                                @Forwarding.MvcApp.App_Resources.App_Resources.Percentage
                                                            </th>
                                                            <th>
                                                                @Forwarding.MvcApp.App_Resources.App_Resources.IsDefault
                                                            </th>
                                                            <th class="rounded-right hide"></th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                            </div>
                                            <!--Region: EOF table header-->
                                            <!--Region: table footer-->
                                            <footer class="panel-footer"></footer>
                                            <!--Region: EOF table footer-->
                                        </section>

                                        <div class="row">
                                            <div id="div-Text-Total-Modal" class="col-sm-4 text-center">
                                                <small class="text-muted inline">
                                                    @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                                                    <span id="spn-first-page-row-Modal"></span>- <span id="spn-last-page-row-Modal"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                                                    <span id="spn-total-count-Modal"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                                                </small>
                                            </div>
                                            <div id="div-Pager-Modal" class="col-sm-4 text-right text-center-xs"></div>
                                        </div>

                                        @*<div id="divAgentNetworkSummary" class="form-group float-right m-r-lg"> <!--i need those Divs for not to start from a new line-->
                    @Forwarding.MvcApp.App_Resources.App_Resources.NumberOfMAWBs &nbsp;<label id="lblTotalNumberOfMAWBs" purpose="dynamicLabel" class="static-text-blue"></label>
                </div>*@
                                    </div>
                                </div>
                                <!--EOFUploadFiles-->

                            </div>
                            <!--EOF Wizard-->
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer">
                <button type="button" id="btnSaveSupplier" class="btn btn-primary btn-sm">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save</button>
                <button type="button" id="btnSaveandNewSupplier" class="btn btn-primary btn-sm">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew</button>
                <button class="btnClose btn btn-link btn-sm" id="btnSupplierClose" @*onclick="Suppliers_UnlockRecord();"*@ data-dismiss="modal" aria-hidden="true" >@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div> 
        </div>
    </div>
</div>
<!--EOF Suppliers Modal-->

<div class="modal fade" id="SupplierSL_SalesManModal" tabindex="-1" style="z-index: 1050;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="SupplierSL_SalesManModalLabel"
     aria-hidden="true">
    <div class="modal-dialog modalwidth-70">
        <div class="modal-content">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;</button>*@
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.SL_SalesMan &nbsp;<label id="lblSupplierSL_SalesManShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <div class="modal-body">
                <!-- Region Add / Edit Country Form -->
                <div class="span7 form vertical-mode" id="SupplierSL_SalesMan-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hSupplierSL_SalesManID" type="hidden" />
                            <!-- start alert message region -->
                            <div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>
                            <!-- end alert message region -->

                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.SL_SalesMan</label>
                                <select id="slSupplierSL_SalesMan" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control " data-required="true"></select>
                            </div>

                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Percentage</label>
                                <input type="text" id="txtSupplierPercentage" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="250" placeholder="ex.12" style="text-transform:uppercase" />
                            </div>
                            <div class="form-group col-sm-3 m-t">
                                <label><input type="checkbox" id="cbIsDefault" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.IsDefault</label><br />
                            </div>
                        </div>
                    </fieldset>
                </div>
                <!-- Endegion Add / Edit AgentNetwork Form -->
            </div>
            <div class="modal-footer">
                <!--sherif-->
                <button type="button" id="btnSaveSupplierSL_SalesMan" class="btn btn-primary btn-sm" onclick="">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
                </button>
                <button type="button" id="btnSaveandNewSupplierSL_SalesMan" class="btn btn-primary btn-sm" onclick="SupplierSL_SalesMan_Insert(true);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                </button>
