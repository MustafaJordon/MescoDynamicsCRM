<!--RateModal-->
<div class="modal fade" id="RateModal" tabindex="-1" style="z-index:3000;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="RateModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-40">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.Rate &nbsp;<label id="lblRateShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <div class="modal-body">
                <!-- Region Add / Edit Rate Form -->
                <div class="span7 form vertical-mode" id="Rate-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hCustomerRateID" type="hidden" />
                            <!-- start alert message region -->
                            <div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>
                            <!-- end alert message region -->
                            <div class="form-group col-sm-12">
                                <div class="form-group col-sm-8">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Rate</label>
                                    <select id="slRates" name="slRates" class="form-control m-b" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);"></select>
                                </div>
                             
                                <div class="form-group col-sm-4">
                                    <label> Is InActive</label>
                                    <div>
                                        <input id="RateIsInActive" type="checkbox">
                                    </div>
                                </div>
                        </div>
                        </div>
                    </fieldset>
                </div>
               
            </div>
                <!-- Endegion Add / Edit Country Form -->
            <div class="modal-footer">
                <!--sherif-->

                <button type="button" id="btnRateSave" class="btn btn-primary btn-sm" onclick="Rates_Insert(false);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
                </button>

                <button class=" btn btn-link btn-sm" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button>
            </div>    
        </div>
    </div>
</div>
<!--EOF RateModal-->
