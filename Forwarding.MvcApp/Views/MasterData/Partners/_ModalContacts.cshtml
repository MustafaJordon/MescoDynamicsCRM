<!--ContactModal-->
<div class="modal fade" id="ContactModal" tabindex="-1" style="z-index: 3000;" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="ContactModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-70">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.Contact &nbsp;<label id="lblContactShown" purpose="dynamicLabel" class="static-text-primary"></label>
            </h3>
        </div>
        <div class="modal-body">
            <!-- Region Add / Edit Contact Form -->
            <div class="span7 form vertical-mode" id="Contact-form">
                <fieldset>
                    <div class="form-group pull-in  has-error clearfix">
                        <input id="hContactID" type="hidden" />
                        <!-- start alert message region -->
                        <div class="alert alert-danger div-error">
                            @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                        </div>
                        <!-- end alert message region -->
                        <div class="form-group col-sm-6">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                            <input id="txtContactName" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="true" maxlength="100" placeholder="Name" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-6">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.LocalName</label>
                            <input id="txtContactLocalName" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="Local Name" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-4">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Phone1</label>
                            <input id="txtPhone1" onkeypress="DisableEnterKey(id);" onfocus="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="16" placeholder="Phone 1" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-4">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Phone2</label>
                            <input id="txtPhone2" onkeypress="DisableEnterKey(id);" onfocus="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="16" placeholder="Phone 2" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-4">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Mobile1</label>
                            <input id="txtMobile1" onkeypress="DisableEnterKey(id);" onfocus="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="16" placeholder="Mobile 1" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-4" style="display:none;">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Mobile2</label>
                            <input id="txtMobile2" onkeypress="DisableEnterKey(id);" onfocus="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="16" placeholder="Mobile 2" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-4">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Fax</label>
                            <input id="txtFax" onkeypress="DisableEnterKey(id);" onfocus="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="16" placeholder="Fax" style="text-transform:uppercase" />
                        </div>
                        <div class="form-group col-sm-8">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Email</label>
                            <input id="txtEmail" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" name="Email" data-required="false" maxlength="250" placeholder="Email" style="text-transform:uppercase" />
                        </div>
                        @*<div class="form-group col-sm-12" style="display:none;">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                <input type="text" id="txtContactNotes" class="form-control parsley-validated" data-required="false" maxlength="50" placeholder="ex. Notes...." style="text-transform:uppercase" />
                            </div>
                            <div class="form-group col-sm-12" style="display:none;">
                                <label style="margin-top: 5px;"><input type="checkbox" id="cbIsInactive">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.IsInactive</label>
                            </div>*@
                    </div>
                </fieldset>
            </div>
            <!-- Endegion Add / Edit Country Form -->
        </div>
        <div class="modal-footer">
            <!--sherif-->
            <button type="button" id="btnContactSave" class="btn btn-primary btn-sm" onclick="Contacts_Insert(false);">
                <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
            </button>
            <button type="button" id="btnContactSaveandNew" class="btn btn-primary btn-sm" onclick="Contacts_Insert(true);">
                <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
            </button>
            <button class=" btn btn-link btn-sm" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button>
        </div>
    </div>
</div>
</div>
<!--EOF ContactModal-->
