<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="Administration" href="javascript:void(0);" onclick="LoadViews('Settings')">
            @Forwarding.MvcApp.App_Resources.App_Resources.Administration
    </a>
</li>
<li>
    <a id="Settings" href="javascript:void(0);" onclick="LoadViews(id)">
        @Forwarding.MvcApp.App_Resources.App_Resources.Settings
</a>
</li>
<li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.FA_Devisons</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-FA_Devisons">
    <!-- Add / Edit Region Modal -->
    <div class="modal fade" id="FA_DevisonsModal" tabindex="-1" data-keyboard="false" data-backdrop="false" role="dialog" aria-labelledby="FA_DevisonsModalLabel" aria-hidden="true">
        <div class="modal-dialog modalwidth-40">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="m-b-none">
                        @Forwarding.MvcApp.App_Resources.App_Resources.FA_Devisons &nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="FA_Devisons-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hID" type="hidden" />
                            <!-- start alert message region -->
                            <div class="alert alert-danger div-error">
                                @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                            </div>
                            <!-- end alert message region -->
                            <!-- Region Add / Edit Region Form -->

                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                <input type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" id="txtCode" class="form-control parsley-validated" data-required="true" maxlength="50" placeholder="Name" style="text-transform:uppercase" />
                            </div>
                            <div class="form-group col-sm-6">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Name</label>
                                <input type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" id="txtName" class="form-control parsley-validated" data-required="true" maxlength="50" placeholder="Name" style="text-transform:uppercase" />
                            </div>
                            <!-- Region Add / Edit Region Form -->
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer">
                <a id="btnSave" class="btn btn-primary btn-sm" onclick="FA_Devisons_Insert(false);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
                </a>
                <a id="btnSaveandNew" class="btn btn-primary btn-sm" onclick="FA_Devisons_Insert(true);">
                    <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                </a>
                <button class="btnClose btn btn-link btn-sm" id="btnClose" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div>
        </div>
    </div>
</div>
<!-- Region Options & Template  -->

<div id="div-Data">
    <div id="div-Options">
        <div id="div-Action" class="radius-top-left-right">
        </div>
    </div>
    <section class="panel panel-default">
        <header class="panel-heading">
            <h3 class="m-b-none">
                @Forwarding.MvcApp.App_Resources.App_Resources.FA_Devisons
        </h3>
    </header>
    <!--Region the searh, delete, addnew-->
    <div class="row text-sm wrapper">
        <div id="div-main-options" class="col-sm-5 m-b-xs">
            <button id="btn-NewAdd" class="btn btn-sm btn-warning hide" data-toggle="modal" data-target="#FA_DevisonsModal"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New</button>
            <div id="div-option-buttons" class="btn-group hide">
                <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
            </div>
        </div>
        <div id="div-search-option" class="col-sm-7">
            <div id="div-search-controls" class="input-group col-sm-9">
                <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.SearchName">
                <span class="input-group-btn">
                    <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="FA_Devisons_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                </span>
            </div>
            <div class="float-right col-sm-1">
                <button id="btn-Delete" type="button" class="float-right btn btn-sm btn-danger m-r-n hide"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                @*<button id="btn-NewAdd" class="float-right btn btn-warning btn-sm hide" data-toggle="modal" data-target="#FA_DevisonsModal"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New</button>*@
            </div>
        </div>
    </div>
    <!--Region the searh, delete, addnew-->
    <!--Region: table header-->
    <div class="table-responsive">
        <table id="tblFA_Devisons" class="table table-striped b-t b-light text-sm  table-hover">
            <thead>
                <tr>
                    <th id="ID">
                        <input id="cb-CheckAll" type="checkbox" />
                    </th>
                    <th>
                        @Forwarding.MvcApp.App_Resources.App_Resources.Name
                    </th>
                    <th>
                        @Forwarding.MvcApp.App_Resources.App_Resources.Code
                    </th>
                    <th class="rounded-right hide"></th>
                </tr>
    </thead>
    <tbody></tbody>
</table>
</div>
<!--Region: table header-->
<!--Region: footer-->
<footer class="panel-footer">
    <div class="row">
        <div class="col-sm-4 hidden-xs">
            <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                <option value="10">10</option>
                <option value="25">25</option>
                <option value="50">50</option>
            </select>
            <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                @Forwarding.MvcApp.App_Resources.App_Resources.Apply
            </button>
        </div>
        <div id="div-Text-Total" class="col-sm-4 text-center">
            <small class="text-muted inline m-t-sm m-b-sm">
                @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
            </small>
        </div>
        <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
    </div>
</footer>
<!--Region: footer-->
</section>
</div>
</div>
