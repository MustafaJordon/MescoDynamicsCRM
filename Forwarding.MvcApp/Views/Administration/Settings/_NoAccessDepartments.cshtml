<div class="modal fade" id="CheckboxesListModal" tabindex="-1" @*style="z-index: 3000;"*@ data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="CheckboxesListModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-90">
        <div class="modal-content m-t-n-lg">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true" title="Close">
                    &times;
                </button>
                <h3 class="m-b-none">
                    Select Charges For <label id="lblShownDeparmentCharge" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <div class="modal-body">
                <!-- Region CheckboxesList Form -->
                <button id="btn-CheckAllDepartmentCharge" type="button" class="btn btn-lightblue btn-sm m-l m-t-xs" onclick="DepartmentCharge_SetAllCharges(true);"><i class="fa "></i><b>&nbsp;Check All</b></button>
                <button id="btn-ClearAllDepartmentCharge" type="button" class="btn btn-warning btn-sm m-l m-t-xs" onclick="DepartmentCharge_SetAllCharges(false);"><i class="fa "></i><b>&nbsp;Clear All</b></button>
                <div class="col-sm-3 float-right hide">
                    <div id="div-search-controls" class="input-group m-t-xs">
                        <input id="txtSearchItems" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search" />
                        <span class="input-group-btn">
                            <button id="btn-SearchItems" class="btn btn-sm btn-primary" type="button" onclick="DepartmentCharge_FilterDepartmentChargeModal();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                </div>


                <div style="clear:both;" class="span7 form vertical-mode m-t-sm" id="CheckboxesList-form">
                    <fieldset>

                        <div id="divCheckboxesList">
                        </div>
                    </fieldset>
                </div>
                <!-- Endegion CheckboxesList Form -->
            </div>
            <div class="modal-footer m-t-n">
                <!--sherif-->
                <button id="btn-print-FromSelectOperations" type="button" class="btn btn-lightblue btn-sm hide" onclick="DepartmentCharge_Print('Print');"><i class="fa fa-print"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print</b></button>
