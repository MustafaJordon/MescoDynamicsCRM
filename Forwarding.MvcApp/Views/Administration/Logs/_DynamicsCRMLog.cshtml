<div id="div-CustomersReport">
    <!-- Region Options & Template  -->
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <div id="hExportedTable" class="hide"></div>
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.FilterBy</h3>
                @*<h3 class="m-b-none">Best Print Using Internet Explorer</h3>*@
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-l">
                <div id="div-main-options">
                    <!--Region Filter-->
                    @*<div class="form-group col-sm-3 m-l-n">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Branch</label>
                        <select id="slBranch" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>
                    <div class="form-group col-sm-3 classShowForUnSalemen m-l-n">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Salesman</label>
                        <select id="slSalesman" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>
                    <div class="form-group col-sm-3 m-l-n">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.CreatedBy</label>
                        <select id="slCreator" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>
                    <div class="form-group col-sm-3 m-l-n">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</label>
                        <select id="slCustomer" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>
                    <div class="form-group col-sm-3 m-l-n">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Agent</label>
                        <select id="slAgent" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>*@
                    
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                        <input id="txtFromOpenDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                        <input id="txtToOpenDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="form-group col-sm-2">
                        <label>Range</label>
                        <select id="slRange" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm">
                            <option value="5"><--Top 5--></option>
                            <option value="10"><--Top 10--></option>
                            <option value="20"><--Top 20--></option>
                            <option value="50"><--Top 50--></option>
                            <option value="100"><--Top 100--></option>
                            <option value="999999"><--All--></option>
                        </select>
                    </div>
                    @*<div class="col-sm-3 m-l-n-sm m-t-sm">
                            <br />
                            <label> <input type="checkbox" id="cbIncludeCurrenciesDetails" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"> Show Currencies Details</label>
                            <br /><br />
                        </div>*@

                        <div class="col-sm-2 m-l-n-sm m-t-sm hide">
                            <label> <input type="checkbox" id="cbHideExpiredQuotations" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"> Hide Expired Quotations</label>
                        </div>
                        
                        <div class="form-group col-sm-3" style="clear:both;">
                            <button id="btn-print" type="button" class="btn btn-lightblue btn-sm" onclick="CustomersReport_Print('Print');"><i class="fa fa-print"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print</button>
                            <button id="btn-ExportToExcel" type="button" onclick="CustomersReport_Print('Excel');" class="btn btn-sm btn-primary"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>
                        </div>
                        <div class="form-group col-sm-3 hide">
                            <div class="input-group">
                                <input type="text" id="txtToEmail" placeholder="Send To" class="form-control input-sm">
                                <span id="btn-SendEmail" title="Send PDF Report as Email" class="input-group-addon" onclick="QuotationsStatistics_Print('Email');" style="background-color:green;padding:5px!important;color:white;cursor:pointer;"><span class="fa fa-envelope">&nbsp; @Forwarding.MvcApp.App_Resources.App_Resources.Send</span>&nbsp;</span>
                            </div>
                        </div>

                        <!--EOF Region Filter-->
                </div>
            </div>
            <!--EOF Region the searh, delete, addnew line-->


        </section>
    </div>
</div>
