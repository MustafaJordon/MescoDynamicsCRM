<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="Administration" href="javascript:void(0);" onclick="LoadViews('Logs')">@Forwarding.MvcApp.App_Resources.App_Resources.Administration
    </a></li>
    <li><a id="Logs" href="javascript:void(0);" onclick="LoadViews('Logs')">@Forwarding.MvcApp.App_Resources.App_Resources.Logs
    </a></li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.HousesLogs</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-ProfitStatistics">
    <!-- Region Options & Template  -->
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">

                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.FilterBy</h3>
                @*<h3 class="m-b-none">Best Print Using Internet Explorer</h3>*@
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-t-n-sm">
                <div class="swapChildrenClass">
                    @*div-main-options*@
                    <!--Region Filter-->

                    <div class="form-group col-sm-2 ">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</label>
                        <select id="slMasterOperations" class="input-sm form-control CanEdit HasSearch" onchange=""></select>
                    </div>
                    <div class="form-group col-sm-2 hide">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                        <input id="txtFromOpenDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="form-group col-sm-2 hide">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                        <input id="txtToOpenDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="btn-group col-sm-1 m-t-md" data-toggle="buttons">
                        <button id="btn-LoadHouses" type="button" class="btn btn-lightblue btn-sm" onclick="HousesLogs_LoadHouses();">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.LoadHouses</button>
                    </div>


                    <!--EOF Region Filter-->
                </div>
            </div>
            
            <!--EOF Region the searh, delete, addnew line-->


        </section>
    </div>
</div>


<div id="divTblOperations" class="table-responsive m-t-sm" style="overflow: scroll;">
    <table id="tblOperations" class="table  b-t b-light text-sm  table-hover" style="font-size:85%;">
        <thead>

            <tr>
                <th></th>
                <th>Code</th>
                <th>MasterBL</th>
                <th>HouseNo</th>
                <th>Client</th>
                <th>Route</th>
                <th>Shipment</th>
                <th>GrossWt</th>
                <th>Invoices</th>
                <th class="rounded-right"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>