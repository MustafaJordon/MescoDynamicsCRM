<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li><a id="Administration" href="javascript:void(0);" onclick="LoadViews('Logs')">@Forwarding.MvcApp.App_Resources.App_Resources.Administration
    </a></li>
    <li><a id="Logs" href="javascript:void(0);" onclick="LoadViews('Logs')">@Forwarding.MvcApp.App_Resources.App_Resources.Logs
    </a></li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.OperationsChargesLogs</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-ProfitStatistics">
    <!-- Region Options & Template  -->
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">

                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.FilterBy</h3>
                @*<h3 class="m-b-none">Best Print Using Internet Explorer</h3>*@
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-t-n-sm">
                <div class="swapChildrenClass">
                    @*div-main-options*@
                    <!--Region Filter-->

                    <div class="form-group col-sm-3">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.LogFor</label>
                        <select id="slLogFor" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm">
                            <option value="">Payables & Receivables</option>
                            <option value="10">Only Payables</option>
                            <option value="20">Only Receivables</option>
                        </select>
                    </div>
                    <div class="form-group col-sm-2 ">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</label>
                        <select id="slOperation" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                    </div>
                    <div class="form-group col-sm-2 ">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.ActionType</label>
                        <select id="slActionType" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm">
                            <option value="">@Forwarding.MvcApp.App_Resources.App_Resources.AllActions</option>
                            <option value="I">Insert</option>
                            <option value="U">Update</option>
                            <option value="D">Delete</option>
                        </select>
                    </div>
                    <div class="form-group col-sm-2 ">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                        <input id="txtFromOpenDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="form-group col-sm-2 ">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                        <input id="txtToOpenDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="btn-group col-sm-1 m-t-md" data-toggle="buttons">
                        <button id="btn-print" type="button" class="btn btn-lightblue btn-sm" onclick="OperationChargeLog_Print();"><i class="fa fa-print"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print</button>
                    </div>

                    @*<div class="col-sm-3 m-l-n-sm m-t-sm">
                        <br />
                        <label> <input type="checkbox" id="cbIncludeCurrenciesDetails" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"> Show Currencies Details</label>
                        <br /><br />
                    </div>*@

                    @*<div class="btn-group m-t-n-xs m-r hide">
                        <button data-toggle="dropdown" class="btn btn-sm btn-default dropdown-toggle"><span class="dropdown-label">PDF File</span> <span class="caret"></span></button>
                        <ul id="ulReportTypes" class="dropdown-menu dropdown-select">
                        </ul>
                    </div>*@

                    <!--EOF Region Filter-->
                </div>
            </div>
            
            <!--EOF Region the searh, delete, addnew line-->


        </section>
    </div>
</div>
