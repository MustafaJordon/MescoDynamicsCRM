    <li>
        <a id="hl-homepage" href="javascript:void(0);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="Administration" href="javascript:void(0);" onclick="LoadViews('Settings')">
            @Forwarding.MvcApp.App_Resources.App_Resources.Administration
        </a>
