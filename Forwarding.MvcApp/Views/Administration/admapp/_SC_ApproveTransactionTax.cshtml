<div id="div-SC_ApproveTransaction">
    <!-- Add / Edit Region Modal --> <!--data-keyboard="false" data-backdrop="false" to prevent modal closure on pressing escape and pressing outside the modal-->
    <!-- Region Options & Template  -->

    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.SC_ApproveTransactionTax
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div id="FilterSec" class="row text-sm wrapper hidden-print m-t-n-sm">
                <div class="swapChildrenClass">


                    <div class="form-group  col-sm-3">
                        <div class="col-sm-12">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                            <input id="txtCode" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated  input-sm" data-required="true" data-minlength="1" maxlength="10" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.ex25" style="text-transform:uppercase" />
                        </div>
                    </div>
                    <div class="form-group  col-sm-3 hidden">
                        <div class="col-sm-12">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.SC_TransactionTypes</label>
                            <select id="slTransactionsTypes" class="input-sm form-control" data-required="" onchange=""></select>
                        </div>
                    </div>
                    <div class="form-group  col-sm-3">
                        <div class="col-sm-12">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.PSInvoices</label>
                            <select id="slPSInvoices" class="input-sm form-control" data-required="" onchange=""></select>
                        </div>
                    </div>
                    <div class="form-group  col-sm-3">
                        <div class="col-sm-12">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Supplier</label>
                            <select id="slSuppliers" class="input-sm form-control" data-required="" onchange=""></select>
                        </div>
                    </div>
                    <div class="form-group  col-sm-3">

                        <div class="input-group col-sm-12">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.FromDate</label>
                            <input id="txtFromDate" onchange="" type="text" onfocus="DisableEnterKey(id);" data-required="true" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control input-sm" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />

                        </div>
                    </div>
                </div>
            </div>

            <div id="FilterSec" class="row text-sm wrapper hidden-print m-t-n-sm">
                <div class="swapChildrenClass">
                    <div class="form-group  col-sm-3">

                        <div class="input-group col-sm-12">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.ToDate</label>
                            <input id="txtToDate" type="text" onchange="" onfocus="DisableEnterKey(id);" data-required="true" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control input-sm" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                        </div>
                    </div>

                    <div class="form-group  col-sm-3" style="margin-top:5px;">
                        <div class="input-group col-sm-12">
                            <br />
                            <button id="btn-search1" class="btn btn-sm btn-lightblue" type="button" onclick="SC_TransactionsTax_LoadingWithPaging();"><i class="fa fa-search">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Search</i></button>
                            <button id="btn-Approve" class="btn btn-sm btn-danger" type="button" onclick="SC_TransactionsTax_Approve();"><i class="fa fa-check-circle">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Approve</i></button>
                            &nbsp;&nbsp;
                            <div class="hide">
                                <button id="btn-NewAdd" onclick="SC_TransactionsTax_ClearAllControls();" class="btn btn-warning btn-sm " data-toggle="modal" data-target="#SC_TransactionsTaxModal"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Add</button>
                                &nbsp;&nbsp;
                            </div>
                            <div class="hide">
                                <button id="btn-Delete" type="button" class=" btn btn-sm btn-danger m-r-n hide "><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                            </div>
                        </div>
                    </div>
                    <div class="form-group  col-sm-3">

                    </div>
                    <div class="form-group  col-sm-3">

                    </div>
                </div>
            </div>


            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-12">






                    <div id="div-search-controls " class="input-group col-sm-9 hide">
                        <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.SearchStoreNameAccountName">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="SC_TransactionsTax_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-1">
                    </div>
                </div>
            </div>
            <!--Region the searh, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblSC_TransactionsTax" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th class="th-sortable" data-toggle="class">
                                @Forwarding.MvcApp.App_Resources.App_Resources.Code
                            </th>
                            <th class="th-sortable" data-toggle="class">
                                @Forwarding.MvcApp.App_Resources.App_Resources.Type
                            </th>
                            <th class="th-sortable" data-toggle="class">
                                @Forwarding.MvcApp.App_Resources.App_Resources.InvoiceNumber
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.DateAndTime
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Partner
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Amount
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.ExpensesAmount
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Notes
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>


                    <select id="hidden_slItems" style="visibility:hidden" class="input-sm form-control" data-required="false" onchange=""></select>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>






<div class="modal fade" id="SC_TransactionsTaxModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="SC_TransactionsTaxModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-100">
        @*<div class="modal-content modalwidth-100">*@
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="m-b-none">
                    Details&nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="table-responsive" style="overflow-x:scroll;overflow-y:scroll;">
                    <hr />
                    <h4 class="text-center">Items</h4>
                    <table id="tblItems" style="font-size:17px;" class="table  b-t b-light text-sm  table-hover">
                        <thead>
                            <tr>

                                <th class="" data-toggle="class">
                                    @Forwarding.MvcApp.App_Resources.App_Resources.Item
                                </th>

                                <th class="" data-toggle="class">
                                    @Forwarding.MvcApp.App_Resources.App_Resources.Store
                                </th>
                                <th>
                                    @Forwarding.MvcApp.App_Resources.App_Resources.Quantity
                                </th>
                                <th>
                                    Average Price
                                </th>
                                <th>
                                    @Forwarding.MvcApp.App_Resources.App_Resources.Notes
                                </th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>






            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer">


                <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->

            </div>
        </div>
    </div>
</div>