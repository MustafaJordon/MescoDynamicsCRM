
<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="Administration" href="javascript:void(0);" onclick="LoadViews('Settings')">
            @Forwarding.MvcApp.App_Resources.App_Resources.Administration
        </a>
    </li>
    <li>
        <a id="admapp" href="javascript:void(0);" onclick="LoadViews(id)">
            @Forwarding.MvcApp.App_Resources.App_Resources.admapp
        </a>
    </li>
    <li id="liPost_Restore_Unpost_JVsTax" class="active"></li>
</ul>
<!-- end breadcrumb  -->
<div id="div-JournalVouchers">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 id="h3Post_Restore_Unpost_JVsTax" class="m-b-none"></h3>
            </header>
            <!--Region search, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.JVNo</label>
                    <input id="txtSearchJVNo" tabindex="10" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="50" placeholder="" style="text-transform:uppercase" />
                </div>
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                    <input id="txtSearchFrom" tabindex="20" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                </div>
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                    <input id="txtSearchTo" tabindex="30" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" readonly="readonly" style="background-color:white; cursor:text;" class="form-control datepicker-input input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                </div>
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.JournalType</label>
                    <select id="slSearchJournalType" tabindex="40" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                </div>
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.JVType</label>
                    <select id="slSearchJVType" tabindex="50" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false"></select>
                </div>
                <div class="col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.ReceiptNo</label>
                    <input id="txtSearchReceiptNo" tabindex="60" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="50" placeholder="" data-required="false" style="text-transform:uppercase" />
                </div>
            </div>
            <div class="row text-sm wrapper hidden-print m-t-n-sm swapChildrenClass">
                <div class="swapChildrenClass m-t-n">
                    <div class="col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Value</label>
                        <input id="txtSearchValue" tabindex="70" type="text" onkeypress="CheckValueIsDecimal(id);" onfocus="CheckValueIsDecimal(id);" onblur="CheckDecimalFormat(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="11" @*placeholder="value from 0.00001 to 99999.99999"*@ />
                    </div>
                    <div class="col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                        <input id="txtSearchRemarksHeader" tabindex="80" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" maxlength="100" placeholder="" data-required="false" style="text-transform:uppercase" />
                    </div>
                    <div class="col-sm-2 hide">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Status</label>
                        <select id="slSearchStatus" tabindex="90" class="form-control m-b input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false">
                            <option value="0">@Forwarding.MvcApp.App_Resources.App_Resources.SelectFromMenu</option>
                            <option value="10">@Forwarding.MvcApp.App_Resources.App_Resources.Posted</option>
                            <option value="20">@Forwarding.MvcApp.App_Resources.App_Resources.UnPosted</option>
                        </select>
                    </div>

                    <div class=" col-sm-4 swapChildrenClass m-t-md">
                        <button id="btn-Search" type="button" class="btn btn-lightblue btn-sm swapChildrenClass" onclick="Post_Restore_Unpost_JVsTax_LoadingWithPaging();" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-search"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Search&nbsp;</b></button>
                        <button id="btn-Post" type="button" onclick="Post_Restore_Unpost_JVsTax_PostOrUnpostList(1);" class="btn btn-primary btn-sm swapChildrenClass hide" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-mail-forward"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.PostingJVs&nbsp;</b></button>
                        <button id="btn-UnPost" type="button" onclick="Post_Restore_Unpost_JVsTax_PostOrUnpostList(0);" class="btn btn-primary btn-sm swapChildrenClass hide" @*data-toggle="modal" data-target="#BankModal"*@><i class="fa fa-backward"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.UnPostingJVs&nbsp;</b></button>
                        <button id="btn-Restore" type="button" onclick="Post_Restore_Unpost_JVsTax_RestoreList();" class="btn btn-sm btn-primary swapChildrenClass hide"><i class="fa fa-undo"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.RestoringJVs&nbsp;</b></button>
                        <button id="btn-PermanentDelete-MaunalShow" type="button" onclick="Post_Restore_Unpost_JVsTax_DeleteList('D');" class="btn btn-sm btn-danger swapChildrenClass hide"><i class="fa fa-ban"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.PermanentDelete&nbsp;</b></button>
                    </div>
                    <div class="col-sm-4"></div>
                </div>
            </div>
            <!--EOF Region search, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive m-t-n-sm">
                <table id="tblJournalVouchers" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.JVNo</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Date</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Value</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.JournalType</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.JVType</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ReceiptNo</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</th>
                            <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.Posted</th>
                            <th class="rounded-right hide"></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>

