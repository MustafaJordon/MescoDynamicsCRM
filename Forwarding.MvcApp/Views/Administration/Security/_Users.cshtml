                                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Email</label>
                                                <input id="txtEmail" name="Email" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="Email" />
                                            </div>
                                            <div class="form-group col-sm-4">
                                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Email_Password</label>
                                                <input id="txtEmail_Password" name="EmailPassword" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="password" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="Email Password" />
                                            </div>
                                            <div class="form-group col-sm-4">
                                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Email_DisplayName</label>
                                                <input id="txtEmail_DisplayName" name="Email_DisplayName" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="Display Name" />
                                            </div>
                                        </div>
                                        <div class="row text-sm wrapper hidden-print m-t-n-sm">
                                            <div class="form-group col-sm-4">
                                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Email_Host</label>
                                                <input id="txtEmail_Host" name="Email_Host" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="Email Host" />
                                            </div>
                                            <div class="form-group col-sm-4">
                                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Email_Port</label>
                                                <input id="txtEmail_Port" name="Email_Port" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="number" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="Email_Port" />
