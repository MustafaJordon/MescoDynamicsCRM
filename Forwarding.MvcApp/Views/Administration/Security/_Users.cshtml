                                            </div>
                                        </div>
                                        <div class="row">
                                            <h3>Signature</h3>
                                            <div class="col-sm-12 text-sm wrapper hidden-print m-t-n-sm">
                                                <textare id="txtEmail_Footer"></textare>
                                            </div>
                                        </div>
                                        @*send test email*@
                                        <div class="row text-sm wrapper hidden-print m-t-n-sm">
                                            <h3>@Forwarding.MvcApp.App_Resources.App_Resources.SendTestEmail @Forwarding.MvcApp.App_Resources.App_Resources.TestYourSetting</h3>
                                        </div>
                                        <div class="row text-sm wrapper hidden-print m-t-n-sm">
                                            <div class="form-group col-sm-2">
                                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                                                <input id="txtEmail_To" name="Email_To" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="To" />
                                            </div>
                                            <div class="form-group col-sm-2">
                                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Subject</label>
                                                <input id="txtEmail_Subject" name="Email_Subject" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="Subject" />
                                            </div>
                                            <div class="form-group col-sm-6">
                                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Body</label>
                                                <input id="txtEmail_Body" name="Email_Subject" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated" data-required="false" maxlength="100" placeholder="Body" />
                                            </div>
                                            <div class="form-group col-sm-2">
                                                <button type="button" id="btnSendTestEmail" class="btn btn-warning btn-sm" onclick="SendTestEmail();" style="margin-top: 25px;">
                                                    <span class="fa fa-envelope-o"></span> @Forwarding.MvcApp.App_Resources.App_Resources.SendTestEmail
                                                </button>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="step-pane  step-content m-t-n-lg" id="UserSalesmen">
                                        <div class="row text-sm wrapper hidden-print m-t-n-sm">
                                            <section id="secUserSalesmen" class="m-t-xs m-l col-sm-5 panel panel-default">
                                                <div class="">
                                                    <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllUserSalesmen" onchange="cbCheckAllUserSalesmenChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;Select All Salesmen&nbsp;</u></label><br />
                                                </div>
                                                <div id="divCbUserSalesmen" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                                                </div>
                                            </section>
                                        </div>

                                    </div>
                                    <!-- Region Add / Edit Region Form -->
                                </div>
                            </fieldset>
                        </div>
                    </div>
                </div>
                <!-- Endregion Add / Edit Region Form -->
                <div class="modal-footer m-t-n-lg">
                    <a id="btnSave" class="btn btn-primary btn-sm" onclick="Users_Insert(false);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
                    </a>
                    <a id="btnSaveandNew" class="btn btn-primary btn-sm" onclick="Users_Insert(true);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                    </a>
                    <button class="btnClose btn btn-link btn-sm" id="btnClose" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
                </div>
            </div>
        </div>
    </div>



