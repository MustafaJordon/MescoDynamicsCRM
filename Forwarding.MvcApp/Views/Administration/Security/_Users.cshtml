                                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Address</label>
                                                <input type="text" id="txtAddress" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated" data-required="false" maxlength="250" placeholder="Address" style="text-transform:uppercase" />
