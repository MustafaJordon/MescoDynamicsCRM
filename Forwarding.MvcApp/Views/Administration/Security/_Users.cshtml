        <div class="modal-dialog modalwidth-90">
            <div class="modal-content m-t-n">
                <div class="modal-header">
                    <h3 class="m-b-none">
                        @Forwarding.MvcApp.App_Resources.App_Resources.User &nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                        &nbsp;&nbsp;&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Role &nbsp;<label id="lblRoleShown" class="static-text-danger"></label>
                    </h3>
                </div>
                <!-- Region Add / Edit Region Form -->
                <div class="modal-body m-t-n">
                    <div class="span7 form vertical-mode" id="User-form">

                        <div class="wizard wizard-vertical ">
                            <ul class="steps">
                                <li data-target="#BasicData" id="stepsBasicData" onclick="" class="active"><span class="badge badge-info" style="background-color:midnightblue;">1</span>@Forwarding.MvcApp.App_Resources.App_Resources.BasicData</li>
                                <li data-target="#EmailSetting" id="stepsEmailSetting" onclick="" class="complete non_basic"><span class="badge badge-info" style="background-color:midnightblue;">2</span>@Forwarding.MvcApp.App_Resources.App_Resources.Email_Setting</li>
                                <li data-target="#UserSalesmen" id="stepsUserSalesmen" onclick="" class="complete non_basic"><span class="badge badge-info" style="background-color:midnightblue;">3</span>Salesmen</li>

                            </ul>
                            <div class="actions" style="display:none;">
                                <!--Next & Previous buttons-->
                                <button type="button" class="btn btn-default btn-xs btn-prev" disabled="disabled">@Forwarding.MvcApp.App_Resources.App_Resources.Prev</button>
