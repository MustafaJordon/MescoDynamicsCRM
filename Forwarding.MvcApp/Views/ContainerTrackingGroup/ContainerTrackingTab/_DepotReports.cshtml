<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);" onclick="LoadViews(id);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="ContainerTrackingGroup" href="javascript:void(0);" onclick="LoadViews('ContainerTrackingGroup')">
            @Forwarding.MvcApp.App_Resources.App_Resources.ContainerTracking
        </a>
    </li>
    @*<li><a id="ContainerTrackingTab" href="javascript:void(0);" onclick="LoadViews(ContainerTrackingGroup)">@Forwarding.MvcApp.App_Resources.App_Resources.ContainerTracking
        </a></li>*@
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.DepotReports </li>
</ul>


<div id="div-ContainerReports">

    <!-- Region Options & Template  -->

    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>

        <div class="row">
            @*<div class="col-sm-12">*@
            <section class="panel panel-default">
                <header class="panel-heading font-bold">
                    <h3 class="m-b-none">
                        @Forwarding.MvcApp.App_Resources.App_Resources.DepotReports
                    </h3>
                </header>
                <div class="panel-body" id="ContainerReportModal">
                    <form role="form">




                        <div class="form-group col-sm-4" style="clear:both;">

                            <div class="form-group m-l-lg">
                                <div>
                                    <div class="row">
                                        <div>
                                            <!-- radio -->
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rContainerStock" value="ContainerStock" checked="checked">
                                                    Container Stock
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rContainerStockSummary" value="ContainerStockSummary">
                                                    Container Stock Summary
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rEirSummary" value="EirSummary">
                                                    Eir Summary
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rContainerStockByLocation" value="ContainerStockByLocation">
                                                    Container Stock By Location
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rContainerStockByLine" value="ContainerStockByLine">
                                                    Container Stock By Line
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rContainerStockByCustomers" value="ContainerStockByCustomers">
                                                    Container Stock By Customers
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rEirSummaryByDriver" value="EirSummaryByDriver">
                                                    Eir Summary By Driver
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rContainerStockByconsignee" value="ContainerStockByconsignee">
                                                    Container Stock By consignee
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rEirSummaryByTransporter" value="EirSummaryByTransporter">
                                                    Eir Summary By Transporter
                                                </label>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="form-group col-sm-8">
                            <div class="form-group">

                                <div id="divFromDate" class="form-group col-sm-7 hidden">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                                    <input id="dtpFromDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="false" data-date-format="dd/mm/yyyy" placeholder="From Date" autocomplete="off" />
                                </div>
                                <div id="divToDate" class="form-group col-sm-7 hidden">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                                    <input id="dtpToDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="false" data-date-format="dd/mm/yyyy" placeholder="To Date" autocomplete="off" />
                                </div>
                                <div id="divContainerNo" class="col-sm-7" style="clear:both;">
                                    <label id="lblContainerNo">ContainerNo</label>
                                    <input type="text" id="txtContainerNo" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="11" placeholder="Container No" style="text-transform:uppercase">
                                </div>
                                <div id="divSize" class="col-sm-7 hidden" style="clear:both;">
                                    <label id="lblSize">Size</label>
                                    <input type="text" id="txtSize" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="10" placeholder="Size" style="text-transform:uppercase">
                                </div>
                                <div id="divCustomer" class="col-sm-7" style="clear:both;">
                                    <label id="lblCustomer">Customer</label>
                                    <input type="text" id="txtCustomer" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="Customer" style="text-transform:uppercase">
                                </div>
                                <div id="divConsignee" class="col-sm-7" style="clear:both;">
                                    <label id="lblConsignee">Consignee</label>
                                    <input type="text" id="txtConsignee" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="Consignee" style="text-transform:uppercase">
                                </div>
                                <div id="divLocation" class="col-sm-7" style="clear:both;">
                                    <label id="lblLocation">Location</label>
                                    <input type="text" id="txtLocation" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="30" placeholder="Location" style="text-transform:uppercase">
                                </div>
                                <div id="divLoad" class="col-sm-7" style="clear:both;">
                                    <label id="lblLoad">Load</label>
                                    <input type="text" id="txtLoad" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="10" placeholder="Load" style="text-transform:uppercase">
                                </div>
                                <div id="divJobNo" class="col-sm-7" style="clear:both;">
                                    <label id="lblJobNo">JobNo</label>
                                    <input type="text" id="txtJobNo" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="JobNo" style="text-transform:uppercase">
                                </div>
                                <div id="divTransporter" class="col-sm-7" style="clear:both;">
                                    <label id="lblTransporter">Transporter</label>
                                    <input type="text" id="txtTransporter" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="Transporter" style="text-transform:uppercase">
                                </div>
                                <div id="divTruckNo" class="col-sm-7" style="clear:both;">
                                    <label id="lblTruckNo">TruckNo</label>
                                    <input type="text" id="txtTruckNo" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="TruckNo" style="text-transform:uppercase">
                                </div>
                                <div id="divDriver" class="col-sm-7" style="clear:both;">
                                    <label id="lblDriver">Driver</label>
                                    <input type="text" id="txtDriver" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="Driver" style="text-transform:uppercase">
                                </div>
                                <div id="divBookingNo" class="col-sm-7" style="clear:both;">
                                    <label id="lblBookingNo">BookingNo</label>
                                    <input type="text" id="txtBookingNo" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="BookingNo" style="text-transform:uppercase">
                                </div>
                            </div>
                        </div>

                            @*<button id="btnPrint" type="submit" class="btn btn-sm btn-default">Submit</button>*@
                            <div class="col-sm-4" style="clear:both;">
                                <br /><br /><br /><br /><br /><br />
                                <button type="button" id="btnPrint" class="btn btn-primary btn-sm" onclick="PrintCntrReports();">
                                    <span class="glyphicon glyphicon-print"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Print

                                </button>
                            </div>

                    </form>
                </div>
            </section>
            @*</div>*@
        </div>




    </div>




</div>

