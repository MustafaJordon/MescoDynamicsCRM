
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);" onclick="LoadViews(id);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="ContainerTrackingGroup" href="javascript:void(0);" onclick="LoadViews('ContainerTrackingGroup')">
            @Forwarding.MvcApp.App_Resources.App_Resources.ContainerTracking
        </a>
    </li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.OperationsACIDDetails </li>
</ul>

<div id="div-OperationsACIDDetails">
    <!-- Add / Edit OperationsACIDDetails Modal -->
    <div class="modal fade " id="OperationsACIDDetailsModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="OperationsACIDDetailsModalLabel" aria-hidden="true">
        <div class="modal-dialog modalwidth-80">
            <div class="modal-content">
                <div class="modal-header">
                    @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        &times;</button>*@
                    <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.OperationsACIDDetails &nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                    </h3>
                </div>
                <div class="modal-body">
                    <!-- OperationsACIDDetails Add / Edit OperationsACIDDetails Form -->
                    <div class="span7 form vertical-mode" id="OperationsACIDDetails-form">
                        <fieldset>
                            <div class="form-group pull-in  has-error clearfix">
                                <input id="hID" type="hidden" />
                                <input id="hACIDDetailsOperationID" type="hidden" />
                                <!-- start alert message OperationsACIDDetails -->
                                <div class="alert alert-danger div-error">
                                    @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                </div>
                                <!-- end alert message OperationsACIDDetails -->
                                <div class="row">
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.OperationCode</label>
                                        <input id="txtACIDDetailsOperationCode" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.ACIDNumber</label>
                                        <input id="txtACIDDetailsACIDNumber" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Open By</label>
                                        <input id="txtACIDDetailsOpenedBy" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.OperationMan</label>
                                        <input id="txtACIDDetailsOperationMan" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.ExpirationDate</label>
                                        <input id="txtACIDDetailsExpirationDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text; background-color:white;" class="datepicker-input form-control input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Consignee Name</label>
                                        <input id="txtACIDDetailsConsigneeName" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Consignee 2 Name</label>
                                        <input id="txtACIDDetailsConsignee2Name" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.MasterBL</label>
                                        <input id="txtACIDDetailsMasterBL" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.HBL</label>
                                        <input id="txtACIDDetailsHBL" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Quantity</label>
                                        <input id="txtACIDDetailsQuantity" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                        <input id="txtACIDDetailsNotes" disabled class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Re-Import Approval</label>
                                        <input id="txtACIDDetailsReImportApproval" class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group col-sm-3">
                                        <label>Re-Import Approval Number</label>
                                        <input id="txtACIDDetailsReImportApprovalNumber" class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Survey Request</label>
                                        <input id="txtACIDDetailsSurveyRequest" class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Survey Date</label>
                                        <input id="txtACIDDetailsSurveyDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text; background-color:white;" class="datepicker-input form-control input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Upload Cargo Date</label>
                                        <input id="txtACIDDetailsUploadCargoDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text; background-color:white;" class="datepicker-input form-control input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group col-sm-3">
                                        <label>Bank Nomination</label>
                                        <input id="txtACIDDetailsBankNomination" class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Transaction Method</label>
                                        <input id="txtACIDDetailsTransactionMethod" class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Bank Nomination Opened By</label>
                                        <input id="txtACIDDetailsBankNominationOpenedBy" class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label>Customs Certificate No</label>
                                        <input id="txtACIDDetailsCustomsCertificateNo" class="form-control input-sm" style="text-transform:uppercase;" />
                                    </div>
                                </div>
                                
                            </div>
                            
                        </fieldset>
                    </div>
                    <!-- Endegion Add / Edit OperationsACIDDetails Form -->
                </div>
                <div class="modal-footer">
                    <button type="button" id="btnSave" class="btn btn-primary btn-sm" onclick="OperationsACIDDetails_Update();">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save</button>
                    <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="OperationsACIDDetails_UnlockRecord();" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
                </div>
            </div>
        </div>
    </div>
    <!-- OperationsACIDDetails Options & Template  -->
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.OperationsACIDDetails
                </h3>
            </header>
            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>Copy </button>
                        @*<button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>Print </button>*@
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-7">
                    <div id="div-search-controls" class="input-group col-sm-9">
                        <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase"  type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search ACID Number">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="OperationsACIDDetails_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    
                </div>
            </div>
            <div class="table-responsive">
                <table id="tblOperationsACIDDetails" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            @*<th id="ID">
            <input id="cb-CheckAll" type="checkbox" />
        </th>*@
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.OperationCode
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.ACIDNumber
                            </th>
                            <th>
                                Open By
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.OperationMan
                            </th>
                            <th>
                                Expiration Date
                            </th>

                            <th>
                                Consignee Name
                            </th>
                            <th>
                                Consignee 2 Name
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.MasterBL
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.HBL
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Quantity
                            </th>


                            <th>
                                Re-Import Approval
                            </th>
                            <th>
                                Re-Import Approval Number
                            </th>
                            <th>
                                Survey Request
                            </th>
                            <th>
                                Survey Date
                            </th>
                            <th>
                                Upload Cargo Date
                            </th>
                            <th>
                                Bank Nomination
                            </th>
                            <th>
                                Transaction Method
                            </th>
                            <th>
                                Bank Nomination Opened By
                            </th>
                            <th>
                                Customs Certificate No
                            </th>

                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Notes
                            </th>

                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm"> @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
        </section>
    </div>
</div>
