<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li><a id="hl-homepage" href="javascript:void(0);" onclick="LoadViews(id);"><i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
    </a></li>
    <li>
        <a id="TR" href="javascript:void(0);" onclick="LoadViews('TR_Transactions')">
            @Forwarding.MvcApp.App_Resources.App_Resources.TR
        </a>
    </li>
    <li><a id="TR_Reports" href="javascript:void(0);" onclick="LoadViews('TR_Reports')">@Forwarding.MvcApp.App_Resources.App_Resources.Reports
    </a></li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.TruckingOrderReportForSupplier</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-TrailerProfitability">
    <!-- Region Options & Template  -->
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <div id="hExportedTable" class="hide"></div>
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.FilterBy</h3>
                @*<h3 class="m-b-none">Best Print Using Internet Explorer</h3>*@
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-l">
                <div id="div-main-options">
                    <!--Region Filter-->




                    <div class="form-group col-sm-2">
                        <label>Serial</label>
                        <input type="text" id="txtFilterTruckingOrderCode" autocomplete="off" class="form-control input-sm" maxlength="200" placeholder="" style="text-transform:uppercase">
                    </div>

                    <div class="form-group col-sm-2">
                        <label>Operation Serial</label>
                        <input type="text" id="txtFilterOperationSerial" autocomplete="off" class="form-control input-sm" maxlength="200" placeholder="" style="text-transform:uppercase">
                    </div>

                    <div class="form-group col-sm-2 hide" style="">
                        <label>Equipment</label>
                        <select id="slFilterEquipment" class="input-sm col-sm-12"><option value=""><--Select--></option></select>
                    </div>
                    <div class="form-group col-sm-2 hide" style="">
                        <label>Trailer</label>
                        <select id="slFilterTrailer" class="input-sm col-sm-12"><option value=""><--Select--></option></select>
                    </div>

                    <div class="form-group col-sm-2 hide" style="clear:both;">
                        <label>Truck Number</label>
                        <input type="text" id="txtFilterTruckNumber" autocomplete="off" class="form-control input-sm" maxlength="200" placeholder="" style="text-transform:uppercase">
                    </div>

                    <div class="form-group col-sm-4" style="">
                        <label>Trucking Order</label>
                        <select id="slFilterTruckingOrder" class="form-control input-sm"><option value=""><--Select--></option></select>
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                        <input id="txtFromDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                        <input id="txtToDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>
                    <div class="form-group col-sm-2" style="clear:both;">
                        <label>Client</label>
                        <select id="slFilterCustomer" class="form-control input-sm"><option value=""><--Select--></option></select>
                    </div>
                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Trucker</label>
                        <select id="slFilterTrucker" data-required="false" class="form-control input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);"><option value=""><--Select--></option></select>
                    </div>

                    <div class="form-group col-sm-2">
                        <label>Bill Number</label>
                        <input type="text" id="txtFilterBillNumber" autocomplete="off" class="form-control input-sm" maxlength="200" placeholder="" style="text-transform:uppercase">
                    </div>

                    <div class="form-group col-sm-2">
                        <label>Booking Number</label>
                        <input type="text" id="txtFilterBookingNumber" autocomplete="off" class="form-control input-sm" maxlength="200" placeholder="" style="text-transform:uppercase">
                    </div>

                    <div class="form-group col-sm-2">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Status</label>
                        <select id="slSearchStatus" tabindex="100" class="form-control input-sm" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" data-required="false">
                            <option value="0" selected>@Forwarding.MvcApp.App_Resources.App_Resources.SelectFromMenu</option>
                            <option value="10">Service Done</option>
                            <option value="20">Service Not Done</option>
                        </select>
                    </div>
                    <div class="form-group col-sm-2" style="">
                        <label>CreatedBy</label>
                        <select id="slFilterCreator" class="form-control input-sm"><option value=""><--Select--></option></select>
                    </div>

                    <div class="form-group col-sm-2 hide">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Invoice</label>
                        <input type="text" id="txtFilterInvoiceNumber" autocomplete="off" class="form-control input-sm" maxlength="200" placeholder="" style="text-transform:uppercase">
                    </div>

                    <div class="form-group col-sm-2 hide classShowForELI">
                        <label>Stuffing Date - تاريخ التحميل </label>
                        <input id="txtFilterStuffingDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control input-sm" data-required="false" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                    </div>

                    <div>
                        @*<div class="btn-group m-t-n-xs m-r hide">
                <button data-toggle="dropdown" class="btn btn-sm btn-default dropdown-toggle"><span class="dropdown-label">PDF File</span> <span class="caret"></span></button>
                <ul id="ulReportTypes" class="dropdown-menu dropdown-select">
                </ul>
            </div>*@
                        <div class="col-sm-4 m-t-sm" data-toggle="buttons" style="clear:both;">
                            <button id="btn-print" type="button" class="btn btn-lightblue btn-sm hide" onclick="TruckingOrderReportForSuppliers_Print('Print');"><i class="fa fa-print"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print</button>
                            <button id="btn-ExportToExcel" type="button" onclick="TruckingOrderReportForSuppliers_Print('Excel');" class="btn btn-sm btn-primary"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>
                        </div>



                    </div>
                    <!--EOF Region Filter-->
                </div>
            </div>
            <!--EOF Region the searh, delete, addnew line-->
            
            
        </section>
    </div>
</div>