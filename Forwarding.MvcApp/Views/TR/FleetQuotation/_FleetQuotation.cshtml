                                @*<button id="btn-GenerateDefaultLocations" class="btn btn-info btn-sm m-t-xxs" data-toggle="modal" data-target="" onclick="PickupDetails_GenerateDefaultLocations();"><i class="fa fa-hand-o-down"></i>&nbsp;Default Locations</button>*@
                            </div>

                            <div class="form-group col-sm-1 m-t-27 hide">
                                <label><input type="checkbox" id="cbIsFinalized">&nbsp;Finalized</label>
                            </div>
                            <div id="divFinalize" class="col-sm-2 m-t-md hide">
                                <button id="btn-Finalize" class="btn btn-lightblue btn-sm m-t-xxs classDisableForFinalized" data-toggle="modal" data-target="" onclick="Contract_Finalize();"><i class="fa fa-hand-o-right"></i>&nbsp;Finalize</button>
                                @*<button id="btn-GenerateDefaultLocations" class="btn btn-info btn-sm m-t-xxs" data-toggle="modal" data-target="" onclick="PickupDetails_GenerateDefaultLocations();"><i class="fa fa-hand-o-down"></i>&nbsp;Default Locations</button>*@
                            </div>
                            <!--Details-->
                            <div id="divFleetQuotationDetailsBtns" class="col-sm-12 m-t" style="clear:both;">
                                <div class="form-group col-sm-1">
                                    <button id="btn-AddFleetQuotationDetails" class="btn btn-warning btn-sm classDisableForFinalized" data-toggle="modal" data-target="" onclick="FleetQuotationDetails_FillControls(0);"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.AddDetails</button>
                                </div>
                                
                                <div class="input-group col-sm-2">
                                    <select id="slFilterRoutingsPOL" onchange="FleetQuotationDetails_LoadAll();" onfocus="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="input-sm form-control" data-required="false">
                                        <option value="0"><--All POL--></option>
                                    </select>
                                </div>
                                <div class="input-group col-sm-2">
                                    <select id="slFilterRoutingsPOD" onchange="FleetQuotationDetails_LoadAll();" onfocus="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="input-sm form-control" data-required="false">
                                        <option value="0"><--All POD--></option>
                                    </select>
                                </div>

                                <div class="form-group col-sm-3 m-l-n-lg">
                                    @*<select id="slFilterDivision" onchange="FleetQuotationDetails_LoadAll();" class="form-control m-b input-sm" data-required="false"></select>*@
                                    
                                    <div id="div-search-controls-FitlerDivision" class="input-group col-sm-3">
                                        <select id="slFilterDivision" onchange="FleetQuotationDetails_LoadAll();" onfocus="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="input-sm form-control" data-required="false">
                                            <option value="0"><--All Division--></option>
                                        </select>
                                        <span class="input-group-btn">
                                            <button id="btn-SearchCharges-FitlerDivision" onclick="FleetQuotationDetails_LoadAll();" class="btn btn-sm btn-primary" type="button"><i class="fa fa-search"></i></button>
                                        </span>
                                    </div>
                                </div>


                                <button id="btn-DeleteFleetQuotationDetails" title="Delete Details" type="button" class="float-right btn btn-sm btn-danger classDisableForFinalized" onclick="FleetQuotationDetails_DeleteList();"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                            </div>

                            <div id="divFleetQuotationDetails" class="col-sm-12 m-t-xs" style="clear:both;">
                                <section class="panel panel-default">
                                    <header class="panel-heading font-bold text-ul">@Forwarding.MvcApp.App_Resources.App_Resources.Details</header>
                                    <!--Region: table header-->
                                    <div class="table-responsive" style="width:100%; height:320px; overflow:scroll;">
                                        <table id="tblRoutings" class="table table-striped b-t b-light text-sm  table-hover">
                                            <thead>
                                                <tr>
                                                    <th id="HeaderDeleteFleetQuotationDetailsID" style="width:5%;"><input id="cb-CheckAll-FleetQuotationDetails" type="checkbox" /></th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.Code</th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.POL</th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.POD</th>
                                                    <th class="hide">@Forwarding.MvcApp.App_Resources.App_Resources.Equipment</th>
                                                    <th>Model</th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.Commodity</th>
                                                    <th class="hide">Loading/Unloading</th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.Cost</th>
                                                    <th>Freight Rate</th>
                                                    <th>@Forwarding.MvcApp.App_Resources.App_Resources.Division</th>
                                                    <th class="rounded-right hide"></th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                    <!--Region: EOF table header-->
                                    <!--Region: table footer-->
                                    <footer class="panel-footer hide">
                                        <div class="col-sm-12 m-t-n-sm swapChildrenClass">
                                            <!--
                                                <div class="col-sm-6 swapChildrenClass"><b>&emsp;Row Max Weight(KGM)</b><label id="lblRowMaxWeight" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                                <div class="col-sm-6 text-right swapChildrenClass"><b>&emsp;Row Max Volume(CBM)</b><label id="lblRowMaxVolume" @*purpose="dynamicLabel"*@ class="static-text-blue"></label></div>
                                            -->
                                        </div>
                                    </footer>
                                    <!--Region: EOF table footer-->
                                </section>
                            </div>

                            <!--EOF Details-->
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer m-t-n">
                <label id="lblIsSpanish" class=""><input type="checkbox" id="cbIsSpanish" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;Español</label>
                <button id="btn-PrintQuotation" type="button" class="btn btn-sm btn-lightblue m-t-n-sm" onclick="QuotationsEdit_Draw('Print');"><i class="fa fa-print"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print</button>
