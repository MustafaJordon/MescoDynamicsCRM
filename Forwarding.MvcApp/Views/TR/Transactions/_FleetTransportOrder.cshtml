<div id="div-TransportOrder">
    <input id="hOperationID" type="hidden" />
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 id="h3LblMainScreen" class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.FleetTransportOrder
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-t-n-xs">
                <div id="div-main-options" class="col-sm-4 m-b-xs">
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm swapChildrenClass hide" data-toggle="modal" data-target="#TransportOrderModal"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>
                    <button id="btn-FilterOptions" class="btn btn-lightblue btn-sm m-l" data-toggle="modal" onclick="" data-target="#FilterModal"><i class="fa fa-filter"></i> Filter Options</button>
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                    </div>
                </div>

                <div id="div-search-controls" class="input-group col-sm-4 hide">
                    <input id="txt-Search" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="">
                    <span class="input-group-btn">
                        <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="TruckingOrders_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                    </span>
                </div>
                <div class="float-right col-sm-5">
                    <button id="btn-Delete" type="button" class="float-right btn btn-sm btn-danger m-r-n swapChildrenClass hide"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>
                    <button id="btn-UnApproveRoute" type="button" class="float-right btn btn-warning btn-sm hide" onclick="Routings_ApproveList(0);"><i class="fa fa-undo"></i>&nbsp;UnUpprove</button>
                    <button id="btn-ApproveRoute" type="button" class="float-right btn btn-primary btn-sm hide" onclick="Routings_ApproveList(1);"><i class="fa fa-check-square-o"></i>&nbsp;Service Done</button>
                    <button id="btn-RequestApproveRoute" type="button" class="float-right btn btn-info btn-sm" onclick="Routings_RequestApproval();"><i class="fa fa-check-square-o"></i>&nbsp;Approval Request</button>
                </div>
            </div>
            <!--Region the searh, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive m-t-n">
                <table id="tblRoutings" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID" style="width:5%;"><input id="cb-CheckAll" type="checkbox" /></th>
                            <th style="width:5%;">@Forwarding.MvcApp.App_Resources.App_Resources.Code</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Route</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Carrier</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Commodity</th>
                            <th class="hide">Freight Rate</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Cost</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.CreatedBy</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Invoice</th>
                            <th class="rounded-right"></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>

