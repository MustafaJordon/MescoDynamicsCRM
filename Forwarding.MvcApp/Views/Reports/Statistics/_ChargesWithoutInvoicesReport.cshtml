<div id="div-ChargesWithoutInvoicesReport">
    <!-- Region Options & Template  -->
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <div id="hExportedTable" class="hide"></div>
                <h3 class="m-b-none">@Forwarding.MvcApp.App_Resources.App_Resources.FilterBy</h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print m-l">
                <div id="div-main-options">
                    <!--Region Filter-->
                    &nbsp;&nbsp;&nbsp;
                    <div class="row">
                        <div class="form-group col-sm-4">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.FromDate</label>
                            <input id="txtFromDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                        </div>
                        <div class="form-group col-sm-4">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.ToDate</label>
                            <input id="txtToDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Type</label>
                            <select id="slIsROrP" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm">
                                <option value=""><--Select--></option>
                                <option value="1">Receivables</option>
                                <option value="0">Payables</option>
                            </select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.OperationCode</label>
                            <select id="slOperations" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</label>
                            <select id="slClient" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.POL</label>
                            <select id="slPOL" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                        </div>
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.POD</label>
                            <select id="slPOD" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                        </div>
                        @*<div class="form-group col-sm-2">
                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Branches</label>
                <select id="slBranch" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
            </div>*@
                        <div class="form-group col-sm-2">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.ChargeTypeName</label>
                            <select id="slChargeType" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-sm-3">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.BookingNumber</label>
                            <input type="text" id="txtFilterBookingNumbers" autocomplete="off" class="form-control input-sm" maxlength="200" placeholder="" style="text-transform:uppercase">
                        </div>
                        <div class="form-group col-sm-3">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.MasterBL</label>
                            <input type="text" id="txtFilterMasterBL" autocomplete="off" class="form-control input-sm" maxlength="200" placeholder="" style="text-transform:uppercase">
                        </div>
                        <div class="form-group col-sm-3">
                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.House</label>
                            <input type="text" id="txtFilterHouseBLs" autocomplete="off" class="form-control input-sm" maxlength="200" placeholder="" style="text-transform:uppercase">
                        </div>
                    </div>
                    

                    <br /><br />
                    <div class="row">
                        <div class="col-sm-3">
                            <button id="btn-print" type="button" class="btn btn-lightblue btn-sm" onclick="ChargesWithoutInvoicesReport_Print('Print');"><i class="fa fa-print"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print</b></button>
                        </div>
                        <div class="col-sm-3">
                            <button id="btn-ExportToExcel" type="button" onclick="ChargesWithoutInvoicesReport_Print('Excel');" class="btn btn-sm btn-primary"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>
                        </div>
                    </div>

                </div>
            </div>
            <!--EOF Region the searh, delete, addnew line-->

        </section>
    </div>
</div>
