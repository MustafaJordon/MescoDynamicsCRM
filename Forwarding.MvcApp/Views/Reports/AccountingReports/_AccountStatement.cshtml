<div id="div-SubAccountLedger">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.SubAccountLedger
            </h3>
        </header>
        <div class="row text-sm wrapper hidden-print m-t-n-sm">
            <div class="input-group col-sm-2">
                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Group</label>
                <select id="slSubAccountGroup" onchange="SubAccountLedger_slSubAccountGroupChanged();" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
            </div>
            <div class="input-group col-sm-2">
                <label>Sub-Accounts</label>
                <select id="slSubAccount" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"><option value=""><--Select--></option></select>
            </div>
            <div class="input-group col-sm-1">
                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                <select id="slCurrency" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"></select>
            </div>
            <div class="form-group col-sm-2">
                <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                <input id="txtFromDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
            </div>
            <div class="form-group col-sm-2">
                <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                <input id="txtToDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="Select Date" />
            </div>
            <div class="input-group col-sm-3 m-t-27">
                <label class=""><input type="checkbox" id="cbSuppressForZeroes" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SuppressForZeroes</label>
                &emsp;<label class=""><input type="checkbox" id="cbHeaderLogo" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;Header</label>
            </div>
            @*<div class="input-group col-sm-2 m-t-27">
                <label class=""><input type="checkbox" id="cbHeaderLogo" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;Header Logo</label><br />
            </div>*@
            <div class="input-group col-sm-2 hide">
                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Status</label>
                <select id="slStatus" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm">
                    <option value="0">@Forwarding.MvcApp.App_Resources.App_Resources.All</option>
                    <option value="1">@Forwarding.MvcApp.App_Resources.App_Resources.Posted</option>
                    <option value="2">@Forwarding.MvcApp.App_Resources.App_Resources.UnPosted</option>
                </select>
            </div>
        </div>
        <div class="row text-sm wrapper hidden-print m-t-n-md">
            <div class="col-sm-1 swapChildrenClass m-t-xs">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsDetails" checked="checked" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Detailed&nbsp;</b>
            </div>
            <div class="col-sm-2 swapChildrenClass m-t-xs hide">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsGroupByCostCenter" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"> <b> Cost Center By Sub Account</b> @*<b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.GroupByCostCenter&nbsp;</b>*@
            </div>
            <div class="col-sm-2 swapChildrenClass m-t-xs hide">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsByCostCenter" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b> Cost Center By Account</b>
            </div>
            <div class="col-sm-1 swapChildrenClass m-t-xs hide">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsByCurrency" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>By Currency</b>
            </div>
            <div class="col-sm-1 swapChildrenClass m-t-xs">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsClientStatmentEn" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>Client Statment En</b>
            </div>
            <div class="col-sm-1 swapChildrenClass m-t-xs hide">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsClientStatmentAr" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>Client Statment Ar</b>
            </div>
            <div class="col-sm-1 swapChildrenClass m-t-xs">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsAgentStatmentEn" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>Agent Statment En</b>
            </div>
            <div class="col-sm-1 swapChildrenClass m-t-xs hide">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsAgentStatmentAr" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>Agent Statment Ar</b>
            </div>
            <div class="col-sm-1 swapChildrenClass m-t-xs hide">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsCostCenterSummary" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>Cost Center Summary</b>
            </div>
            <div class="col-sm-1 swapChildrenClass m-t-xs hide">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsCostCenterProfit" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>Cost Center Profit</b>
            </div>
            <div class="col-sm-2 swapChildrenClass hide m-t-xs">
                <input tabindex="" type="radio" name="nameRdGroup" id="cbIsSupplierStatementEn" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SupplierStatementEn&nbsp;</b>
            </div>
            <div class="input-group col-sm-2 hide m-t-xs">
                <label class=""><input type="checkbox" id="cbShowSubAccount" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.ShowSubAccounts</label><br />
            </div>
            <div class="col-sm-4">
                <button id="btn-ExportToExcel" type="button" onclick="SubAccountLedger_Print('Excel');" class="btn btn-sm btn-lightblue"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>
                <button id="btn-Print" type="button" onclick="SubAccountLedger_Print('Print');" class="btn btn-sm btn-info"><i class="fa fa-print"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print&nbsp;</b></button>
                <div class="input-group col-sm-7">
                    <input type="text" id="txtToEmail" placeholder=" Send To " class="form-control input-sm">
                    <span BaseUrl="@HttpContext.Current.Request.Url.Scheme://@HttpContext.Current.Request.Url.Authority" id="btn-Email" title="Send Report as Email " class="input-group-addon" onclick="SubAccountLedger_Print('Email');" style="background-color:green;padding:5px!important;color:white;cursor:pointer;"><span class="fa fa-envelope">&nbsp; @Forwarding.MvcApp.App_Resources.App_Resources.Send</span>&nbsp;</span>
                </div>
            </div>
        </div>
        <div class="row text-sm wrapper hidden-print m-t-n-md hide">
            <section id="secSubAccount" class="m-l col-sm-5 panel panel-default" style="clear:both;">
                <div class="">
                    <label class=""><input type="checkbox" id="cbCheckAllSubAccounts" onchange="SubAccountLedger_cbCheckAllSubAccountsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;Select All SubAccounts</u></label><br />
                </div>
                <div id="divCbSubAccount" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                </div>
            </section>
            <section id="secAccount" class="m-l col-sm-3 panel panel-default">
                <div class="">
                    <label class=""><input type="checkbox" id="cbCheckAllAccounts" onchange="SubAccountLedger_cbCheckAllAccountsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;Select All Accounts</u></label><br />
                </div>
                <div id="divCbAccount" style="width:107%; height:370px; font-size:100%; overflow:scroll;">
                </div>
            </section>
            <section id="secCostCenter" class="m-l col-sm-3 panel panel-default">
                <div class="">
                    <label class=""><input type="checkbox" id="cbCheckAllCostCenters" onchange="SubAccountLedger_cbCheckAllCostCentersChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;Select All Cost Centers</u></label><br />
                </div>
                <div id="divCbCostCenter" style="width:107%; height:370px; font-size:100%; overflow:scroll;">
                </div>
            </section>

            <!--EOF Region Filter-->
            <div id="hExportedTable" class="hide"></div>
            <div id="hExportdiv" class="hide"></div>
        </div>


        <footer class="panel-footer hide">
            <div class="row">
                <div class="col-sm-4 hidden-xs">
                    <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                        <option value="10">10</option>
                        <option value="25">25</option>
                        <option value="50">50</option>
                    </select>
                    <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                        @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                    </button>
                </div>
                <div id="div-Text-Total" class="col-sm-4 text-center">
                    <small class="text-muted inLine m-t-sm m-b-sm">
                        @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                        <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                        <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                    </small>
                </div>
                <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
            </div>
        </footer>
    </section>
</div>
</div>
