    <li>
        <a id="hl-homepage" href="javascript:void(0);" onclick="LoadViews(id);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="Reports" href="javascript:void(0);" onclick="LoadViews('OperAccountingReports')">
            @Forwarding.MvcApp.App_Resources.App_Resources.Reports
    </a>
</li>
<li>
    <a id="AccountingReports" href="javascript:void(0);" onclick="LoadViews('OperAccountingReports');">
        @Forwarding.MvcApp.App_Resources.App_Resources.AccountingReports
</a>
