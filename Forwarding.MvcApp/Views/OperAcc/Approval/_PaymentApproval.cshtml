                                        <button id="btn-DeleteReceivable" type="button" class="float-right btn btn-sm btn-danger" onclick="Receivables_DeleteList();"><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>
                                    </div>*@
                                    

                                    <div id="divPaymentDetails" class="m-t-xs">
                                        <section class="panel panel-default">
                                            <header class="panel-heading font-bold text-ul">@Forwarding.MvcApp.App_Resources.App_Resources.PaymentDetails</header>
