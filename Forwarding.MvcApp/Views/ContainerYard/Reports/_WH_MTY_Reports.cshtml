<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);" onclick="LoadViews(id);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="ContainerFreightStation" href="javascript:void(0);" onclick="LoadViews('ContainerYard')">
            @Forwarding.MvcApp.App_Resources.App_Resources.ContainerYard1
        </a>
    </li>
    <li>
        <a id="ContainerFreightStationTransactions" href="javascript:void(0);" onclick="LoadViews('ContainerYardReports');">
            @Forwarding.MvcApp.App_Resources.App_Resources.ContainerYardReports
        </a>
    </li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.WH_MTY_Reports</li>
</ul>
<!-- end breadcrumb  -->


<div id="div-WH_MTY_Reports">

    <!-- Region Options & Template  -->

    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>

        <div class="row">
            @*<div class="col-sm-12">*@
            <section class="panel panel-default">
                <header class="panel-heading font-bold">
                    <h3 class="m-b-none">
                        @Forwarding.MvcApp.App_Resources.App_Resources.WH_MTY_Reports
                    </h3>
                </header>
                <div class="panel-body" id="WH_MTY_ReportModal">
                    <form role="form">




                        <div class="form-group col-sm-4" style="clear:both;">

                            <div class="form-group m-l-lg">
                                <div>
                                    <div class="row">
                                        <div>
                                            <!-- radio -->
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rGateIN" value="GateIN" checked="checked">
                                                    Gate IN
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rInventory" value="Inventory">
                                                    Inventory
                                                </label>
                                            </div>
                                            <div class="radio">
                                                <label>
                                                    <input type="radio" name="radio" id="rGateOut" value="GateOut">
                                                    Gate Out
                                                </label>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="form-group col-sm-8">
                            <div class="form-group">
                                <div id="divFromDate" class="form-group col-sm-7">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                                    <input id="dtpFromDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="false" data-date-format="dd/mm/yyyy" placeholder="From Date" autocomplete="off" />
                                </div>
                                <div id="divToDate" class="form-group col-sm-7">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                                    <input id="dtpToDate" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control" data-required="false" data-date-format="dd/mm/yyyy" placeholder="To Date" autocomplete="off" />
                                </div>
                                <div id="divContainerNo" class="col-sm-7" style="clear:both;">
                                    <label id="lblContainerNo">ContainerNo</label>
                                    <input type="text" id="txtContainerNo" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="11" placeholder="Container No" style="text-transform:uppercase">
                                </div>
                                <div id="divCntrType" class="col-sm-7 hidden" style="clear:both;">
                                    <label id="lblCntrType">Container Type </label>
                                    <input type="text" id="txtCntrType" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="10" placeholder="Container Type" style="text-transform:uppercase">
                                </div>
                                <div id="divWH_Warehouse" class="col-sm-7" style="clear:both;">
                                    <label id="lblWH_Warehouse">Ware house</label>
                                    <input type="text" id="txtWH_Warehouse" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="30" placeholder="Ware house" style="text-transform:uppercase">
                                </div>
                                <div id="divArea" class="col-sm-7" style="clear:both;">
                                    <label id="lblArea">Area</label>
                                    <input type="text" id="txtArea" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="10" placeholder="Area" style="text-transform:uppercase">
                                </div>
                                <div id="divRow" class="col-sm-7" style="clear:both;">
                                    <label id="lblRow">Row</label>
                                    <input type="text" id="txtRow" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="10" placeholder="Row" style="text-transform:uppercase">
                                </div>
                                <div id="divRowLocation" class="col-sm-7" style="clear:both;">
                                    <label id="lblRowLocation">RowLocation</label>
                                    <input type="text" id="txtRowLocation" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="Row Location" style="text-transform:uppercase">
                                </div>
                                <div id="divTruckNo" class="col-sm-7" style="clear:both;">
                                    <label id="lblTruckNo">TruckNo</label>
                                    <input type="text" id="txtTruckNo" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="TruckNo" style="text-transform:uppercase">
                                </div>
                                <div id="divDriver" class="col-sm-7" style="clear:both;">
                                    <label id="lblDriver">Driver</label>
                                    <input type="text" id="txtDriverName" autocomplete="off" class="form-control input-sm" data-required="false" maxlength="50" placeholder="Driver" style="text-transform:uppercase">
                                </div>
                            </div>
                        </div>

                        @*<button id="btnPrint" type="submit" class="btn btn-sm btn-default">Submit</button>*@
                        <div class="col-sm-4" style="clear:both;">
                            <br /><br /><br /><br /><br /><br />
                            <button type="button" id="btnPrint" class="btn btn-primary btn-sm" onclick="PrintCntrReports();">
                                <span class="glyphicon glyphicon-print"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Print

                            </button>
                        </div>

                    </form>
                </div>
            </section>
            @*</div>*@
        </div>




    </div>




</div>