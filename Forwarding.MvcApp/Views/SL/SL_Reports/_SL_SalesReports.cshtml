<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="SC" href="javascript:void(0);" onclick="LoadViews('SL')">
            @Forwarding.MvcApp.App_Resources.App_Resources.SL
    </a>
</li>
<li>
    <a id="SL_Reports" href="javascript:void(0);" onclick="LoadViews('SL_Reports')">
        @Forwarding.MvcApp.App_Resources.App_Resources.SL_Reports
</a>
</li>
<li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.SL_SalesReports</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-AccountLedger">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.SL_SalesReports
            </h3>
        </header>
        <div class="row text-sm wrapper hidden-print m-t-n-sm">
            <div class="swapChildrenClass">
                <!--Region Filter-->
                <div class="form-group col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.From</label>
                    <input id="txtFromDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                </div>
                <div class="form-group col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.To</label>
                    <input id="txtToDate" readonly="readonly" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="background-color:white; cursor:text;" class="input-sm datepicker-input form-control" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                </div>
                <div class="col-sm-8 m-t-xs">

                </div>
            </div>

            </div>
            <div class="row text-sm wrapper hidden-print ">
                <div class="swapChildrenClass">
                    @*//Sales FollowUp(Customers)
        //Sales FollowUp_CostCenter
        //SalesFollowUp(MonthPeriod)
        //SalesFollowUp(TotalItems)*@
                    @*<div class="col-sm-8 swapChildrenClass m-t-xs">*@
                    <div class="row">
                        <div class="col-sm-2 swapChildrenClass">
                            <input onchange="SHowHideOptionReuqired();" tabindex="" type="radio" name="nameReportType" id="7" checked="checked" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.InvoicesSalesTotals&nbsp;</b>
                        </div>
                        <div class="col-sm-2 swapChildrenClass">
                            <input onchange="SHowHideOptionReuqired();" tabindex="" type="radio" name="nameReportType" id="Sales FollowUp(Customers)" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.CustomersSalesFollowUpTotal&nbsp;</b>
                        </div>
                        <div class="col-sm-2 swapChildrenClass">
                            <input onchange="SHowHideOptionReuqired();" tabindex="" type="radio" name="nameReportType" id="Sales FollowUp_CostCenter" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.CostCenterSalesFollowUp&nbsp;</b>
                        </div>
                        <div class="col-sm-2 swapChildrenClass">
                            <input onchange="SHowHideOptionReuqired();" tabindex="" type="radio" name="nameReportType" id="SalesFollowUp(MonthPeriod)" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SalesFollowUpTotalForPeriod&nbsp;</b>
                        </div>
                        <div class="col-sm-2 swapChildrenClass">
                            <input onchange="SHowHideOptionReuqired();" tabindex="" type="radio" name="nameReportType" id="SalesFollowUp(TotalItems)" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.ItemsSalesFollowUpTotal&nbsp;</b>

                        </div>
                        <div class="col-sm-2 swapChildrenClass">
                            <input onchange="SHowHideOptionReuqired();" tabindex="" type="radio" name="nameReportType" id="Rep_SLInvoice_SalesFollowUp_Detailed" tag="5" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SalesFollowUpDetailed&nbsp;</b>

                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2 swapChildrenClass">
                            <input onchange="SHowHideOptionReuqired();" tabindex="" type="radio" name="nameReportType" id="Sales_CustomersBalanceOnDate" tag="5" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp; @Forwarding.MvcApp.App_Resources.App_Resources.CustomersBalanceOnDate  &nbsp;</b>
                        </div>
                        <div class="col-sm-2 swapChildrenClass">
                            <input onchange="SHowHideOptionReuqired();" tabindex="" type="radio" name="nameReportType" id="SalesAnalyticsReport" tag="5" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SalesAnalyticsReport&nbsp;</b>
                        </div>
                        
                        <div class="col-sm-8 swapChildrenClass">

                        </div>
                    </div>

                    </div>
                </div>
                <div class="row text-sm wrapper hidden-print m-t-n-md">
                    <div class="swapChildrenClass">
                        <div class="col-sm-4 swapChildrenClass">
                            <button id="btn-ExportToExcel" type="button" onclick="SL_Reports_Print('Excel');" class="btn btn-sm btn-lightblue"><i class="fa fa-table"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.TraExportExcel&nbsp;</b></button>
                            <button id="btn-Print" type="button" onclick="SL_Reports_Print('Print');" class="btn btn-sm btn-info"><i class="fa fa-print"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Print&nbsp;</b></button>
                        </div>
                    </div>
                </div>
                <div class="row text-sm wrapper hidden-print m-t-n-md">
                    <div class="swapChildrenClass">
                        <section id="secCustomers" class="m-t-xs m-l col-sm-3 panel panel-default">
                            <div class="">
                                <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllStores" onchange="cbCheckAllCustomersChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllCustomers&nbsp;</u></label><br />
                            </div>
                            <div id="divCbCustomers" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                            </div>
                        </section>

                        <section id="secItems" class="m-t-xs m-l col-sm-3 panel panel-default ">
                            <div class="">
                                <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllItems" onchange="cbCheckAllItemsChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SelectAllItems&nbsp;</u></label><br />
                            </div>
                            <div id="divCbItems" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                            </div>
                        </section>
                        <section id="secSalesMen" class="m-t-xs m-l col-sm-3 panel panel-default hide">
                            <div class="">
                                <label class="swapChildrenClass"><input type="checkbox" id="cbCheckAllSalesMen" onchange="cbCheckAllSalesMenChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><u>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SL_SalesMan&nbsp;</u></label><br />
                            </div>
                            <div id="divCbSalesMen" style="width:104%; height:370px; font-size:100%; overflow:scroll;">
                            </div>
                        </section>
                        <section class=" m-t-xs m-l col-sm-1"></section>
                        <!--EOF Region Filter-->
                        <div id="hExportedTable" class="hide"></div>
                    </div>
                </div>

                <footer class="panel-footer hide">
                    <div class="row">
                        <div class="col-sm-4 hidden-xs">
                            <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                                <option value="10">10</option>
                                <option value="25">25</option>
                                <option value="50">50</option>
                            </select>
                            <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                                @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                            </button>
                        </div>
                        <div id="div-Text-Total" class="col-sm-4 text-center">
                            <small class="text-muted inLine m-t-sm m-b-sm">
                                @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                                <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                                <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                            </small>
                        </div>
                        <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                    </div>
                </footer>
</section>
</div>
</div>
