<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="SL" href="javascript:void(0);" onclick="LoadViews('SL')">
            @Forwarding.MvcApp.App_Resources.App_Resources.Sales
        </a>
    </li>
    <li>
        <a id="SL_Transactions" href="javascript:void(0);" onclick="LoadViews(id)">
            @Forwarding.MvcApp.App_Resources.App_Resources.SL_Transactions
        </a>
    </li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.ClientDbtCrdtNotes</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-SL_Invoices">
    <!-- Add / Edit Region Modal --> <!--data-keyboard="false" data-backdrop="false" to prevent modal closure on pressing escape and pressing outside the modal-->

    <div class="modal fade" id="SL_InvoicesModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="SL_InvoicesModalLabel" aria-hidden="true">
        <div class="modal-dialog modalwidth-80">
            @*<div class="modal-content modalwidth-100">*@
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="m-b-none">
                        @Forwarding.MvcApp.App_Resources.App_Resources.ClientDbtCrdtNotes&nbsp;<label id="lblShown" purpose="dynamicLabel" class="static-text-primary"></label>
                    </h3>
                </div>
                <!-- Region Add / Edit Region Form -->
                <div class="modal-body">
                    <div class="span7 form vertical-mode" id="SL_Invoices-form">
                        <fieldset>
                            <div class="form-group pull-in  clearfix">
                                <input id="hID" type="hidden" />
                                <!-- start alert message region -->
                                <div class="alert alert-danger div-error">
                                    @Forwarding.MvcApp.App_Resources.App_Resources.ErrorMessage
                                </div>
                                <!-- end alert message region -->
                                <!-- Region Add / Edit Region Form -->
                                <div class="row">
                                    <div class="col-sm-12" style="text-align:center">
                                        <div class="col-sm-12 m-t-sm" style="text-align:center;border-right:solid 1px">
                                            <input tabindex="650" type="radio" name="nameChequeOrDepositOrPO" id="cbIsCredit" checked="checked" onchange="Voucher_cbIsChequeChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Credit&nbsp;</b>
                                            <input tabindex="650" type="radio" name="nameChequeOrDepositOrPO" id="cbIsDebit" checked="checked" onchange="Voucher_cbIsChequeChanged();" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);"><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Debit&nbsp;</b>

                                        </div>
                                        <div class="col-sm-6 text-left swapChildrenClass m-t-sm hide" style="text-align:left;border-left:solid 1px">
                                        </div>
                                        <hr style="width:30%;text-align:center;  border: 1px solid black;border-radius:1px">
                                    </div>
                                </div>
                                <div class="row text-sm wrapper hidden-print m-t-n-md ">
                                    <div class="swapChildrenClass">
                                        <div class="form-group col-sm-2">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.No</label>
                                            <input id="txtNo" disabled="disabled" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated  input-sm" data-required="true" data-minlength="1" placeholder="ex. 15894" style="text-transform:uppercase" />
                                        </div>
                                        <div class="form-group  col-sm-2 ">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Date</label>
                                            <input id="txtDate" @*onblur="RecalculateExchangeRate();"  onchange="RecalculateExchangeRate();"*@ type="text" onfocus="DisableEnterKey(id);" data-required="true" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control input-sm Edit-input" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                                        </div>
                                        <div class="form-group col-sm-2">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Serial</label>
                                            <input id="txtSerial" type="text" class="form-control parsley-validated  input-sm" data-required="true" data-minlength="1" style="text-transform:uppercase" />
                                        </div>
                                        <div class="form-group col-sm-3">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Client</label>
                                            <select id="slClientID" class="input-sm form-control Edit-input" data-required="true" onchange=""></select>
                                        </div>
                                        <div class="form-group col-sm-3">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                                            <select id="slCurrencyID" class="input-sm form-control Edit-input" data-required="false" onchange="CalculateAll();"></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="row text-sm wrapper hidden-print m-t-n-md ">
                                    <div class="swapChildrenClass">
                                        <div class="form-group has-error clearfix col-sm-3">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Invoice</label>
                                            <select id="slInvoice" class="input-sm form-control Edit-input" data-required="true" onchange=""></select>
                                        </div>
                                        <div class="col-sm-9">
                                        </div>
                                    </div>
                                </div>
                                <div class="row text-sm wrapper hidden-print m-t-n-md swapChildrenClass">
                                    <div class="col-sm-12 ">
                                        <div class="form-group has-error clearfix col-sm-6">
                                            <label>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</label>
                                            <textarea id="txtNotes" style="resize:none;" rows="2" class="form-control parsley-validated Edit-input  input-sm"></textarea>
                                        </div>
                                        <div class="col-sm-6">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </div>


                    @*taaaaaaabs*@
                    <ul class="nav nav-tabs">
                        <li class="active"><a data-toggle="tab" href="#tabItems"><b hidden>Items</b> <span hidden id="lblTotalItems">0.00</span> [<b hidden>I:</b> <span hidden id="lblTotalItems_I">0.00</span> <b>S:</b>&nbsp;<span id="lblTotalItems_S">0.00</span>&nbsp;] </a> </li>
                    </ul>

                    <div class="tab-content">
                        <div id="tabItems" class="tab-pane fade in active">
                            @* Iteeeeeeems*@
                            <div class="row">
                                <a href="#" onclick="AddNewItemsRow();" style="border-radius:25px;margin-left:20px" class="btn btn-lightblue Edit-btn float-left m-sm"><span class="fa fa-plus "></span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.AddDetails</a>
                            </div>
                            <div class="table-responsive" style="height:140px;overflow:scroll;">
                                <table id="tblItems" class="table b-t b-light text-sm  table-hover">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th></th>
                                            <th class="hidden">Name</th>
                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Account</th>
                                            <th class="hidden">Store</th>
                                            <th class="hidden">Qty in store</th>
                                            <th class="hidden">Unit Price</th>
                                            <th class="hidden">Discount</th>
                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Price</th>
                                            <th class="hidden">Price</th>
                                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Notes</th>
                                            <th class="hidden">Tax Ratio</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>




                            @* Iteeeeeeems*@
                        </div>
       
                    </div>

                </div>
                <!-- Endregion Add / Edit Region Form -->
                <div class="modal-footer">
                    <button type="button" id="btnSave" class="btn btn-primary btn-sm Edit-btn" onclick="SL_Invoices_Save(false);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Save
                    </button>

                    <button type="button" id="btnPrint2" class="btn btn-lightblue btn-sm PrintTtansaction hide" onclick="PrintInvoice(0);">
                        <span class="fa fa-print"></span>@Forwarding.MvcApp.App_Resources.App_Resources.Print
                    </button>



                    <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm hide" onclick="SL_Invoices_Save(true);">
                        <span class="glyphicon glyphicon-save"></span>@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew
                    </button>
                    <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->

                </div>
            </div>
        </div>
    </div>
    <!-- Region Options & Template  -->

    <div id="div-Data">
        <div id="div-Options">
            <div id="div-SL_Invoices" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.ClientDbtCrdtNotes
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div id="FilterSec" class="row m-t-sm">


                <div class="form-group  col-sm-2">
                    <div class="col-sm-12">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.No</label>
                        <input id="txtInvoiceNo_Filter" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated  input-sm" data-required="true" data-minlength="1" maxlength="10" placeholder="ex. 25" style="text-transform:uppercase" />
                    </div>
                </div>

                <div class="form-group  col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Client</label>
                    <select id="slClients_Filter" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"><option value="">@Forwarding.MvcApp.App_Resources.App_Resources.Select</option><option value="7">ALEXANDRIA</option><option value="6">CAIRO</option></select>
                </div>

                <div class="form-group  col-sm-2">

                    <div class="input-group col-sm-12">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.FromDate</label>
                        <input id="txtFromDate_Filter" onchange="" type="text" onfocus="DisableEnterKey(id);" data-required="true" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control input-sm" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />

                    </div>
                </div>

                <div class="form-group  col-sm-2">
                    <div class="input-group col-sm-12">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.ToDate</label>
                        <input id="txtToDate_Filter" type="text" onchange="" onfocus="DisableEnterKey(id);" data-required="true" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="datepicker-input form-control input-sm" data-date-format="dd/mm/yyyy" placeholder="@Forwarding.MvcApp.App_Resources.App_Resources.Select" />
                    </div>
                </div>
                <div class="form-group  col-sm-2">
                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Currency</label>
                    <select id="slCurrency_Filter" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm"><option value="">@Forwarding.MvcApp.App_Resources.App_Resources.Select</option><option value="7">ALEXANDRIA</option><option value="6">CAIRO</option></select>
                </div>

                <div class="form-group  col-sm-2">
                    <div class="col-sm-12">
                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.Amount</label>
                        <input id="txtTotalPrice_Filter" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" type="text" class="form-control parsley-validated  input-sm" data-required="true" data-minlength="1" maxlength="10" placeholder="ex. 25" style="text-transform:uppercase" />
                    </div>
                </div>
                <div class="form-group  col-sm-3" style="margin-top:5px;">
                    <div class="input-group col-sm-12">
                        <br />
                        <button id="btn-search1" class="btn btn-sm btn-lightblue" type="button" onclick="SL_Invoices_LoadingWithPaging();"><i class="fa fa-search">&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Search</i></button>
                        &nbsp;&nbsp;
                        <button id="btn-NewAdd" onclick="SL_Invoices_ClearAllControls();SL_HideShowEditBtns(false);" class="btn btn-warning btn-sm " data-toggle="modal" data-target="#SL_InvoicesModal"><i class="fa fa-plus"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New</button>
                        &nbsp;&nbsp;
                        <button id="btn-Delete" type="button" class=" btn btn-sm btn-danger m-r-n hide "><i class="fa fa-trash-o"></i>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete</button>

                        <div class="">
                        </div>
                    </div>
                </div>
            </div>



            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-12">






                    <div id="div-search-controls " class="input-group col-sm-9 hide">
                        <input id="txt-Search" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search Store Name / Account Name">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="SL_Invoices_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-1">
                    </div>
                </div>
            </div>
            <!--Region the searh, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblSL_Invoices" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th class="th-sortable" data-toggle="class">
                                @Forwarding.MvcApp.App_Resources.App_Resources.No
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Date
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Client
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Approved
                            </th>

                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Currency
                            </th>
                            <th>
                                @Forwarding.MvcApp.App_Resources.App_Resources.Notes
                            </th>
                            <th></th>


                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>


                    @* <select id="hidden_slItems" style="visibility:hidden" class="input-sm form-control" data-required="false" onchange=""></select>*@
                </div>
            </footer>
            <select id="hidden_slItems" style="visibility:hidden" class="input-sm form-control" data-required="false" onchange=""></select>
            <select id="hidden_slServices" style="visibility:hidden" class="input-sm form-control" data-required="false" onchange=""></select>
            <select id="hidden_slExpenses" style="visibility:hidden" class="input-sm form-control" data-required="false" onchange=""></select>
            <select id="hidden_slTaxes" style="visibility:hidden" class="input-sm form-control" data-required="false" onchange=""></select>
            <select id="hidden_slAccounts" style="visibility:hidden" class="input-sm form-control" data-required="false" onchange=""></select>

            
            <div id="hExportedTable" class="hide"></div>
            <!--Region: footer-->
        </section>
    </div>
</div>