<!-- Region Options & Template  -->
<!--FilterModal-->
<div class="modal fade" id="FilterModal" tabindex="-1" @*style="z-index: 3000;"*@ data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="FilterModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-100">
        <div class="modal-content m-t-n-lg">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true" title="Close">
                    &times;
                </button>
                <h3 class="m-b-none">
                    <label id="lblShownFilter" purpose="dynamicLabel" class="static-text-primary">Filter</label>
                </h3>
            </div>
            <div class="modal-body">
                <!-- Region Filter Form -->
                <button id="btn-ClearAllFilter" type="button" class="btn btn-warning btn-sm m-l m-t-n-sm hide" onclick=""><i class="fa "></i><b>&nbsp;Clear All</b></button>
                <div class="span7 form vertical-mode m-t-sm classApplyFilter" id="Filter-form">
                    <fieldset>
                        <div class="row hide">
                            <div id="div-Filter-controls" class="input-group m-l col-sm-5">
                                <input id="txtFilter" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="Search" />
                                <span class="input-group-btn">
                                    <button id="btn-Filter" class="btn btn-sm btn-primary" type="button"><i class="fa fa-search"></i></button>
                                </span>
                            </div>
                        </div>

                        <div id="divFilter" class="m-t-n">
                            <!--Region Filter-->

                            <div class="form-group col-sm-2">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.Creator</label>
                                <select id="slFilterUsers" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm">
                                    <option value=""><--Select--></option>
                                </select>
                            </div>

                            <div class="form-group col-sm-2">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.OperationSerial</label>
                                <input type="text" id="txtFilterOperationCodeSerial" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="50" placeholder="" style="text-transform:uppercase">
                            </div>
                            <div class="form-group col-sm-2">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.CRMComplaintName</label>
                                <input type="text" id="txtFilterComplaintName" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" class="form-control parsley-validated input-sm" data-required="false" maxlength="50" placeholder="" style="text-transform:uppercase">
                            </div>

                            <div class="form-group col-sm-1 m-l-n">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.ValueInEGP</label>
                                <input type="text" id="txtFilterValueInEGP" onkeypress="CheckValueIsInteger(id);" onfocus="CheckValueIsInteger(id);" data-type="number" class="form-control parsley-validated input-sm" maxlength="5" placeholder="0" />
                            </div>
                            <div class="form-group col-sm-1 m-l-n">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.ValueInUSD</label>
                                <input type="text" id="txtFilterValueInUSD" onkeypress="CheckValueIsInteger(id);" onfocus="CheckValueIsInteger(id);" data-type="number" class="form-control parsley-validated input-sm" maxlength="5" placeholder="0" />
                            </div>
                            <div class="form-group col-sm-1 m-l-n">
                                <label>@Forwarding.MvcApp.App_Resources.App_Resources.ValueInEUR</label>
                                <input type="text" id="txtFilterValueInEUR" onkeypress="CheckValueIsInteger(id);" onfocus="CheckValueIsInteger(id);" data-type="number" class="form-control parsley-validated input-sm" maxlength="5" placeholder="0" />
                            </div>
                            <!--EOF Region Filter-->

                        </div>

                    </fieldset>
                </div>
                <!-- Endegion Filter Form -->
            </div>
            <div class="modal-footer m-t-n">
                <!--sherif-->
                <button type="button" data-dismiss="modal" title="Search" onclick="Complaint_LoadingWithPaging();" id="btnFilterApply" class="btn btn-primary btn-sm">
                    <span class="glyphicon glyphicon-search"></span>&nbsp;Search
                </button>
                @*<button class=" btn btn-link btn-sm" data-dismiss="modal" aria-hidden="true">@Forwarding.MvcApp.App_Resources.App_Resources.Close</button>*@
            </div>
        </div>
    </div>
</div>
<!--EOF FilterModal-->
