<div id="div-Complaint">
    <div id="div-Data">
        <div id="div-Options">
            <div id="div-Action" class="radius-top-left-right">
            </div>
        </div>
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.Complaints
                </h3>
            </header>
            <!--Region the searh, delete, addnew-->
            <div class="row text-sm wrapper hidden-print ">
                <div id="div-main-options" class="col-sm-5 m-b-xs">
                    <button id="btn-FilterOptions" class="btn btn-lightblue btn-sm m-l" data-toggle="modal" onclick="" data-target="#FilterModal"><i class="fa fa-filter"></i> Filter Options</button>&nbsp;
                    <button id="btn-NewAdd" class="btn btn-warning btn-sm swapChildrenClass hide" data-toggle="modal" data-target="#ComplaintModal"><i class="fa fa-plus"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.New&nbsp;</b></button>
                    <div id="div-option-buttons" class="btn-group hide">
                        <button id="btn-Copy" type="button" class="btn btn-sm btn-default"><i class="fa fa-copy"></i>&nbsp;Copy </button>
                        <button id="btn-Print" type="button" class="btn btn-sm btn-default"><i class="fa fa-print"></i>&nbsp;Print </button>
                        <button id="btn-Send" type="button" class="btn btn-sm btn-default"><i class="fa fa-envelope-o"></i>&nbsp;Send </button>
                    </div>
                </div>
                <div id="div-search-option" class="col-sm-7">
                    <div id="div-search-controls" class="input-group col-sm-9 hide">
                        <input id="txt-Search" autocomplete="off" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="text-transform:uppercase" type="text" class="input-sm form-control" binding-name="SearchPlaceHolder" binding-prop="placeholder" placeholder="">
                        <span class="input-group-btn">
                            <button id="btn-search" class="btn btn-sm btn-primary" type="button" onclick="Complaint_LoadingWithPaging();"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                    <div class="float-right col-sm-1">
                        <button id="btn-Delete" type="button" class="float-right btn btn-sm btn-danger m-r-n swapChildrenClass hide"><i class="fa fa-trash-o"></i><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Delete&nbsp;</b></button>
                    </div>
                </div>
            </div>
            <!--Region the searh, delete, addnew-->
            <!--Region: table header-->
            <div class="table-responsive">
                <table id="tblComplaint" class="table table-striped b-t b-light text-sm  table-hover">
                    <thead>
                        <tr>
                            <th id="ID" style="width:5%;">
                                <input id="cb-CheckAll" type="checkbox" />
                            </th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Code</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Creator</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.CRMComplaintName</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ValueInEGP</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ValueInUSD</th>
                            <th>@Forwarding.MvcApp.App_Resources.App_Resources.ValueInEUR</th>
                            <th class="rounded-right"></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <!--Region: table header-->
            <!--Region: footer-->
            <footer class="panel-footer">
                <div class="row">
                    <div class="col-sm-4 hidden-xs">
                        <select id="select-page-size" class="input-sm form-control input-s-sm inline">
                            <option value="10">10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                        </select>
                        <button id="btn-applypaging" class="btn btn-sm btn-default" type="button">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Apply
                        </button>
                    </div>
                    <div id="div-Text-Total" class="col-sm-4 text-center">
                        <small class="text-muted inline m-t-sm m-b-sm">
                            @Forwarding.MvcApp.App_Resources.App_Resources.Showing
                            <span id="spn-first-page-row"></span>- <span id="spn-last-page-row"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Of
                            <span id="spn-total-count"></span> @Forwarding.MvcApp.App_Resources.App_Resources.Items
                        </small>
                    </div>
                    <div id="div-Pager" class="col-sm-4 text-right text-center-xs float-right"></div>
                </div>
            </footer>
            <!--Region: footer-->
        </section>
    </div>
</div>

<!-- Add / Edit Region Modal -->
<div class="modal fade" id="ComplaintModal" tabindex="-1" data-keyboard="true" data-backdrop="false" role="dialog" aria-labelledby="ComplaintModalLabel" aria-hidden="true">
    <div class="modal-dialog modalwidth-80">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h3 class="m-b-none swapChildrenClass">
                    <span>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Complaint &nbsp;</span><label id="lblShown" purpose="dynamicLabel" class="static-text-primary hide"></label>
                </h3>
            </div>
            <!-- Region Add / Edit Region Form -->
            <div class="modal-body">
                <div class="span7 form vertical-mode" id="Complaint-form">
                    <fieldset>
                        <div class="form-group pull-in  has-error clearfix">
                            <input id="hID" type="hidden" />

                            <div class="row col-sm-12">
                                <div class="form-group col-sm-3">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Code</label>
                                    <input type="text" id="txtCode" autocomplete="off" disabled="disabled" class="form-control parsley-validated input-sm" data-required="false" maxlength="50" placeholder="Auto-Generated" style="text-transform:uppercase" />
                                </div>
                                <div class="form-group col-sm-5" style="">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Customer</label>
                                    <select id="slCustomer" class="form-control m-b input-sm" onchange="Complaint_CustomerChanged();" data-required="true"><option value=""><--Select--></option></select>
                                </div>
                                <div class="form-group col-sm-4" style="">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Operation</label>
                                    <select id="slOperationForHeader" class="form-control m-b input-sm" onchange="" data-required="false"><option value=""><--Select--></option></select>
                                </div>
                            </div>
                            <div class="row col-sm-12">
                                <div class="form-group col-sm-3">
                                    <button id="btn-NewAddDetails" class="btn btn-warning btn-sm swapChildrenClass m-t-sm" onclick="ComplaintDetails_ClearAllControls(); OpenCRM_ComplaintDetailsModal();" data-toggle="modal" data-target="#"><i class="fa fa-plus"></i><b>&nbsp;Add Details</b></button>
                                </div>
                                <div class="float-right col-sm-2">
                                    <button id="btn-DeleteDetails" type="button" class="float-right btn btn-sm btn-danger m-t-sm m-r-n swapChildrenClass" onclick="ComplaintDetails_DeleteList();"><i class="fa fa-trash-o"></i><b>&nbsp;Delete&nbsp;</b></button>
                                </div>
                            </div>

                            <div class="row col-sm-12">

                                <div class="table-responsive">
                                    <table id="tblCRM_ComplaintDetails" class="table table-striped b-t b-light text-sm  table-hover">
                                        <thead>
                                            <tr>
                                                <th id="ID_CRM_ComplaintDetails">
                                                    <input id="cb-CheckAll_ComplaintDetails" type="checkbox">
                                                </th>
                                                <th>
                                                    Operation
                                                </th>
                                                <th>
                                                    Status
                                                </th>
                                                <th>
                                                    Complaint Name
                                                </th>
                                                <th>
                                                    Complaint Date
                                                </th>
                                                <th class="hide">
                                                    Response Date
                                                </th>
                                                <th>
                                                    Sales Rep
                                                </th>
                                                <th class="hide">
                                                    Sales Rep 2
                                                </th>

                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- Endregion Add / Edit Region Form -->
            <div class="modal-footer">
                <button type="button" id="btnSave" class="btn btn-primary btn-sm swapChildrenClass" onclick="Complaint_Save(false);">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.Save&nbsp;</b>
                </button>
                <button type="button" id="btnSaveandNew" class="btn btn-primary btn-sm swapChildrenClass" onclick="Complaint_Save(true);">
                    <span class="glyphicon glyphicon-save"></span><b>&nbsp;@Forwarding.MvcApp.App_Resources.App_Resources.SaveAndNew&nbsp;</b>
                </button>
                <button class="btnClose btn btn-link btn-sm" id="btnClose" onclick="" data-dismiss="modal" aria-hidden="true"><b>@Forwarding.MvcApp.App_Resources.App_Resources.Close</b></button><!--sherif: i added an id and the onclick fn go set the timelocked in the DB to null-->
            </div>
        </div>
    </div>
</div>

