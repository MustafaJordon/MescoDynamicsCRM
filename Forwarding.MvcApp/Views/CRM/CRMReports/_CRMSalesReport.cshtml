<!-- start breadcrumb  -->
<ul id="ul-breadcrumb" class="breadcrumb no-border no-radius b-b b-light pull-in">
    <li>
        <a id="hl-homepage" href="javascript:void(0);">
            <i class="fa fa-home icon-home"></i><span>@Forwarding.MvcApp.App_Resources.App_Resources.HomePage</span>
        </a>
    </li>
    <li>
        <a id="MasterData" href="javascript:void(0);" onclick="LoadViews('CRM')">
            @Forwarding.MvcApp.App_Resources.App_Resources.CRM
        </a>
    </li>
    <li>
        <a id="CRMReports" href="javascript:void(0);" onclick="LoadViews(id)">
            @Forwarding.MvcApp.App_Resources.App_Resources.CRMReports
        </a>
    </li>
    <li class="active">@Forwarding.MvcApp.App_Resources.App_Resources.CRMSalesReport</li>
</ul>
<!-- end breadcrumb  -->

<div id="div-CRMSalesReport">
    <!-- Add / Edit Region Modal --> <!--data-keyboard="false" data-backdrop="false" to prevent modal closure on pressing escape and pressing outside the modal-->
    <!-- Region Options & Template  -->

    <div id="div-Data">
        <section class="panel panel-default">
            <header class="panel-heading">
                <h3 class="m-b-none">
                    @Forwarding.MvcApp.App_Resources.App_Resources.FilterBy
                </h3>
            </header>
            <div class="m-t-sm">
                <div class="span7 form" id="Search-form">
                    <fieldset>
                        <div class="form-group pull-in    row">
                            <div class="form-group has-error  col-sm-3">
                                <div class="col-sm-12">
                                    <label>@Forwarding.MvcApp.App_Resources.App_Resources.Salesman</label>
                                    <select id="slSalesRep_search" class="input-sm form-control" onchange=""></select>
                                </div>
                            </div>
                            <div class="form-group has-error  col-sm-3">
                                <div class="col-sm-12">
                                    <label> @Forwarding.MvcApp.App_Resources.App_Resources.From </label>
                                    <input id="txtYearFrom" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="form-control input-sm" data-required="false" placeholder=" Year" />
                                </div>
                            </div>
                            <div class="form-group has-error  col-sm-3">
                                <div class="col-sm-12">
                                    <label> @Forwarding.MvcApp.App_Resources.App_Resources.To </label>
                                    <input id="txtYearTo" type="text" onfocus="DisableEnterKey(id);" onkeypress="DisableEnterKey(id);" style="cursor:text;" class="form-control input-sm" data-required="false" placeholder=" Year" />
                                </div>
                            </div>
                            <div class="form-group has-error  col-sm-3">
                                <div class="col-sm-12">
                                    <button id="btn-print" type="button" class="btn btn-lightblue btn-sm m-t-md" onclick="CRMSalesReport_Print('Print');"><i class="fa fa-print"></i><b>&nbsp;Print</b></button>
                                </div>
                            </div>

                        </div>

                        <div class="form-group pull-right    row">

                            <div class="col-sm-12 ">
                                <br />
                                &nbsp;&nbsp;&nbsp;
                               
                            </div>


                        </div>







                    </fieldset>
                </div>
            </div>
            <footer class="panel-footer"></footer>
            <!--Region: footer-->
        </section>
    </div>
</div>
<input type="hidden" id="hExportedTable" />
