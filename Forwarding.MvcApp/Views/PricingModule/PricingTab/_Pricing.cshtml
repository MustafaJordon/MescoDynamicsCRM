                                    <!--5th row-->
                                    <div class="form-group col-sm-6 hide">
                                        <label>@Forwarding.MvcApp.App_Resources.App_Resources.LineOrTrucker</label>
                                        <select id="slLines" name="slLines" onfocus="DisableBackspaceKey(id);" onkeydown="DisableBackspaceKey(id);" onkeypress="DisableBackspaceKey(id);" class="form-control input-sm" data-required="false"></select>
                                    </div>
                                    <div class="form-group col-sm-6 hide">
